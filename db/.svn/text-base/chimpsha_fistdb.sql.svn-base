INSERT INTO `card` VALUES (1575, 'Shamanistic Punk', 'Hood Sorcerer', 1, '', 0x654d, 1, 0, 0, '+1 damage to <Cop> and <Police> cards. -1 damage to <Demon> and <Spirit> cards.', 'They crave wealth, not wisdom.', '', 'Cristian Saksida', 5, 5, 2, 12, 'N', '| shamanistic punk hood sorcerer |');
INSERT INTO `card` VALUES (1576, 'Shell Game', 'Event', 0, 0x61, '', 0, 0, 0, 'Target player reveals the top three cards of his or her deck. The player to his or her left puts one on the bottom of the deck, and target puts the other two into his or her hand.', 'The Ascended can give you what you want in such a way that you regret it.', '', 'Patrick McEvoy', 4, 2, 3, 12, 'N', '| shell game |');
INSERT INTO `card` VALUES (1577, 'Silver Jet vPAP', 'Secret Warrior', 5, 0x6d6d, 0x6d64, 8, 0, 0, 'Unique. Independent. Toughness: 1. When you declare him as an attacker, name a card, which cannot be played in response :: His target''s controller must show you his or her hand and discard all cards with that title.', '', '', 'Melissa Benson', 5, 7, 1, 12, 'N', '| silver jet secret warrior |');
INSERT INTO `card` VALUES (1578, 'Skin and Darkness Zealots', 'Netherworld Horrors', 3, 0x6d4d, 0x6d, 4, 0, 0, 'Assassinate. When Skin and Darkness Zealots enter combat with a Character, treat that Character''s rules text as if it were blank until the end of combat.', '', '', 'Mike Jackson', 5, 7, 2, 12, 'N', '| skin darkness darknes darkne darkn zealot zealots netherworld horror horrors |');
INSERT INTO `card` VALUES (1579, 'Slo Mo Vengeance', 'Event', 100, 0x64, '', 0, 0, 0, 'Play X Weapon or <Gun> States at no cost from your smoked pile onto a Character you control.', 'This is the moment when it all goes down...', '', 'Heather Hudson', 4, 4, 3, 12, 'N', '| slo mo vengeance |');
INSERT INTO `card` VALUES (1580, 'Snowfall', 'Event', 2, 0x6d6d, '', 0, 0, 0, 'All Characters cease attacking.', 'Pi Tui flew into the air, her arms outstretched. The snow began to fall, and everyone drifted off into gentle slumber.', '', 'Melissa Benson', 4, 7, 3, 12, 'N', '| snowfall |');
INSERT INTO `card` VALUES (1581, 'Sonic Reducer', 'State', 1, 0x6a48, '', 0, 0, 0, 'Unique. Weapon. Turn to turn target Character at subject Character''s location and inflict X damage to it. X = one-half target''s Fighting (round up).', 'Ain''t no loser!', '', 'Matt Bober', 3, 10, 3, 12, 'N', '| sonic reducer |');
INSERT INTO `card` VALUES (1582, 'Spin Doctoring', 'Edge', 2, 0x616161, '', 0, 0, 0, 'When a Character you control that requires resources is smoked, you must return a random card from your smoked pile to your hand.', '', '', 'Mike Jackson', 2, 2, 3, 12, 'N', '| spin doctoring |');
INSERT INTO `card` VALUES (1583, 'Stand Together', 'Edge', 1, '', '', 0, 0, 0, 'Limited. You must have at least two resources each from two different factions in your pool to play this card. Choose a designator when you play this card. All Characters with that designator gain +1 Fighting.', '', '', 'Matt Bober', 2, 12, 3, 12, 'N', '| stand together |');
INSERT INTO `card` VALUES (1584, 'Stone Dolmens', 'Feng Shui Site', 0, '', '', 0, 1, 8, 'Turn to smoke a State on a card you own.', 'A place for rituals of cleansing.', '', 'Patrick McEvoy', 1, 6, 2, 12, 'N', '| stone dolmen dolmens |');
INSERT INTO `card` VALUES (1585, 'Stone Spirals', 'Netherworld Feng Shui Site', 0, '', '', 0, 1, 8, 'Gain 1 Power when an opponent plays a card at a reduced cost due to a card he or she controls.', '', '', 'Glen Osterberger', 1, 6, 2, 12, 'N', '| stone spiral spirals netherworld |');
INSERT INTO `card` VALUES (1586, 'Ten Thousand Agonies', 'Demon Lord', 4, 0x6565654d, 0x65, 9, 0, 0, 'Unique. All damage inflicted on Ten Thousand Agonies is increased by 1.', 'Shelun cast his spell and the demon appeared, towering above the Dragons.', '', 'Roberto Campus', 5, 5, 1, 12, 'N', '| ten thousand agony agon agonies demon lord |');
INSERT INTO `card` VALUES (1587, 'The Bazaar', 'Netherworld Battleground Site', 0, '', '', 0, 0, 5, 'Unique.  Any player who seizes this card gains 1 Power. If this Site is in your front row, you may turn it to draw a card, then shuffle one card from your hand into your deck.', '"Everything''s for sale-but there might be a few dents."', '', 'Glen Osterberger', 1, 12, 6, 12, 'N', '| the bazaar netherworld battleground |');
INSERT INTO `card` VALUES (1588, 'The Enemy of my Enemy', 'Edge', 1, 0x6161, '', 0, 0, 0, 'Once per turn, you may give an opponent one Power :: Draw two cards.', 'The Ascended prefer to have you fight their battles.', '', 'Brian Rood', 2, 2, 3, 12, 'N', '| the enemy my |');
INSERT INTO `card` VALUES (1589, 'The Forest of Fallen Banners', 'Netherworld Site', 2, '', '', 0, 1, 7, 'Unique. Generates no Power if there are Characters at this location. Draw a card when a Character you control is smoked by an opponent.', 'An eerie testament to the savagery of mankind.', '', 'Heather Bruton', 1, 12, 1, 12, 'N', '| the forest fallen banner banners netherworld |');
INSERT INTO `card` VALUES (1590, 'The Ickies', 'Netherworld Rabble', 1, '', '', 1, 0, 0, 'At the end of your turn, steal 1 Power from any one opponent who is closer to victory than you. Only one "The Ickies" may trigger each turn.', 'They want their IKTV.', '', 'Roberto Campus', 5, 12, 2, 12, 'N', '| the icky ick ickies netherworld rabble |');
INSERT INTO `card` VALUES (1591, 'The Orange Principle', 'Event', 1, 0x6743, '', 0, 0, 0, 'Shuffle X cards from your hand into your deck :: Draw X+1 cards.', 'The principle of diversity brings the blessings of good fortune.', '', 'Brian Rood', 4, 8, 2, 12, 'N', '| the orange principle |');
INSERT INTO `card` VALUES (1592, '"There''s Always One More..."', 'Event', 1, 0x6a6a, '', 0, 0, 0, 'Toast It. Randomly return a Character from your smoked pile to play.', '"It''s like playing Whack-a-Mole."', '', 'Matt Bober', 4, 10, 2, 12, 'N', '| ther there theres alway always one more |');
INSERT INTO `card` VALUES (1593, 'The Thunder Dome', 'Netherworld Site', 3, 0x6d, 0x6d, 0, 2, 8, 'Unique. Turn to look through your deck for a Faceoff Event, show it to your opponents, and put it in your hand. Turn to immediately play a Faceoff Event from your smoked pile. That card is considered to be Toast-It.', '', '', 'Joachim Barrum', 1, 7, 1, 12, 'N', '| the thunder dome netherworld |');
INSERT INTO `card` VALUES (1594, 'Throne of Skulls', 'Edge', 1, 0x65654d, '', 0, 0, 0, 'Unique. Gain 1 Power when a Unique Character is smoked.', 'A new skull appears whenever a hero dies.', '', 'Mark Poole', 2, 5, 1, 12, 'N', '| throne skull skulls |');
INSERT INTO `card` VALUES (1595, 'Thunder Apprentice', 'Fledgling Sorcerer', 1, 0x6d, 0x6d4d, 1, 0, 0, 'Turn to give target Character +1 damage until the end of the turn.', 'The most junior of Huan Ken''s clergy.', '', 'Lissanne Lake', 5, 7, 2, 12, 'N', '| thunder apprentice fledgling sorcerer |');
INSERT INTO `card` VALUES (1596, 'Thunder Gladiator', 'Netherworld Warrior', 1, '', 0x6d, 1, 0, 0, 'You may substitute [m] for each resource condition of a Faceoff Event. This card''s rules text is still active in your smoked pile.', '', '', 'Lissanne Lake', 5, 7, 2, 12, 'N', '| thunder gladiator netherworld warrior |');
INSERT INTO `card` VALUES (1597, 'Tracer Implant', 'Arcanowave State', 0, 0x66, '', 0, 0, 0, 'When subject Character turns, it takes 1 damage and you may unturn a Character you control. Smoke this card if you control its subject.', '', '', 'Cristian Saksida', 3, 1, 2, 12, 'N', '| tracer implant arcano wave |');
INSERT INTO `card` VALUES (1598, 'Trade Center', 'Site', 3, '', '', 0, 2, 9, 'Feng Shui Sites to the immediate right and left of this card generate no Power.', '"Old Mr. Tsang told me that it would cut off my chi, but I couldn''t exactly cancel a billion-dollar contract, could I?"', '', 'Mark Poole', 1, 12, 2, 12, 'N', '| trade center |');
INSERT INTO `card` VALUES (1599, 'Transmogrification', 'Event', 0, 0x6d4d, '', 0, 0, 0, 'Smoke a State on target Character, and inflict X damage on that Character. X= the cost of the State.', 'Pi Tui waved her hand and the weapon became an albino python.', '', 'Matt Bober', 4, 7, 2, 12, 'N', '| transmogrification |');
INSERT INTO `card` VALUES (1600, 'Two-Face', 'Punk Anarchists', 3, 0x6a, 0x6a, 3, 0, 0, 'Unique. Turn to take control of exactly two non-Unique Characters you do not control. They must each either cost 1 or neither require nor provide resources.', '', '', 'Matt Bober', 5, 10, 1, 12, 'N', '| two face punk anarchist anarchists |');
INSERT INTO `card` VALUES (1601, 'Uprising', 'Event', 100, '', '', 0, 0, 0, 'Return to play from your smoked pile one plus one-half of X (round down) <Rebel> and <Rabble> Characters that are not Unique. X must be at least 3.', '', '', 'Joachim Barrum', 4, 12, 3, 12, 'N', '| uprising |');
INSERT INTO `card` VALUES (1602, 'Verminous Rain', 'Event', 0, 0x654d, '', 0, 0, 0, 'Cancel the effects and rules text of target Site until the end of the turn. Reload: Toast two random cards in your smoked pile.', 'Vermin and pestilence bring corruption and ill fortune.', '', 'Cristian Saksida', 4, 5, 2, 12, 'N', '| verminou verminous rain |');
INSERT INTO `card` VALUES (1603, 'Violence Junkies', 'Crazy Rabble', 2, 0x6a6a, 0x6a, 5, 0, 0, 'When you play Violence Junkies from your hand, sacrifice an unturned Character. Must intercept attacks at their location.', 'Dangerous to everyone.', '', 'Ravindra Rana', 5, 10, 3, 12, 'N', '| violence junky junk junkies crazy rabble |');
INSERT INTO `card` VALUES (1604, 'Void', 'Entropy Spirit', 3, 0x704d4d, 0x704d, 4, 0, 0, 'Unique. Toughness: 1. Once per turn, you may sacrifice a Character to unturn this card. Once per turn, you may toast a Character you control to heal this card.', '', '', 'Andrew Barlow', 5, 13, 3, 12, 'N', '| void entropy spirit |');
INSERT INTO `card` VALUES (1605, 'Wandering Hero', 'Netherworld Ally', 3, 0x64, 0x64, 3, 0, 0, '+1 Fighting for every Character you control that shares one or more designators with one or more cards in play.', 'Warriors from forgotten times, looking for a cause.', '', 'Andrew Barlow', 5, 4, 3, 12, 'N', '| wandering hero netherworld ally |');
INSERT INTO `card` VALUES (1606, 'War of Attrition', 'Event', 1, 0x6666, '', 0, 0, 0, 'Every player chooses a Character he or she controls if possible :: Smoke those Characters.', '"When I gave the Unspoken Name the latest intelligence from 2063, he laughed for several minutes straight."', '', 'Mark Evans', 4, 1, 3, 12, 'N', '| war attrition |');
INSERT INTO `card` VALUES (1607, 'Wasting Curse', 'Edge', 1, 0x6565, '', 0, 0, 0, 'Characters with a cost of 4 or more take 1 damage when they unturn.', 'The Lotus sow sorrow and pain with vile maledictions.', '', 'Mike Jackson', 2, 5, 3, 12, 'N', '| wasting curse |');
INSERT INTO `card` VALUES (1608, 'Wave Disruptor', 'Event', 1, 0x4848, '', 0, 0, 0, 'Inflict two damage on all opponents'' Characters that provide more than 1 resource. Reload: 1', '', '', 'Glen Osterberger', 4, 9, 2, 12, 'N', '| wave disruptor |');
INSERT INTO `card` VALUES (1609, 'Wedding Gifts', 'Event', 0, 0x6d64, '', 0, 0, 0, 'One-Shot. Gain one Power for each unturned Character you do not control.', '"We get all this neat stuff just for getting married... We gotta do this more often."', '', 'Roberto Campus', 4, 7, 6, 12, 'N', '| wedding gift gifts |');
INSERT INTO `card` VALUES (1610, 'Wrath of the Monarchs', 'Event', 1, 0x6d, '', 0, 0, 0, 'Cancel and smoke a non-Unique <Netherworld> card.', 'In the Netherworld, it''s not wise to anger the Monarchs.', '', 'Patrick McEvoy', 4, 7, 2, 12, 'N', '| wrath monarch monarchs |');
INSERT INTO `card` VALUES (1611, 'Xiaoyang Yun', 'Spy Mastermind', 3, 0x6767, 0x67, 3, 0, 0, 'Unique. During your turn, opponents cannot play Events that require resources in your pool.', '"No, I know you too well."', '', 'Quinton Hoover', 5, 8, 1, 12, 'N', '| xiaoyang yun spy mastermind |');
INSERT INTO `card` VALUES (1612, 'Yakuza Enforcer', 'Killer Hood', 3, '', '', 4, 0, 0, 'Yakuza Enforcer takes no damage from cards you own. You cannot redirect damage to Yakuza Enforcer.', '"Traitors deserve no mercy."', '', 'Melissa Benson', 5, 12, 2, 12, 'N', '| yakuza enforcer killer hood |');
INSERT INTO `card` VALUES (1613, 'Zino the Greek', 'Netherworld Information Broker', 1, '', '', 1, 0, 0, 'Unique. Turn to look at target player''s hand. Take one card from that player''s hand and put it on top of their deck. That player may pay 1 Power to take control of Zino the Greek.', '', '', 'Anthony Grabski', 5, 12, 1, 12, 'N', '| zino greek netherworld information broker |');
INSERT INTO `card` VALUES (1614, '200 Guys With Hatchets and Ladders', 'Edge', 2, 0x6161, '', 0, 0, 0, 'Limited. Smoke all non-Unique Characters you control when this card leaves play. When an opponent plays a Unique Character, you may return up to X 1-cost Characters from your smoked pile to play. X= the Unique Character''s cost.', '', '', 'Michael Komarck', 2, 2, 3, 13, 'N', '| 200 guy guys with hatchet hatchets ladder ladders |');
INSERT INTO `card` VALUES (1615, '401k Squad', 'Mutated Soldiers', 4, 0x6a6a, 0x6a, 6, 0, 0, 'Toughness: 1. When this card comes into play, discard a card. Target opponent must discard X cards at random. X= the cost of the card you discarded.', '', '', 'Michael Komarck', 5, 10, 3, 13, 'N', '| 401k squad mutated soldier soldiers |');
INSERT INTO `card` VALUES (1616, 'Aerial Bombardment', 'Event', 1, 0x66, '', 0, 0, 0, 'Inflict X damage, divided any way you choose, on Characters controlled by an opponent. You may not inflict more than 3 damage on any Character. X= the number of one type of resource in that player''s pool.', 'BuroMil isn''t happy until even the rubble is destroyed.', '', 'Zak Hennessey', 4, 1, 2, 13, 'N', '| aerial bombardment |');
INSERT INTO `card` VALUES (1617, 'Ang Dao the Corrupt', 'Demon Warrior', 4, 0x6565, 0x65, 6, 0, 0, 'Unique. Regenerate. When Ang Dao the Corrupt enters play you may move any Weapons onto him.', 'He once challenged hell itself. He failed.', '', 'Patrick McEvoy', 5, 5, 6, 13, 'N', '| ang dao corrupt demon warrior |');
INSERT INTO `card` VALUES (1618, 'Ape Nuts', 'Crazy Cyborg Monkeys', 1, '', 0x6a, 1, 0, 0, 'Takes no damage from Sites. If you attack, Ape Nuts must be declared as an attacker if possible.', 'The mad scientists crank these guys out by the dozens.', '', 'Paul "Prof" Herbert', 5, 10, 2, 13, 'N', '| ape nut nuts crazy cyborg monkey monkeys |');
INSERT INTO `card` VALUES (1619, 'Arcanogardener', 'CDCA Scientist', 2, 0x66, 0x664d48, 2, 0, 0, 'When this card enters play, you may inflict one damage each on up to four Sites. Turn and maintain to give a Feng Shui Site you control +3 Body.', '', '', 'Stacy Drum', 5, 1, 2, 13, 'N', '| arcano gardener cdca scientist |');
INSERT INTO `card` VALUES (1620, 'Arcanoleech', 'State', 1, 0x666648, '', 0, 0, 0, 'Limited. Play on an opponent''s front-row Feng Shui Site. When subject generates Power for an opponent, this card generates one Power for you.', '', '', 'Zak Hennessey', 3, 1, 3, 13, 'N', '| arcano leech |');
INSERT INTO `card` VALUES (1621, 'Balanced Harmonies', 'Edge', 2, 0x7373, '', 0, 0, 0, 'Limited. Turn to remove all damage from cards you control. Inflict an equal amount of damage, divided any way you choose, on your cards. This damage cannot be reduced or redirected. You cannot put enough damage on a card to equal or exceed its Fighting or Body.', '', '', 'Matt Bober', 2, 14, 3, 13, 'N', '| balanced harmony harmon harmonies |');
INSERT INTO `card` VALUES (1622, 'Baptism of Fire', 'State', 1, 0x64, '', 0, 0, 0, 'Play on a Character when damage is inflicted on it. Subject gains +2 Fighting.', '"It''s just a flesh wound. You''ll get used to them."', '', 'L.A. Williams', 3, 4, 3, 13, 'N', '| baptism fire |');
INSERT INTO `card` VALUES (1623, 'Beneficent Tao', 'Event', 0, 0x73, 0x73, 0, 0, 0, 'Heal target Feng Shui Site. You may Reload this card when the Body of a Site you control is reduced to 0.', 'Stories and rumors say the Seven Masters possess lost secrets of geomancy.', '', 'Cristian Saksida', 4, 14, 2, 13, 'N', '| beneficent tao |');
INSERT INTO `card` VALUES (1624, 'Blanket of Darkness', 'Netherworld Edge', 2, 0x6d, '', 0, 0, 0, 'Unique. Turn and maintain to reduce the damage target Character inflicts by 3.', '', '', 'Mike Raabe', 2, 7, 1, 13, 'N', '| blanket darkness darknes darkne darkn netherworld |');
INSERT INTO `card` VALUES (1625, 'Bloody Horde', 'Demon Trouble', 3, '', 0x65, 3, 0, 0, 'Bloody Horde''s Fighting is increased by the number of other <Demon> <Hordes> you control, plus the number of <Demon> <Hordes> in your smoked pile.', '', '', 'Ron Spencer', 5, 5, 2, 13, 'N', '| bloody horde demon trouble |');
INSERT INTO `card` VALUES (1626, 'Boundless Heaven Sword', 'State', 6, 0x4d4d, '', 0, 0, 0, 'Unique. Weapon. This card''s cost cannot be reduced, even when returning it to play. Subject Character gains +10 Fighting and its damage cannot be reduced or redirected. Turn to give subject Superleap until the end of the turn.', '', '', 'Jeremy Dale', 3, 11, 1, 13, 'N', '| boundles boundless heaven sword  |');
INSERT INTO `card` VALUES (1627, 'Brain Tap Rifle', 'State', 1, 0x666648, '', 0, 0, 0, 'Weapon. Play on a Character. Turn to inflict 1 damage on target Character. You may look at target''s controller''s hand or reveal a Site he or she controls.', '"Mind reading''s much easier when the brain''s not in the skull anymore."', '', 'Roberto Campus', 3, 1, 2, 13, 'N', '| brain tap rifle |');
INSERT INTO `card` VALUES (1628, 'Buddha''s Palm', 'Event', 0, 0x4343, '', 0, 0, 0, 'Target a Character that is about to enter combat with a Character you control and toast the top X cards of your deck :: Inflict X damage on target.  X= the number of [C] resources in your pool.', '', '', 'Cara Mitten', 4, 3, 3, 13, 'N', '| buddha buddhas palm |');
INSERT INTO `card` VALUES (1629, 'Buro Scientist', 'Loyal Researcher', 2, 0x66, 0x6648, 1, 0, 0, 'Once per turn, when you play a [H] Event or [H] State, you may unturn a Character.', 'Not all of the CDCA sided with Boatman.', '', 'Jeremy Dale', 5, 1, 3, 13, 'N', '| buro scientist loyal researcher |');
