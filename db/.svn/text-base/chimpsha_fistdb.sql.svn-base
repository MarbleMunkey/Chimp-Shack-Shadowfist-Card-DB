INSERT INTO `card` VALUES (1328, 'Blade of Darkness', 'Netherworld Killer', 4, 0x6d6d, 0x6d, 6, 0, 0, 'When Blade of Darkness smokes an interceptor by inflicting combat damage, gain Power equal to the number of resources that interceptor provides.', '', '', 'Tony Moseley', 5, 7, 5, 11, 'N', '| blade darkness darknes darkne darkn netherworld killer |');
