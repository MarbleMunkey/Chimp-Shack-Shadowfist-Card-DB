INSERT INTO `card` VALUES (2414, 'The Steam Laundry Company', 'Feng Shui Site', 0, '', '', 0, 1, 5, 'Unique. Turn to heal a non-Unique Site.', 'Coffee, mud, blood - they can get rid of any stain.', '', 'Flavio Augusto', 1, 6, 1, 15, 'N', '| the steam laundry company |');
INSERT INTO `card` VALUES (2411, 'Six-Gun', 'State', 1, '', '', 0, 0, 0, 'Weapon. Play on a Character during any Main Shot. Subject gains +4 damage to Characters.', '"That''s one bullet for each of you."', '', 'Matthew Laznicka', 3, 12, 2, 15, 'N', '| six gun  |');
