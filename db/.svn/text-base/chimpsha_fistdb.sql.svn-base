INSERT INTO `card` VALUES (1269, 'Maverick Trucker', 'Blue-Collar Hero', 3, 0x6464, 0x64, 3, 0, 0, 'Guts. Toughness: 1. You may play a Vehicle on Maverick Trucker at -1 cost.', 'It''s a tough life, what with Smokeys, "white line fever" and bare-knuckle brawlin''.', '', 'Cristian Saksida', 5, 4, 2, 9, 'N', '| maverick trucker blue collar hero |');
INSERT INTO `card` VALUES (1270, 'Max Brunner', 'Pledged Cop', 4, 0x616161, 0x61, 5, 0, 0, 'Unique. You may treat <Cop> and <Pledged> as matching designators.', 'The most corrupt cop in San Francisco.', '', 'Patrick McEvoy', 5, 2, 1, 9, 'N', '| max brunner pledged cop |');
INSERT INTO `card` VALUES (1271, 'Mr. Simms', 'Hood Lieutenant', 2, '', '', 2, 0, 0, 'Unique. Once per turn, you may redirect damage inflicted by a non-Character source on a card you control to any Character controlled by the player who controlled the source of the damage.', '', '', 'Thomas Gianni', 5, 12, 1, 9, 'N', '| mr simm simms hood lieutenant |');
INSERT INTO `card` VALUES (1272, 'Napalm Sunrise', 'Event', 2, 0x6666, '', 0, 0, 0, 'Play during your turn. Inflict 2 damage on each Character and Site at target location.', '"Normally oranges and greens together clash, but I think it works here."', '', 'Edward Beard, Jr.', 4, 1, 2, 9, 'N', '| napalm sunrise |');
INSERT INTO `card` VALUES (1273, 'Night Moves', 'Edge', 1, 0x616161, '', 0, 0, 0, 'Limited. Characters intercepting your Characters inflict -1 damage. All opponents gain 2 Power when this card leaves play.', '', '', 'Thomas Gianni', 2, 2, 3, 9, 'N', '| night mov move moves |');
INSERT INTO `card` VALUES (1274, 'Nuclear Power Plant', 'Site', 1, 0x61, 0x61, 0, 1, 8, 'Limited. You cannot turn Sites at this location.', '"An accident? It''s all media hype. Trust me, there''s nothing to worry about."', '', 'Brian Rood', 1, 2, 3, 9, 'N', '| nuclear power plant |');
INSERT INTO `card` VALUES (1275, 'Nunchuks', 'State', 1, 0x4343, '', 0, 0, 0, 'Weapon. Turn to inflict 3 damage, divided any way you choose, on Characters at subject Character''s location.', 'Seems like everybody''s got a pair of these things nowadays.', '', 'nilson', 3, 3, 2, 9, 'N', '| nunchuk nunchuks |');
INSERT INTO `card` VALUES (1276, 'Outlaw Bikers', 'Easy Riders', 2, 0x6a6a, 0x6a, 2, 0, 0, 'Mobility. Once during your Main Shot, you may play a Weapon or Vehicle from your smoked pile on Outlaw Bikers.', 'They''re born losers.', '', 'Joe Mueller', 5, 10, 2, 9, 'N', '| outlaw biker bikers easy rider riders |');
INSERT INTO `card` VALUES (1277, 'Partners', 'Cop State', 1, '', '', 0, 0, 0, 'Play on a <Cop> Character you control and choose another <Cop> Character. Both Characters gain +1 Fighting.  When either Character is smoked, unturn the other, and it gains +1 Fighting until it leaves play.', '', '', 'Kenneth Waters', 3, 12, 2, 9, 'N', '| partner partners cop |');
INSERT INTO `card` VALUES (1278, 'Pinball Arcade', 'Feng Shui Site', 0, '', '', 0, 1, 6, 'When an attack you declared succeeds, turn to inflict 1 damage on any card in play.', '"He sure plays a mean pinball."', '', 'nilson', 1, 6, 2, 9, 'N', '| pinball arcade |');
INSERT INTO `card` VALUES (1279, 'Political Corruption', 'Edge', 2, 0x6161, '', 0, 0, 0, 'Generates 1 Power if you control at least one Feng Shui Site.', 'Watergate was just a distraction.', '', 'Patrick McEvoy', 2, 2, 3, 9, 'N', '| political corruption |');
INSERT INTO `card` VALUES (1280, 'Punks', 'Anarchist Rebels', 1, '', 0x6a, 1, 0, 0, '+2 damage against Unique cards.', '"Disco sucks!"', '', 'Cristian Saksida', 5, 10, 2, 9, 'N', '| punk punks anarchist rebel rebels |');
