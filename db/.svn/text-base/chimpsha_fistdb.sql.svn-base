INSERT INTO `card` VALUES (1081, '"Time to Kick Ass!"', 'Event', 0, 0x6464, '', 0, 0, 0, 'Play when an attack is declared against a card you control :: Unturn all your Characters.', '"What time is it, Sifu?"', '', 'Lissanne Lake', 4, 4, 6, 7, 'N', '| time kick as ass |');
INSERT INTO `card` VALUES (1082, 'Tong Su Yin', 'Master of the Snake Style', 5, 0x676767, 0x6743, 4, 0, 0, 'Unique. Tong Su Yin''s Fighting is increased by the number of cards in your hand.', '"They say that a snake can never strike farther than half its length. They lie."', '', 'Val Mayerik', 5, 8, 1, 7, 'N', '| tong su yin master snake style |');
INSERT INTO `card` VALUES (1083, '"Try My Kung Fu!"', 'Event', 0, 0x6767, '', 0, 0, 0, 'Not cumulative. Target <Monk>, <Master>, or <Martial> Character gains +1 Fighting and Toughness: 1 until the end of the turn.', '"There are only thirty-five of you..."', '', 'Lissanne Lake', 4, 8, 2, 7, 'N', '| try my kung fu |');
