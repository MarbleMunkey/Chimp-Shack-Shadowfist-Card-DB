INSERT INTO `card` VALUES (1035, 'Path of the Storm Turtle', 'State', 0, 0x4343, '', 0, 0, 0, 'Not cumulative. Subject Character gains Toughness: 1 against cards that are not [C] or [M], and are not the subject of a [C] or [M] State. You may substitute [a] for [C] when playing this card on a <Transformed> or <Lodge> Character.', '', '', 'Carisa Swenson', 3, 3, 2, 7, 'N', '| path storm turtle |');
