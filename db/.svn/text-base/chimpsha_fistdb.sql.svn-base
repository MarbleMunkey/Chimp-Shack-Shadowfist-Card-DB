INSERT INTO `card` VALUES (2396, 'Revenge on the Patent Office!', 'Weird Science Event', 1, 0x6a, '', 0, 0, 0, 'Inflict 2 damage on target Character and 2 damage on the front-row Site at its location.', '"How about this? Is this ''physically possible''?"', '', 'Ed Cox', 4, 10, 2, 15, 'N', '| revenge patent office weird science |');
INSERT INTO `card` VALUES (2393, 'Ranchers', 'Cowboy Heroes', 1, '', 0x64, 1, 0, 0, 'Toughness: 1 while the subject of any <Gun> States.', '"Territory folks should stick together."', '', 'Zak Hennessey', 5, 4, 2, 15, 'N', '| rancher ranchers cowboy hero heroe heroes |');
