INSERT INTO `card` VALUES (2384, 'One-Eye Chan', 'Chinatown Elder', 3, 0x6767, 0x6743, 4, 0, 0, 'Unique. Select State, Edge, Event, or Character when this card enters play. Once during your Main Shot, you may play one card of that type from an opponent''s smoked pile at regular cost, ignoring resource conditions. Events played in this manner are Toast It.', '', '', 'Matthew Laznicka', 5, 8, 1, 15, 'N', '| one eye chan chinatown elder |');
INSERT INTO `card` VALUES (2383, 'Nothing Happens', 'Event', 0, 0x70, '', 0, 0, 0, 'Cancel target Site until the end of the turn. Any opponent may pay you 1 Power to cancel this effect. Reload this card if they do so.', '"Wait for it... wait for it..."', '', 'Cristian Saksida', 4, 13, 3, 15, 'N', '| nothing happen happens |');
INSERT INTO `card` VALUES (2382, 'Nitro Jack', 'Unsavory Demolitions Expert', 3, 0x6a6a, 0x6a, 3, 0, 0, 'Unique. Uncopyable. Stealth. When he inflicts combat damage on a Site, and its Body is not reduced to 0, smoke it. When he is smoked while at a location you control, smoke a Site at that location.', '', '', 'Chris Quilliams', 5, 10, 1, 15, 'N', '| nitro jack unsavory demolition demolitions expert |');
INSERT INTO `card` VALUES (2380, 'Night Horror', 'Rogue Abomination', 3, 0x66, 0x66, 3, 0, 0, 'Ambush. Can be played during an opponent''s turn. May attack during an opponent''s turn.', 'With their handlers gone, many abominations ran amok.', '', 'Matt Dixon', 5, 1, 2, 15, 'N', '| night horror rogue abomination |');
