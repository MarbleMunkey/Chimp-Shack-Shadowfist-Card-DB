INSERT INTO `card` VALUES (2409, 'Simple Paper Fan', 'State', 0, 0x67, '', 0, 0, 0, 'Weapon. Play on a Character. Once per turn, you may redirect one damage that would be inflicted on subject to another Character at this location.', 'The monks could use two ounces to deflect a thousand pounds.', '', 'Matthew Laznicka', 3, 8, 2, 15, 'N', '| simple paper fan |');
