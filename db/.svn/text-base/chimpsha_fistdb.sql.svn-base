INSERT INTO `card` VALUES (1565, 'RedGlare Chapel', 'Netherworld Site', 2, 0x48, '', 0, 1, 6, 'Unique. Turn to unturn "Reverend RedGlare". Turn to inflict 1 damage on every other turned Site.', 'Thanks to its onboard computers, the Reverend''s deadly chapel is smarter than he is.', '', 'Anson Maddocks', 1, 9, 1, 12, 'N', '| red glare chapel netherworld |');
