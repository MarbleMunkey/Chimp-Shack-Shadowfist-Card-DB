INSERT INTO `card` VALUES (3275, 'Shadowy Mentor v2', 'State', 4, 0x61, '', 0, 0, 0, 'You control subject Character. Subject gains the designator <Pledged>.', '"I have done you many favors in the past, my young friend. Now I have one to ask in return..."', '', 'Dennis Detwiller', 3, 2, 2, 17, 'N', '| shadowy mentor v2 |');
