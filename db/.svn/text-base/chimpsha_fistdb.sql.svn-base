INSERT INTO `card` VALUES (2378, 'Naikute', 'Tribal Chief', 3, 0x6d6d6d, 0x6d, 5, 0, 0, 'Unique. When he enters play, you may return a Weapon to play from your smoked pile onto him.', '"Our braves have the strength of sky in their hearts, as well as bows in their hands."', '', 'Jonny Scull', 5, 7, 1, 15, 'N', '| naikute tribal chief |');
INSERT INTO `card` VALUES (2376, 'Moonlight Raid', 'Event', 0, 0x6161, '', 0, 0, 0, 'Until end of turn, target Character you control gains Stealth and you gain 1 Power when that Character inflicts combat damage on the target of its attack.', '', '', 'Matt Hughes', 4, 2, 2, 15, 'N', '| moonlight raid |');
