INSERT INTO `card` VALUES (1344, 'City Hospital', 'Feng Shui Site', 0, '', '', 0, 1, 8, 'Turn to remove 1 damage from a Character. Unturn this card when a Character you control is smoked.', 'All secret warriors eventually end up here, or in the morgue.', '', 'Mark Poole', 1, 6, 5, 11, 'N', '| city hospital |');
INSERT INTO `card` VALUES (1345, 'City Park', 'Feng Shui Site', 0, '', '', 0, 1, 8, 'Heal City Park at the end of the turn it is revealed. When a Site you control is burned, you may play City Park face up at no cost if you have not played a Site this turn.', '', '', 'Doug Shuler', 1, 6, 5, 11, 'N', '| city park |');
INSERT INTO `card` VALUES (1346, 'City Square', 'Feng Shui Site', 0, '', '', 0, 1, 6, 'Unique. Turn to redirect the damage inflicted by a single source on target Site to any Site you control.', 'The city''s chi flow is channeled and magnified by the many streets that converge here.', '', 'Ken Meyer, Jr.', 1, 6, 5, 11, 'N', '| city square |');
INSERT INTO `card` VALUES (1347, 'Claw of Fury', 'Shadowy Assassin', 2, '', 0x65, 2, 0, 0, 'Assassinate. When an opponent targets you (not a card or location you control) with an Event, you may play Claw of Fury from your hand at no cost.', '', '', 'Drew Tucker', 5, 5, 5, 11, 'N', '| claw fury shadowy assassin |');
INSERT INTO `card` VALUES (1348, 'Claws of Darkness', 'Triumvirate State', 1, 0x6d, '', 0, 0, 0, 'Weapon. Subject Character gains +1 Fighting. If subject is still in play at the end of any turn in which it smoked one or more Characters by inflicting combat damage, you gain 1 Power.', 'It only takes one scratch.', '', 'Heather Bruton', 3, 7, 5, 11, 'N', '| claw claws darkness darknes darkne darkn triumvirate |');
INSERT INTO `card` VALUES (1349, 'Close Call', 'Event', 1, 0x6a6a, '', 0, 0, 0, 'Redirect up to 3 damage from a single source that would damage a Character to a Site controlled by that source''s controller.', '"OK, that was a little too close."', '', 'Lissanne Lake', 4, 10, 5, 11, 'N', '| close call |');
INSERT INTO `card` VALUES (1350, 'Cognitive Spirit', 'Self-Aware Construct', 3, 0x7070, 0x704d, 4, 0, 0, 'Select a Character when this card attacks :: That Character may not intercept until the end of the attack.', '', '', 'Cristian Saksida', 5, 13, 5, 11, 'N', '| cognitive spirit self aware construct |');
INSERT INTO `card` VALUES (1351, 'Confucian Stability', 'Event', 1, 0x67, '', 0, 0, 0, 'Play in response to an Event or State :: Cancel and smoke that card.', 'Chance is only for those who have not embraced the Principle of Principles.', '', 'Richard Kane Ferguson', 4, 8, 5, 11, 'N', '| confucian stability |');
INSERT INTO `card` VALUES (1352, 'Curio Shop', 'Feng Shui Site', 0, '', '', 0, 1, 7, 'Takes no damage from opponents'' cards that have a cost of 1.', '"You''ve gotta have good chi to keep this much crockery intact."', '', 'Phil Foglio', 1, 6, 5, 11, 'N', '| curio shop |');
INSERT INTO `card` VALUES (1353, 'Dallas Rocket', 'Fanatic Guerrilla', 3, 0x6a, 0x6a, 3, 0, 0, 'Ambush against <PubOrd> or <Buro> Characters. Dallas Rocket inflicts +3 damage on the target of her attack.', '"I was in the Dallas uprisings. This is nothing."', '', 'Mark Poole', 5, 10, 5, 11, 'N', '| dalla dallas rocket fanatic guerrilla |');
INSERT INTO `card` VALUES (1354, 'Dangerous Experiment', 'Event', 0, 0x66666666, '', 0, 0, 0, 'Limited. Toast It. You gain 5 Power, and the opponent to your left may toast a card you control, a card in your smoked pile, or a card in your burned-for-victory pile.', 'The CDCA became ever more reckless as the civil war raged.', '', 'Heather Hudson', 4, 1, 5, 11, 'N', '| dangerou dangerous experiment |');
INSERT INTO `card` VALUES (1355, 'Darkness Warriors', 'Netherworld Soldiers', 2, '', 0x6d, 2, 0, 0, 'Sacrifice to gain 1 Power.', 'They would willingly die for their Queen, and they do; both on the battlefield and on the altar.', '', 'Jim Pavelec', 5, 7, 5, 11, 'N', '| darkness darknes darkne darkn warrior warriors netherworld soldier soldiers |');
INSERT INTO `card` VALUES (1356, 'Death-O-Rama', 'Event', 0, 0x6a6a, '', 0, 0, 0, 'Play during an attack. All Characters inflict +2 damage on Characters during the attack.', 'Lisa shouldn''t have taken the extra propane tanks to the rendezvous.', '', 'Pete Venters', 4, 10, 5, 11, 'N', '| death o rama |');
INSERT INTO `card` VALUES (1357, 'Derek Han', 'Martial Artist', 6, 0x676767, 0x6743, 11, 0, 0, 'Unique. Pay 1 Power to give him Superleap until the end of the turn. Pay 2 Power :: He is not affected by Events until the end of the turn.', 'Shih Ho Kuai''s best student.', '', 'Jeremy Dale', 5, 8, 5, 11, 'N', '| derek han martial artist |');
INSERT INTO `card` VALUES (1358, 'Die!!!', 'Event', 0, 0x656565, '', 0, 0, 0, 'Turn X [e] Characters you control to smoke any number of opponents'' Characters whose total cost is less than X+2. (X may be zero.)', '"They dare to defy me? Kill them! Kill them all!"', '', 'April Lee', 4, 5, 5, 11, 'N', '| die |');
INSERT INTO `card` VALUES (1359, 'Dirk Wisely''s Gambit', 'Event', 0, 0x64, 0x6448, 0, 0, 0, 'Limited. Play when you declare an attack with only one attacker against a target controlled by an opponent who controls at least four cards. When your attacker inflicts combat damage on its target during this attack, gain 2 Power.', '', '', 'Mark Poole', 4, 4, 5, 11, 'N', '| dirk wisely wiselys gambit |');
INSERT INTO `card` VALUES (1360, 'Discerning Fire', 'Event', 100, 0x4d4d, '', 0, 0, 0, 'Smoke X target cards that share a designator other than <Netherworld>. X must be greater than one. Power-generating Sites are not legal targets.', '', '', 'Brian Snoddy', 4, 11, 5, 11, 'N', '| discerning fire |');
INSERT INTO `card` VALUES (1361, 'Drug Lab', 'Hood Site', 1, '', '', 0, 1, 6, 'Limited. Smoke Drug Lab when it is damaged by a <Cop> card.', 'There''s always been a brisk business selling fools their own destruction.', '', 'John T. Snyder', 1, 12, 5, 11, 'N', '| drug lab hood |');
INSERT INTO `card` VALUES (1362, 'Edge Warrior', 'Subversive Op', 1, '', 0x6a, 1, 0, 0, 'Cannot be intercepted by <Buro>, <PubOrd>, or <Cop> Characters.', 'She can sniff the whiff of law enforcement at a hundred paces.', '', 'Julie Baroh', 5, 10, 5, 11, 'N', '| warrior subversive op |');
INSERT INTO `card` VALUES (1363, 'Entropy Sphere', 'Event', 1, 0x7070, '', 0, 0, 0, 'Smoke target Character and inflict X damage on a front-row Feng Shui Site you control. X= the target''s cost.', '"Destruction has its price."', '', 'Arthur Roberg', 4, 13, 5, 11, 'N', '| entropy sphere |');
INSERT INTO `card` VALUES (1364, 'Entropy Tap', 'Event', 0, 0x704d, '', 0, 0, 0, 'Limited. Play when a Site you control takes combat damage but its Body is not reduced to 0 :: Gain X Power. X= half (round down) the damage inflicted.', 'Most geomancers will repair damaged chi. The Purists scavenge it.', '', 'Tony Moseley', 4, 13, 5, 11, 'N', '| entropy tap |');
INSERT INTO `card` VALUES (1365, 'Everyday Hero', 'Brave Scrapper', 2, '', 0x64, 2, 0, 0, 'Guts.', 'If justice is to prevail in the secret war, ordinary people all around the world must stand up for what''s right.', '', 'Liz Danforth', 5, 4, 5, 11, 'N', '| everyday hero brave scrapper |');
INSERT INTO `card` VALUES (1366, 'Ex-Commando', 'Dangerous Vet', 3, 0x6464, 0x64, 3, 0, 0, 'Ex-Commando unturns when a Weapon or <Gun> State is played on him.', 'Secretly, he''s glad peacetime isn''t so peaceful.', '', 'Steve Bryant', 5, 4, 5, 11, 'N', '| ex commando dangerou dangerous vet |');
INSERT INTO `card` VALUES (1367, 'Explosives', 'State', 1, '', '', 0, 0, 0, 'Play on an unturned Character. Subject''s controller may sacrifice Explosives during an attack :: Subject inflicts +5 damage against the next Site it inflicts combat damage on during this attack.', '"Will that be nitro or C-4 today, Potemkin?" (Jammer deck) "Are you sure Tsien? Potemkin took the C4." (Lotus deck)', '', 'Dan Frazier', 3, 12, 5, 11, 'N', '| explosiv explosive explosives |');
INSERT INTO `card` VALUES (1368, 'Faked Death', 'Event', 0, 0x61, '', 0, 0, 0, 'Return a Character from your smoked pile to your hand.', 'The Lodge can write you a death certificate any time they want.', '', 'L.A. Williams', 4, 2, 5, 11, 'N', '| faked death |');
INSERT INTO `card` VALUES (1369, 'Family Estate', 'Site', 2, '', 0x61, 0, 1, 5, 'Turn to play a <Pledged> Character at -1 cost. When a <Lodge> Character is smoked, inflict 2 damage on Family Estate.', 'The Pledged do the Lodge''s dirty work.', '', 'Andi Rusu', 1, 2, 5, 11, 'N', '| family estate |');
INSERT INTO `card` VALUES (1370, 'Family Home', 'Feng Shui Site', 0, '', '', 0, 1, 5, 'Generates an additional Power if the player to your left has more Power than you.', '"This is why I keep fighting."', '', 'Ken Meyer, Jr.', 1, 6, 5, 11, 'N', '| family home |');
INSERT INTO `card` VALUES (1371, 'Father of Chaos', 'Spirit of Destruction', 6, 0x7070704d, 0x704d, 11, 0, 0, 'Unique. Father of Chaos takes no combat damage from Characters whose Fighting is less than the number of cards in their controller''s hand.', '', '', 'Cristian Saksida', 5, 13, 5, 11, 'N', '| father chao chaos spirit destruction |');
INSERT INTO `card` VALUES (1372, 'Festival Circle', 'Feng Shui Site', 0, '', '', 0, 1, 6, 'Cannot be healed except when seized. If Festival Circle is in your front row, turn and inflict 2 damage on it in response to an Event that targets one or more Characters you control :: Cancel that Event.', '', '', 'Nicola Leonard', 1, 6, 5, 11, 'N', '| festival circle |');
INSERT INTO `card` VALUES (1373, 'Field of Tentacles', 'Netherworld Feng Shui Site', 0, '', '', 0, 1, 8, 'Turn and maintain to cancel the effects and rules text of target non-Feng Shui Site controlled by an opponent.', 'Tentacles coming up through the floorboards can really drive down property values.', '', 'Mark Tedin', 1, 6, 5, 11, 'N', '| field tentacl tentacle tentacles netherworld |');
INSERT INTO `card` VALUES (1374, 'Final Brawl', 'Event', 0, 0x64, '', 0, 0, 0, 'Inflict 2 damage on all Characters.', 'Hamlet, Oedipus, Dirty Harry - the classic stories always end in blood.', '', 'Richard Kane Ferguson', 4, 4, 5, 11, 'N', '| final brawl |');
INSERT INTO `card` VALUES (1375, 'Fire Acolytes', 'Netherworld Disciples', 1, '', 0x6d4d, 1, 0, 0, 'When Fire Acolytes are smoked, you may inflict 1 damage on a Character at their location if you control another <Fire> Character.', 'The first step on the path of the Fire Righteous.', '', 'Chris Chuckry', 5, 7, 5, 11, 'N', '| fire acolyt acolyte acolytes netherworld discipl disciple disciples |');
INSERT INTO `card` VALUES (1376, 'Fire Mystic', 'Netherworld Sorcerer', 3, 0x6d6d, 0x6d4d, 4, 0, 0, 'When an opponent plays an Event, you may inflict 1 damage on a card that player controls.', '"Li Ting''s followers understand the fine art of retribution."', '', 'Mike Jackson', 5, 7, 5, 11, 'N', '| fire mystic netherworld sorcerer |');
INSERT INTO `card` VALUES (1377, 'Fireworks Factory', 'Feng Shui Site', 0, '', '', 0, 1, 7, 'When an opponent reduces Fireworks Factory''s Body to 0, you may smoke a Character that opponent controls if he or she is now closer to victory than you.', '"Not the wisest place to have a gunfight."', '', 'David Deen', 1, 6, 5, 11, 'N', '| firework fireworks factory |');
INSERT INTO `card` VALUES (1378, 'Floating Restaurant', 'Feng Shui Site', 0, '', '', 0, 1, 8, 'When an opponent plays an Event, you may remove 1 damage from any card you control.', '"Take the boat to Chu''s, then tell the waiter that you want the Sunless Seabass. He''ll hook you up."', '', 'Jeff Menges', 1, 6, 5, 11, 'N', '| floating restaurant |');
INSERT INTO `card` VALUES (1379, 'Flying Sleeves', 'Event', 0, 0x6565, '', 0, 0, 0, 'Toast It. Change one attacker''s target to any Character you control.', 'Most martial artists block with their arms. Kan Li''s students prefer to block with other people.', '', 'Mark Tedin', 4, 5, 5, 11, 'N', '| flying sleev sleeve sleeves |');
INSERT INTO `card` VALUES (1380, 'Flying Sword Stance', 'State', 0, 0x4343, '', 0, 0, 0, 'Not cumulative. Subject Character gains +1 Fighting. Subject gains Superleap against Characters that lack resource conditions.', '"Focus, balance, determination. These will allow us to win this day."', '', 'Carisa Swenson', 3, 3, 5, 11, 'N', '| flying sword  stance |');
INSERT INTO `card` VALUES (1381, 'Four Burning Fists', 'Demon Martial Artist', 3, 0x6565654d, 0x65, 5, 0, 0, 'Unique. You must toast a Feng Shui Site you control when Four Burning Fists enters play, otherwise toast this card. Gains +2 Fighting and Regenerate while any opponent is closer to victory than you.', '', '', 'Kenneth Waters', 5, 5, 5, 11, 'N', '| four burning fist fists demon martial artist |');
INSERT INTO `card` VALUES (1382, 'Fox Pass', 'Feng Shui Site', 0, '', '', 0, 1, 5, 'Unique. Turn to change one attacker''s target to any Character or front-row Site you control.', 'Be as the fox, who leads the hounds astray.', '', 'Heather Bruton', 1, 6, 5, 11, 'N', '| fox pas pass |');
INSERT INTO `card` VALUES (1383, 'Furious George', 'Flying Monkey', 6, 0x6a6a6a48, 0x6a48, 10, 0, 0, 'Unique. Guts. When Furious George enters combat, inflict 1 damage on each Character he is in combat with. (Before combat damage is dealt.)', '', '', 'Paul "Prof" Herbert', 5, 10, 5, 11, 'N', '| furiou furious george flying monkey |');
INSERT INTO `card` VALUES (1384, 'Gambling House', 'Feng Shui Site', 0, '', '', 0, 1, 6, 'Limited. If any opponent controls at least two cards that share a designator, you may turn this card during your Main Shot to gain 1 Power.', 'Dice, cards, tiles, whatever - if it''s played for money, you can find it here.', '', 'Stephen Snyder', 1, 6, 5, 11, 'N', '| gambling house |');
INSERT INTO `card` VALUES (1385, 'Garden of Bronze', 'Netherworld Feng Shui Site', 0, '', '', 0, 1, 8, 'Gain 3 Power when an opponent burns one of your other Feng Shui Sites for Power.', 'It''s both an art installation and chi-feedback resonance enhancer, Johnny.', '', 'Nicola Leonard', 1, 6, 5, 11, 'N', '| garden bronze netherworld |');
INSERT INTO `card` VALUES (1386, 'Genghis X', 'BuroMil Supersoldier', 5, 0x666648, 0x66, 8, 0, 0, 'Unique. Genghis X has Guts while there is at least three damage on him.', '', '', 'Jeff Menges', 5, 1, 5, 11, 'N', '| genghi genghis x buro mil super soldier |');
INSERT INTO `card` VALUES (1387, 'Glimpse of the Abyss', 'Event', 0, 0x656565, '', 0, 0, 0, 'Toast It. Gain X Power. X= the number of Feng Shui Sites controlled by target opponent plus the number of cards in his or her burned-for-victory pile. The number of Feng Shui Sites you need for victory is increased by one.', '"Yessss! I am ssso evil!"', '', 'Mike Raabe', 4, 5, 5, 11, 'N', '| glimpse abys abyss |');
INSERT INTO `card` VALUES (1388, 'Golden Candle Society', 'Secret Society', 1, '', 0x6743, 1, 0, 0, '', 'They have sworn to drive the foreign invader from the proud soil of their native land.', '', 'Margaret Organ-Kean', 5, 8, 5, 11, 'N', '| golden candle society secret |');
INSERT INTO `card` VALUES (1389, 'Golden Comeback', 'Event', 2, 0x64, '', 0, 0, 0, 'Choose a Character in your smoked pile :: Return that Character to play.', '"It''ll take more than nine slugs in the chest to put me down when there are still creeps like you in the world!"', '', 'Nene Thomas', 4, 4, 5, 11, 'N', '| golden comeback |');
INSERT INTO `card` VALUES (1390, 'Golden Mile', 'Feng Shui Site', 0, '', '', 0, 1, 8, 'Unique. Turn to gain 1 Power when an opponent spends 3 or more Power at one time.', 'If you have the right chi, you get the best deals.', '', 'Mark Poole', 1, 6, 5, 11, 'N', '| golden mile |');
INSERT INTO `card` VALUES (1391, 'Gorilla Fighter', 'Flying Monkey', 4, 0x6a6a48, 0x6a48, 6, 0, 0, 'Independent. Gorilla Fighter''s cost is reduced by the total number of Feng Shui Sites and Characters you own that are controlled by opponents or in burned-for-victory piles.', '', '', 'Anson Maddocks', 5, 10, 5, 11, 'N', '| gorilla fighter flying monkey |');
INSERT INTO `card` VALUES (1392, 'Gruff Lieutenant', 'Pledged Cop', 1, 0x61, 0x61, 1, 0, 0, 'Stealth.', 'The Order of the Wheel recruits high-ranking police officers to keep them hip to the action on the streets.', '', 'Doug Shuler', 5, 2, 5, 11, 'N', '| gruff lieutenant pledged cop |');
INSERT INTO `card` VALUES (1393, 'Hacker', 'Gun-Totin'' Techie', 2, '', 0x6448, 2, 0, 0, 'When an opponent plays an Event that steals Power from you or that toasts any card other than itself, you may play Hacker from your hand at no cost in response :: Cancel that Event.', '', '', 'Mark Tedin', 5, 4, 5, 11, 'N', '| hacker gun  totin techie |');
INSERT INTO `card` VALUES (1394, 'Hands Without Shadow v2', 'State', 0, 0x43, '', 0, 0, 0, 'Schtick. When subject Character is in combat with a Character, you may choose to have subject inflict X-1 combat damage instead of its normal damage. X= damage required to smoke the opposing Character, calculated just prior to inflicting damage, but after all other effects have resolved.', '', '', 'Edward Beard, Jr.', 3, 3, 5, 11, 'N', '| hand hands without shadow v2 |');
INSERT INTO `card` VALUES (1395, 'Healing Earth', 'Event', 0, 0x43, '', 0, 0, 0, 'Remove up to X damage from cards you control. X= the number of [C] resources in your pool.', 'The regenerative powers of the earth are triggered by the transcendent soul.', '', 'Nicola Leonard', 4, 3, 5, 11, 'N', '| healing earth |');
INSERT INTO `card` VALUES (1396, 'Heat of Battle', 'Event', 0, 0x67, '', 0, 0, 0, 'Play during an attack on a card you control by an opponent who controls more Power-generating Sites than you. Gain Power equal to the cost of target attacking Character. You can play only one "Heat of Battle" each attack.', '', '', 'Margaret Organ-Kean', 4, 8, 5, 11, 'N', '| heat battle |');
INSERT INTO `card` VALUES (1397, 'Helix Rethread', 'State', 0, 0x66, '', 0, 0, 0, 'Play on a Character controlled by an opponent. When subject is smoked, you gain 1 Power.', '"Oops, forgot to have you sign the consent form! Ha ha ha haha!"', '', 'Ron Spencer', 3, 1, 5, 11, 'N', '| helix rethread |');
INSERT INTO `card` VALUES (1398, 'Hidden Sanctuary', 'Feng Shui Site', 0, '', '', 0, 1, 7, 'Limited. At the end of an attack you declared, you may remove up to X damage from each Character that attacked. X = the number of <Sanctuary> cards you control.', '', '', 'Heather Hudson', 1, 6, 5, 11, 'N', '| hidden sanctuary |');
INSERT INTO `card` VALUES (1399, 'Homemade Tank', 'State', 2, 0x6a48, '', 0, 0, 0, 'Toast It. Vehicle. Subject Character gains Toughness: 1 and +4 Fighting. Flip a coin at the start of your turn. Heads: Toast Homemade Tank.', '"The engine''s as big a threat as enemy fire."', '', 'Dan Frazier', 3, 10, 5, 11, 'N', '| homemade tank |');
INSERT INTO `card` VALUES (1400, 'Ice Diadem', 'State', 1, 0x6d4d, '', 0, 0, 0, 'Turn Ice Diadem to heal subject Character.', 'Pi Tui''s healing kiss brushes your forehead.', '', 'Dianna Vick', 3, 7, 5, 11, 'N', '| ice diadem |');
INSERT INTO `card` VALUES (1401, 'Ice Healer', 'Netherworld Sorceress', 1, '', 0x6d4d, 1, 0, 0, 'When an opponent turns a Character to heal, you may remove one damage from a Character you control.', '', '', 'Daniel Gelon', 5, 7, 5, 11, 'N', '| ice healer netherworld sorceres sorceress |');
INSERT INTO `card` VALUES (1402, 'Inner Sanctum', 'Feng Shui Site', 0, '', '', 0, 1, 11, 'If Inner Sanctum is revealed, you may not place another Feng Shui Site at its location.', 'Too great a chi flow can be as hazardous as too little.', '', 'Jeff Menges', 1, 6, 5, 11, 'N', '| inner sanctum |');
INSERT INTO `card` VALUES (1403, 'Inoue Oram', 'Sorcerer Mastermind', 2, 0x70, 0x704d, 2, 0, 0, 'Unique. If you drew any cards during your draw phase, gain 1 Power at the start of your Main Shot if the number of cards drawn was even.', '', '', 'Melissa Benson', 5, 13, 5, 11, 'N', '| inoue oram sorcerer mastermind |');
INSERT INTO `card` VALUES (1404, 'Instrument of the Hand', 'Martial Artist', 2, '', 0x6743, 2, 0, 0, '', '"Even the noodle lady turned out to be a guerrilla fighting against the foreign powers."', '', 'Quinton Hoover', 5, 8, 5, 11, 'N', '| instrument hand martial artist |');
INSERT INTO `card` VALUES (1405, 'Iron and Silk', 'Event', 0, 0x67, '', 0, 0, 0, 'Choose any Character :: Until the end of the turn, that Character takes no combat damage while intercepting.', 'Embrace the yin principle, and receive your opponents'' blows like gifts of honey.', '', 'Nene Thomas', 4, 8, 5, 11, 'N', '| iron silk |');
INSERT INTO `card` VALUES (1406, 'Isomorphic Spirit', 'Paradox Construct', 4, 0x7070, 0x70, 6, 0, 0, '+2 Fighting while two or more non-Edge cards in play have the same title.', 'Truly fearful symmetry.', '', 'Alexander Leonard', 5, 13, 5, 11, 'N', '| isomorphic spirit paradox construct |');
INSERT INTO `card` VALUES (1407, 'Jaded Cop', 'Corrupt Pawn', 3, 0x61, 0x61, 3, 0, 0, 'Not affected by 0 or 1-cost Events.', 'Nothing shocks you after walking a beat for 15 years.', '', 'Ellym Sirac', 5, 2, 5, 11, 'N', '| jaded cop corrupt pawn |');
INSERT INTO `card` VALUES (1408, 'Jade Valley', 'Feng Shui Site', 0, '', '', 0, 1, 7, 'Limited. While any Feng Shui Site you own is controlled by an opponent or in an opponent''s burned-for-victory pile, and you are not closer to victory than all opponents, Jade Valley generates +1 Power and gains +4 Body.', '', '', 'Matt Cavotta', 1, 6, 5, 11, 'N', '| jade valley |');
INSERT INTO `card` VALUES (1409, 'Johnny Tso', 'Heroic Gunman', 3, 0x646464, 0x64, 4, 0, 0, 'Unique. Assassinate against [e] Characters. You may play Weapons on Johnny Tso at no cost.', '', '', 'Edward Beard, Jr.', 5, 4, 5, 11, 'N', '| johnny tso heroic gun man |');
INSERT INTO `card` VALUES (1410, 'Just Another Consumer', 'Scrappy Rebel', 1, '', 0x6a, 1, 0, 0, 'When Just Another Consumer turns to attack a Site that shares a designator with another Site in play, he gains +1 Fighting until the end of the turn.', '', '', 'Dianna Vick', 5, 10, 5, 11, 'N', '| just another consumer scrappy rebel |');
INSERT INTO `card` VALUES (1411, 'Kallisti', 'Daughter of Entropy', 5, 0x7070, 0x704d, 8, 0, 0, 'Unique. Flip a coin when Kallisti smokes a Character by inflicting combat damage.  Heads: remove 3 damage from her and you may inflict 3 damage on any Character.', '', '', 'Brian Rood', 5, 13, 5, 11, 'N', '| kallisti daughter entropy |');
INSERT INTO `card` VALUES (1412, 'Kan Li', 'Martial Artist', 4, 0x656565, 0x65, 7, 0, 0, 'Unique. When Kan Li enters combat, you may smoke any States on Characters he is in combat with.', '"You defy the Lotus. Prepare to die."', '', 'Daniel Gelon', 5, 5, 5, 11, 'N', '| kan li martial artist |');
INSERT INTO `card` VALUES (1413, 'Karate Cop', 'Martial Artist', 3, 0x64, 0x64, 4, 0, 0, 'Gains +2 damage and Toughness: 1 while any opponent is closer to victory than you.', 'Law-enforcement, HK style.', '', 'Rob Thomas', 5, 4, 5, 11, 'N', '| karate cop martial artist |');
INSERT INTO `card` VALUES (1414, 'Kar Fai''s Crib', 'Site', 2, 0x64, 0x6443, 0, 1, 7, 'Unique. You may play [d] States at -1 cost. Turn to play a Character, ignoring one resource condition.', 'The Dragons remember, and they keep up the fight.', '', 'Doug Shuler', 1, 4, 5, 11, 'N', '| kar fai fais crib |');
INSERT INTO `card` VALUES (1415, 'Kinoshita House', 'Feng Shui Site', 0, '', '', 0, 1, 4, 'Unique. Turn to unturn target attacker. That attacker ceases attacking.', 'The sublime meditation cottage of the great ninja leader Kinoshita retains the essence of his spirit.', '', 'Kumi Yamashita', 1, 6, 5, 11, 'N', '| kinoshita house |');
INSERT INTO `card` VALUES (1416, 'Kung Fu Student', 'Fledgling Shaolin', 1, '', 0x67, 1, 0, 0, '+1 Fighting while any non-Unique Feng Shui Site in your front row has a Body of 4 or less.', 'The Guiding Hand has no shortage of students eager to prove themselves.', '', 'Doug Shuler', 5, 8, 5, 11, 'N', '| kung fu student fledgling shaolin |');
INSERT INTO `card` VALUES (1417, 'Liquidators', 'Pledged Enforcers', 3, '', 0x61, 3, 0, 0, '+1 damage while you control any <Lodge> Characters.', 'The Lodge has assets on both sides of the law.', '', 'Richard Thomas', 5, 2, 5, 11, 'N', '| liquidator liquidators pledged enforcer enforcers |');
INSERT INTO `card` VALUES (1418, 'Lord Hawksmoor', 'Thunder Champion', 5, 0x6d6d, 0x6d, 8, 0, 0, 'Unique. Guts. Gain 2 Power if he is smoked by an opponent.', '', '', 'Cristian Saksida', 5, 7, 5, 11, 'N', '| lord hawksmoor thunder champion |');
INSERT INTO `card` VALUES (1419, 'Lord Shi', 'Netherworld Warrior', 4, 0x6d6d, 0x6d, 6, 0, 0, 'Unique. When Lord Shi attacks, until the end of the attack, all attackers (including him) gain +1 Fighting and Tactics.', 'Only he knows what the Monarchs could have been.', '', 'Roberto Campus', 5, 7, 5, 11, 'N', '| lord shi netherworld warrior |');
INSERT INTO `card` VALUES (1420, 'Magog', 'Unstoppable Abomination', 6, 0x6666664848, 0x66, 11, 0, 0, 'Unique. Guts. When an opponent''s Site generates an effect while Magog is attacking, inflict 2 damage on that Site.', '', '', 'Cristian Saksida', 5, 1, 5, 11, 'N', '| magog unstoppable abomination |');
INSERT INTO `card` VALUES (1421, 'Mark of Fire', 'Event', 1, 0x6d, '', 0, 0, 0, 'Target exactly four Characters and/or Sites :: Inflict 1 damage on each target.', '"Li Ting''s idea of equality is everybody on fire at the same time."', '', 'John T. Snyder', 4, 7, 5, 11, 'N', '| mark fire |');
INSERT INTO `card` VALUES (1422, 'Mathemagician', 'Arcane Student', 1, '', 0x704d, 1, 0, 0, 'Provides an additional [p] while in play.', '"Once something is absolutely understood, it becomes mathematics, even magic."', '', 'Randy Asplund', 5, 13, 5, 11, 'N', '| mathemagician arcan e student |');
INSERT INTO `card` VALUES (1423, 'Mole Network', 'Event', 0, 0x61, '', 0, 0, 0, 'Play during your Main Shot. Target an opponent who has completed at least one turn :: Steal 1 Power from that opponent.', '"There is something you should know. Meet me at the usual place at 9:15 sharp."', '', 'Mike Kimble', 4, 2, 5, 11, 'N', '| mole network |');
INSERT INTO `card` VALUES (1424, 'Napalm Addict', 'Pyromaniac Anarchist', 4, 0x6a6a, 0x6a, 6, 0, 0, 'Guts while attacking. When he smokes a Site by inflicting combat damage, gain Power equal to that Site''s printed cost, or 2 Power if it has no printed cost.', '', '', 'Heather Bruton', 5, 10, 5, 11, 'N', '| napalm addict pyromaniac anarchist |');
INSERT INTO `card` VALUES (1425, 'Nerve Gas', 'Event', 1, 0x66, '', 0, 0, 0, 'Smoke target Character. Does not affect Characters with [M] or [H] resource conditions.', 'When you journey to the year 2062, always remember to pack a gas mask.', '', 'Dan Frazier', 4, 1, 5, 11, 'N', '| nerve ga gas |');
INSERT INTO `card` VALUES (1426, 'Neutron Bomb', 'Event', 3, 0x666648, '', 0, 0, 0, 'Smoke all Characters.', 'Just what the doctor ordered for the lingering urban resistance problems in 2062 - mondo death toll with no property value depreciation.', '', 'Dan Frazier', 4, 1, 5, 11, 'N', '| neutron bomb |');
INSERT INTO `card` VALUES (1427, 'Nightclub', 'Feng Shui Site', 0, '', '', 0, 1, 7, 'Turn to inflict 1 damage on target interceptor.', 'The hottest spots have the best chi.', '', 'Mark Poole', 1, 6, 5, 11, 'N', '| nightclub |');
INSERT INTO `card` VALUES (1428, 'Night Market', 'Feng Shui Site', 0, '', '', 0, 1, 8, 'Limited. Turn during your turn in response to an opponent''s Event :: Gain Power equal to that Event''s cost.', '', '', 'Anthony Waters', 1, 6, 5, 11, 'N', '| night market |');
INSERT INTO `card` VALUES (1429, 'Nine Dragon Temple', 'Feng Shui Site', 0, '', '', 0, 1, 7, 'When an opponent reduces Nine Dragon Temple''s Body to 0, gain 3 Power if that opponent is now closer to victory than you.', 'Those deserving of fortune will find it when faced with oppression.', '', 'Ellym Sirac', 1, 6, 5, 11, 'N', '| nine dragon temple |');
INSERT INTO `card` VALUES (1430, '"Now You''ve Made Us Mad"', 'Event', 0, 0x6464, '', 0, 0, 0, 'Gain 1 Power for each damaged Character you control.', 'Nietzsche said: "That which does not kill us makes us kick more butt." Or something like that.', '', 'Mark Poole', 4, 4, 5, 11, 'N', '| now youve made u us mad |');
INSERT INTO `card` VALUES (1431, 'Operation Killdeer', 'Event', 0, 0x61, '', 0, 0, 0, 'Until the end of the turn, target Character inflicts no damage.', 'The Dragons feel the loss of Jack and Mad Dog in every running gun battle and every wild fight.', '', 'Ron Rousselle', 4, 2, 5, 11, 'N', '| operation killdeer |');
INSERT INTO `card` VALUES (1432, 'Pain Feedback', 'State', 0, 0x70, '', 0, 0, 0, 'One point of subject Character''s combat damage is redirected back onto itself.', 'It kills you slowly.', '', 'Jason Millet', 3, 13, 5, 11, 'N', '| pain feedback |');
INSERT INTO `card` VALUES (1433, 'Paradox Garden', 'Site', 2, '', 0x70, 0, 1, 5, 'Turn to look at the top card of your deck.  You may put it on the bottom.', '"It makes the Mobius Garden seem normal."', '', 'Patrick McEvoy', 1, 13, 5, 11, 'N', '| paradox garden |');
INSERT INTO `card` VALUES (1434, 'Payback Time', 'Edge', 0, 0x6a, '', 0, 0, 0, 'Limited. When an opponent reduces the Body of a Site you control to 0, you gain 1 Power and may inflict 1 damage each on up to 3 Sites controlled by that opponent.', '', '', 'Lissanne Lake', 2, 10, 5, 11, 'N', '| payback time |');
INSERT INTO `card` VALUES (1435, 'Phillippe Benoit', 'Pledged Assassin', 4, 0x6161, 0x61, 5, 0, 0, 'Unique. Stealth.', '"Your pleading for mercy reminds me of a line from Sartre."', '', 'Doug Shuler', 5, 2, 5, 11, 'N', '| phillippe benoit pledged assassin |');
INSERT INTO `card` VALUES (1436, 'Pocket Demon', 'Event', 0, 0x4d, '', 0, 0, 0, 'Limited. Play at the start of your turn. No other card you control or play generates Power during your Establishing Shot this turn. Pocket Demon generates Power equal to the number of Power-generating Sites controlled by target opponent, plus the number of cards in his or her burned-for-victory pile, minus the number of cards in your burned-for-victory pile.', '', '', 'Ron Spencer', 4, 11, 5, 11, 'N', '| pocket demon |');
INSERT INTO `card` VALUES (1437, 'Poison Thorns', 'Dangerous Hoods', 3, 0x65, 0x65, 4, 0, 0, 'Toughness: 2 against <Cop> and <Police> cards. When they inflict combat damage on a Site controlled by a player who is closer to victory than you, gain 1 Power.', '', '', 'Nene Thomas', 5, 5, 5, 11, 'N', '| poison thorn thorns dangerou dangerous hood hoods |');
INSERT INTO `card` VALUES (1438, 'Professional Killer', 'Mercenary Hood', 3, 0x6a, 0x6a, 4, 0, 0, 'Stealth while attacking alone.', 'He''s just in it for the money.', '', 'Alexander Leonard', 5, 10, 5, 11, 'N', '| professional killer mercenary hood |');
INSERT INTO `card` VALUES (1439, 'Pump-Action Shotgun', 'Gun State', 1, '', '', 0, 0, 0, 'Weapon. When a Unique Character you control is smoked by an opponent, unturn this card. Turn this card :: subject Character inflicts +3 damage until the end of the turn.', '"This time..." KA-CHINK "...I''m going for a little redemption action."', '', 'Drew Tucker', 3, 12, 5, 11, 'N', '| pump action gun  |');
INSERT INTO `card` VALUES (1440, 'Purist Aspirant', 'Expendable Lackey', 1, '', 0x70, 1, 0, 0, 'Sacrifice to reduce to 0 all non-combat damage from a single source to Characters you control.', '', '', 'Arthur Roberg', 5, 13, 5, 11, 'N', '| purist aspirant expendable lackey |');
INSERT INTO `card` VALUES (1441, 'Quantum Sorcery', 'Edge', 1, 0x70704d4d, '', 0, 0, 0, 'Turn this card and a [M] Character you control :: Gain one Power or draw a card.', '"The Purists have developed a system of magic unlike any I have seen before." - Fo Shen', '', 'Dennis Calero', 2, 13, 5, 11, 'N', '| quantum sorcery |');
INSERT INTO `card` VALUES (1442, 'Rachel McShane', 'Lodge Enforcer', 5, 0x616161, 0x61, 8, 0, 0, 'Unique. Rachel McShane is not affected by opponents'' effects that are generated by turning a card.', 'An eagle can spot its prey from a mile above.', '', 'Val Mayerik', 5, 2, 5, 11, 'N', '| rachel mc shane lodge enforcer |');
INSERT INTO `card` VALUES (1443, 'Realpolitik', 'Event', 1, 0x61, '', 0, 0, 0, 'Smoke target Edge or State.', 'The Ascended will - and can - do nearly anything to maintain their power over our world.', '', 'Mike Kimble', 4, 2, 5, 11, 'N', '| realpolitik |');
INSERT INTO `card` VALUES (1444, 'Redeemed Gunman', 'Reforming Hood', 1, '', 0x64, 1, 0, 0, 'Cannot intercept. When Redeemed Gunman turns to attack a turned Site, he gains +2 Fighting until the end of the turn.', '', '', 'Melissa Benson', 5, 4, 5, 11, 'N', '| redeemed gun man reforming hood |');
INSERT INTO `card` VALUES (1445, 'Resistance Squad', 'Techie Guerrillas', 2, '', 0x6a48, 2, 0, 0, 'Any time Resistance Squad is in your hand when an opponent attempts to look at your hand or force you to discard a card, you may play Resistance Squad at no cost in response :: Cancel that effect.', '', '', 'Heather Hudson', 5, 10, 5, 11, 'N', '| resistance squad techie guerrilla guerrillas |');
INSERT INTO `card` VALUES (1446, 'Rust Garden', 'Netherworld Feng Shui Site', 0, '', '', 0, 1, 7, 'If an opponent seizes or burns Rust Garden, inflict 2 damage on every other Site he or she controls.', '"Right after we blew it up, we found old mufflers in the Sacred Grove and a junked ''bot in the Lily Pond."', '', 'Matt Wilson', 1, 6, 5, 11, 'N', '| rust garden netherworld |');
INSERT INTO `card` VALUES (1447, 'Scrounging', 'Event', 0, 0x6a, '', 0, 0, 0, 'Limited. Play at the start of your turn. No other card you control or play generates Power during your Establishing Shot. Choose State, Edge, Character, Feng Shui Site, or non-Feng Shui Site. Scrounging generates Power equal to the number of cards of the chosen type that target opponent controls, minus the number of cards in your burned-for-victory pile.', '', '', 'Paul Carrick', 4, 10, 5, 11, 'N', '| scrounging |');
INSERT INTO `card` VALUES (1448, 'Security', 'State', 1, 0x61, '', 0, 0, 0, 'Subject Site gains +4 Body.', '"Remote cameras, electronic sensors, automated machine gun emplacements - all they cost is money, and the Ascended have that to burn."', '', 'Jeff Menges', 3, 2, 5, 11, 'N', '| security |');
INSERT INTO `card` VALUES (1449, 'Shadowy Mentor', 'State', 3, 0x61, '', 0, 0, 0, 'Play on any Character. You take control of subject Character. Subject gains the designator <Pledged>.', '"I have done you many favors in the past, my young friend. Now, I have one to ask in return..."', '', 'Dennis Detwiller', 3, 2, 5, 11, 'N', '| shadowy mentor |');
INSERT INTO `card` VALUES (1450, 'Shaolin Sanctuary', 'Site', 2, '', 0x67, 0, 1, 5, 'Shaolin Sanctuary provides one [C] resource for each <Shaolin> Character and each <Monk> Character you control.', 'Shaolin temples across China serve as staging grounds for Quan Lo''s efforts.', '', 'Doug Shuler', 1, 8, 5, 11, 'N', '| shaolin sanctuary |');
INSERT INTO `card` VALUES (1451, 'Shield of Pure Soul', 'Edge', 0, 0x6743, '', 0, 0, 0, 'Limited. When a Site you control is seized or burned, you gain 1 Power and may search your deck and select a card. Reshuffle your deck, then place the selected card on top of your deck.', '', '', 'Mike Kimble', 2, 8, 5, 11, 'N', '| shield pure soul |');
INSERT INTO `card` VALUES (1452, 'Shih Ho Kuai', 'Martial Arts Master', 3, 0x676767, 0x6743, 100, 0, 0, 'Unique. X = the number of [C] resources in your pool.', '', '', 'Edward Beard, Jr.', 5, 8, 5, 11, 'N', '| shih ho kuai martial art arts master |');
INSERT INTO `card` VALUES (1453, 'Sinister Priest', 'Macabre Sorcerer', 1, '', 0x654d, 1, 0, 0, '', 'These loathsome wanderers make the peasantry pay to exorcise spirits they themselves have summoned.', '', 'Phil Foglio', 5, 5, 5, 11, 'N', '| sinister priest macabre sorcerer |');
INSERT INTO `card` VALUES (1454, 'Sports Car', 'State', 1, '', '', 0, 0, 0, 'Vehicle. Subject Character gains Mobility and +2 damage, except against Characters that are the subject of a Vehicle.', '"The paint job make it less obvious that you had to plow through a bunch of mooks."', '', 'Utakata', 3, 12, 5, 11, 'N', '| sport sports car |');
INSERT INTO `card` VALUES (1455, 'Steven Wu', 'Zen Gunman', 5, 0x6464, 0x64, 8, 0, 0, 'Unique. Independent. Cannot have his damage redirected or reduced. Opponents cannot take control of him.', '"The bullet knows the way."', '', 'Cristian Saksida', 5, 4, 5, 11, 'N', '| steven wu zen gun man |');
INSERT INTO `card` VALUES (1456, 'Stone Garden', 'Feng Shui Site', 0, '', '', 0, 1, 6, 'Turn to remove 1 damage from target Site.', 'An artist in harmony with the earth can channel and focus chi.', '', 'Nicola Leonard', 1, 6, 5, 11, 'N', '| stone garden |');
INSERT INTO `card` VALUES (1457, 'Street Riot', 'Event', 0, 0x6a, '', 0, 0, 0, 'Play during an attack you declared on a Site.  Inflict X damage, divided any way you choose, on non-attacking Characters at the location of the attack. X = the number of attackers.', 'Some quick rabble-rousing can often turn a fight around.', '', 'Paul "Prof" Herbert', 4, 10, 5, 11, 'N', '| street riot |');
INSERT INTO `card` VALUES (1458, 'Student of the Bear', 'Pledged Martial Artist', 1, '', 0x61, 1, 0, 0, '', 'Disciples of the Bear School don''t bother with the subtle approach.', '', 'Quinton Hoover', 5, 2, 5, 11, 'N', '| student bear pledged martial artist |');
INSERT INTO `card` VALUES (1459, 'Sub-Machine Gun', 'State', 1, '', '', 0, 0, 0, 'Weapon. Play on a Character. Turn to inflict 1 damage on all Characters at subject''s location, except the subject.', 'Prefered by mooks everywhere.', '', 'Cristian Saksida', 3, 12, 5, 11, 'N', '| sub machine gun  |');
INSERT INTO `card` VALUES (1460, 'Suicide Mission', 'Event', 1, 0x6161, '', 0, 0, 0, 'Target a card :: Smoke that card. That card''s controller may smoke any card you control.', 'The Pledge is a promise to do anything for the Unspoken Name - even die.', '', 'Mike Kimble', 4, 2, 5, 11, 'N', '| suicide mission |');
INSERT INTO `card` VALUES (1461, 'SWAT Team', 'Cop Pawns', 2, '', 0x61, 2, 0, 0, 'Tactics.', 'The Ascended have the world''s police forces at their disposal.', '', 'Cristian Saksida', 5, 2, 5, 11, 'N', '| swat team cop pawn pawns |');
INSERT INTO `card` VALUES (1462, 'Tank Warfare', 'Edge', 1, 0x48, '', 0, 0, 0, 'You may play <Tank> States from your smoked pile at -1 cost. <Tank> Characters and Characters that are the subject of <Tank> States cannot turn to attack Sites if two or more players control such Characters.', '', '', 'Doug Chaffee', 2, 9, 5, 11, 'N', '| tank warfare |');
INSERT INTO `card` VALUES (1463, 'Temple of Celestial Mercy', 'Feng Shui Site', 0, '', '', 0, 1, 7, 'Unique. Your hand size is increased by 2', 'Once a sanctuary; now a prize in the secret war.', '', 'Mark Poole', 1, 6, 5, 11, 'N', '| temple celestial mercy |');
INSERT INTO `card` VALUES (1464, 'Test Subjects', 'Abominations', 1, '', 0x6648, 1, 0, 0, '', '"The plan was to turn demons from the ancient past into supersoldiers. Didn''t work too well at first."', '', 'Quinton Hoover', 5, 1, 5, 11, 'N', '| test subject subjects abomination abominations |');
INSERT INTO `card` VALUES (1465, 'The Iron Monkey', 'Masked Hero', 4, 0x6767, 0x6743, 5, 0, 0, 'Unique. Superleap. When The Iron Monkey inflicts combat damage on the target of his attack, steal half the Power (round down) of his target''s controller and give that Power to any player you choose.', '', '', 'Ron Rousselle', 5, 8, 5, 11, 'N', '| the iron monkey masked hero |');
INSERT INTO `card` VALUES (1466, 'The Legacy', 'Edge', 0, 0x6d, '', 0, 0, 0, 'Unique. At the end of your turn, put a counter on this card if you control any <Thunder> cards. Do likewise for <Fire>, <Darkness>, and <Ice>. Remove 4 counters :: Gain 1 Power.', '', '', 'Rob Thomas', 2, 7, 5, 11, 'N', '| the legacy |');
INSERT INTO `card` VALUES (1467, 'The Reconstructed', 'Standard Abominations', 3, 0x666666, 0x66, 5, 0, 0, '', '"After they put down the India insurgency, they become synonymous with terror."', '', 'Anthony Waters', 5, 1, 5, 11, 'N', '| the reconstructed standard abomination abominations |');
INSERT INTO `card` VALUES (1468, 'Thunder Bishop', 'Netherworld Sorcerer', 2, 0x6d, 0x6d4d, 2, 0, 0, 'Turn and maintain to give target Character Toughness: 1.', 'They inspire the Thunder King''s warriors to ever greater glories.', '', 'Cristian Saksida', 5, 7, 5, 11, 'N', '| thunder bishop netherworld sorcerer |');
INSERT INTO `card` VALUES (1469, 'Thunder Sword', 'State', 2, 0x6d, '', 0, 0, 0, 'Unique. Weapon. Subject Character gains +4 Fighting and cannot turn to attack Sites.', 'When you face this sword, it''s not the thunder that ends up rolling.', '', 'Mike Kimble', 3, 7, 5, 11, 'N', '| thunder sword  |');
INSERT INTO `card` VALUES (1470, 'Ting Ting', 'Martial Artist', 4, 0x6464, 0x646443, 6, 0, 0, 'Unique. Independent. +2 Fighting for each opponent who controls more Power-generating Sites than you. Opponents cannot take control of Ting Ting.', '', '', 'Brian Snoddy', 5, 4, 5, 11, 'N', '| ting martial artist |');
INSERT INTO `card` VALUES (1471, 'Tommy Hsu', 'Hood Sorcerer', 5, 0x656565, 0x654d4d, 8, 0, 0, 'Unique. Sacrifice a <Hood> card to inflict 1 damage on any target. Once per turn, you may inflict 3 damage on him to change the target of an Event.', '', '', 'nilson', 5, 5, 5, 11, 'N', '| tommy hsu hood sorcerer |');
INSERT INTO `card` VALUES (1472, 'Tortured Memories', 'Event', 2, 0x65, '', 0, 0, 0, 'Take control of target Character until the end of the turn.', '"I saw Keung''s head rise before me. Then he told me to blow up the MacMillan Building."', '', 'Susan Alexander', 4, 5, 5, 11, 'N', '| tortured memory memor memories |');
INSERT INTO `card` VALUES (1473, 'Tricia Kwok', 'Undercover Cop', 4, 0x6464, 0x64, 6, 0, 0, 'Unique. Gains Toughness: 1 and +1 Fighting while she is the subject of any Weapon State.', 'With looks like that, you''d think everybody in the city would recognize her. But no one ever does...', '', 'Mike Kimble', 5, 4, 5, 11, 'N', '| tricia kwok undercover cop |');
INSERT INTO `card` VALUES (1474, 'Turtle Beach', 'Feng Shui Site', 0, '', '', 0, 1, 6, 'Unique. Turn to redirect the damage inflicted by a single source on target Character to Turtle Beach.', 'When protection becomes sacrifice, how much can the land withstand?', '', 'Jeff Menges', 1, 6, 5, 11, 'N', '| turtle beach |');
INSERT INTO `card` VALUES (1475, 'Turtle Island', 'Feng Shui Site', 0, '', '', 0, 1, 8, 'Cannot be damaged or removed from play by opponents'' Events. Cannot be burned.', '', '', 'Rob Alexander', 1, 6, 5, 11, 'N', '| turtle island |');
