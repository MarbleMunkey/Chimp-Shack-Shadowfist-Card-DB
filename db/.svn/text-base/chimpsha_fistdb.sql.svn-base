INSERT INTO `card` VALUES (1550, 'Morphic Spirit', 'Sorcerous Construct', 2, '', 0x70, 2, 0, 0, 'Toughness: 1 while you have at least two different faction resources in your pool. +1 Fighting if you have three, or +2 Fighting if you have four or more.', '', '', 'Jeremy Dale', 5, 13, 2, 12, 'N', '| morphic spirit sorcerou sorcerous construct |');
