INSERT INTO `card` VALUES (2374, 'Military Commandant', 'Pledged Mastermind', 2, 0x6161, 0x61, 1, 0, 0, 'Turn and maintain and target a Character :: that Character gains +2 Fighting.', '"Terminate, with extreme prejudice."', '', 'Mike Kimble', 5, 2, 6, 15, 'N', '| military commandant pledged mastermind |');
