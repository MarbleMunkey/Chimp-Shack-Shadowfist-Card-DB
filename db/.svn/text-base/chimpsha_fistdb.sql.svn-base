INSERT INTO `card` VALUES (1872, 'Year of the Monkey', 'Event', 2, 0x6a6a6a, '', 0, 0, 0, 'One-Shot. Target opponent sets aside his or her hand. Look through target''s deck for an equal number of cards, put them in target''s hand, and reshuffle. At the start of that player''s turn, target must discard either his or her hand or the set-aside hand; the other becomes his or her hand.', '', '', 'Mike Jackson', 4, 10, 1, 14, 'N', '| year monkey |');
INSERT INTO `card` VALUES (1873, '"You Fell Into My Trap!"', 'Event', 0, 0x65, '', 0, 0, 0, 'Play when an opponent declares an attack against a Site you control and you control no Characters :: Play a Character with a cost of 3 or less at no cost.', '"Bwahahahaha!"', '', 'Allan Bednar', 4, 5, 2, 14, 'N', '| you fell into my trap |');
INSERT INTO `card` VALUES (1874, 'Yuen Sheng', 'Red Scorpion Mastermind', 2, 0x6565, 0x65, 2, 0, 0, 'Unique. Stealth. When this card enters play, look at every opponent''s hand, and choose a card from each one. Set these cards aside. They are still part of those players'' hands, but cannot be played or discarded while he is in play.', '', '', 'Gorka Uztarroz', 5, 5, 1, 14, 'N', '| yuen sheng red scorpion mastermind |');
INSERT INTO `card` VALUES (1875, 'Zeppelin', 'State', 1, 0x6666, '', 0, 0, 0, 'Vehicle. Subject Character gains Mobility. Smoke subject when this card leaves play. Turn to inflict 3 damage on target Site at this location, and 1 damage on all Characters other than subject at this location.', '', '', 'Roberto Marchesi', 3, 1, 2, 14, 'N', '| zeppelin |');
INSERT INTO `card` VALUES (2431, 'Yosef Halevi', 'Kabalist Sorcerer', 3, 0x646464, 0x644d, 4, 0, 0, 'Unique. Independent. Twice a turn, you may pay 1 Power to give this card +3 Fighting until the end of the turn.', '"It is every man''s duty to fight evil."', '', 'Mike Jackson', 5, 4, 1, 15, 'N', '| yosef halevi kabalist sorcerer |');
INSERT INTO `card` VALUES (2429, 'The Willow Bends...', 'Event', 0, 0x67, 0x67, 0, 0, 0, 'Play when a card is damaged, but not removed from play :: Heal that card.', '...but the femur usually breaks.', '', 'Scott Reeves', 4, 8, 2, 15, 'N', '| the willow bend bends |');
INSERT INTO `card` VALUES (2430, 'Yippee-Yi-Yo-Kiii-YAAAH!', 'Event', 0, '', '', 0, 0, 0, 'One-Shot. You must have at least two resources each from two different factions in your pool to play this card. Gain X Power, where X= the number of columns in your Site structure.', '', '', 'Chris Quilliams', 4, 12, 3, 15, 'N', '| yippee yi yo kiii yaaah |');
INSERT INTO `card` VALUES (2427, 'Whispers in the Dark', 'State', 0, 0x70, '', 0, 0, 0, 'Subject Character gains -4 damage while at the same location as a Character you control.', 'The Purists have many tricks, but fear itself is often enough.', '', 'Matthew Laznicka', 3, 13, 2, 15, 'N', '| whisper whispers dark |');
INSERT INTO `card` VALUES (2428, 'Wild Spirits', 'Free-Range Constructs', 2, 0x70, 0x704d, 2, 0, 0, 'Turn to exchange the locations of two Sites controlled by one player. This may change the target of an attack.', 'Contradictions clothed in flesh.', '', 'Mike Jackson', 5, 13, 2, 15, 'N', '| wild spirit spirits free range construct constructs |');
INSERT INTO `card` VALUES (2426, 'Wall Running', 'State', 1, 0x7943, '', 0, 0, 0, 'Subject Character cannot be intercepted by Characters whose cost is less than or equal to subject''s.', 'Novices practice in zero-g before learning that gravity is a matter of chi alignment.', '', 'Dan Murray', 3, 15, 3, 15, 'N', '| wall running |');
INSERT INTO `card` VALUES (2425, 'University Library', 'Feng Shui Site', 0, '', '', 0, 1, 7, 'Turn :: Put a card from your hand face-up on this card. You may play it as if it were in your hand, but cannot discard it, and it is not in play. Toast the face-up card when you lose control of University Library or when you use this ability again.', '', '', 'Mike Jackson', 1, 6, 2, 15, 'N', '| university library |');
INSERT INTO `card` VALUES (2424, 'Two-Headed Horror', 'Demon State', 2, 0x6565, '', 0, 0, 0, 'Toast It. Play on a Character and choose another Character with lower Fighting :: Smoke the chosen Character. Subject gains the chosen Character''s rules text.', 'Each was a force on his own; in this horrific union, they are unstoppable.', '', 'Patrick McEvoy', 3, 5, 3, 15, 'N', '| two headed horror demon |');
INSERT INTO `card` VALUES (2423, 'Triad Punks', 'Wired Hoods', 1, '', 0x7948, 1, 0, 0, '', 'No matter how subtle you are, you can''t run the world without some muscle on the street.', '', 'Flavio Augusto', 5, 15, 2, 15, 'N', '| triad punk punks wired hood hoods |');
INSERT INTO `card` VALUES (2422, 'Thunder Stick', 'Gun State', 1, 0x6d6d, '', 0, 0, 0, 'Weapon. Subject Character inflicts +3 damage while this card is unturned. Turn and maintain to give +1 damage to all your Characters while they are at this location.', 'By providing modern weapons to his allies, Huan Ken hoped to seize many Feng Shui sites in the West.', '', 'Zak Hennessey', 3, 7, 2, 15, 'N', '| thunder stick gun  |');
INSERT INTO `card` VALUES (2421, 'Thunder in the West', 'Event', 1, 0x6d6d6d, '', 0, 0, 0, 'Each player must choose one non-Site card he or she controls :: Toast the chosen cards.', 'Troubled times mean hard choices.', '', 'Dan Harding', 4, 7, 3, 15, 'N', '| thunder west |');
INSERT INTO `card` VALUES (2420, 'Thunder Bird', 'Mighty Spirit', 4, 0x6d6d6d, 0x6d, 4, 0, 0, 'Unique. +X Fighting. X= the number of Feng Shui Sites in play.', 'The most powerful creature of the sky.', '', 'Ed Cox', 5, 7, 1, 15, 'N', '| thunder bird mighty spirit |');
INSERT INTO `card` VALUES (2419, 'Texas Jack Cody', 'Lodge Ranger', 5, 0x616161, 0x61, 8, 0, 0, 'Unique. Pay 1 Power prior to combat with target Character intercepting him :: Smoke target.', 'He leaves a trail of bodies behind, but he always gets his man.', '', 'Matthew Laznicka', 5, 2, 1, 15, 'N', '| texa texas jack cody lodge ranger |');
INSERT INTO `card` VALUES (2418, 'Swift Eagle', 'Brave Warrior', 4, 0x646464, 0x6443, 7, 0, 0, 'Unique. Independent. Discard a Feng Shui Site to cancel an Event which targets this card.', '"This ''Thunder King'' is no more our friend than the white men were."', '', 'Stacy Drum', 5, 4, 1, 15, 'N', '| swift eagle brave warrior |');
INSERT INTO `card` VALUES (2417, 'Street Sweepers', 'Enhanced Martial Artists', 2, '', 0x794843, 2, 0, 0, '', 'The rogue abominations demanded an organized response.', '', 'Oliver Specht', 5, 15, 2, 15, 'N', '| street sweeper sweepers enhanced martial artist artists |');
INSERT INTO `card` VALUES (2416, 'Strange Ore', 'Event', 0, 0x70, '', 0, 0, 0, 'Discard X cards and target X unrevealed Sites :: Reveal those Sites. If any of them are Unique, gain 1 Power.', 'Perennial tears descend in gems.', '', 'Banu Adhumika', 4, 13, 3, 15, 'N', '| strange ore |');
INSERT INTO `card` VALUES (2415, 'Steam-Powered Tricycle', 'Weird Science State', 1, 0x6a6a48, '', 0, 0, 0, 'Vehicle. Subject Character inflicts +3 damage. Characters with exactly two designators in their title cannot intercept subject.', 'High-performance machine. Low-performance driver.', '', 'Matthew Laznicka', 3, 10, 2, 15, 'N', '| steam powered tricycle weird science |');
INSERT INTO `card` VALUES (2412, 'Skin and Darkness Bats', 'Netherworld Horrors', 1, '', 0x6d, 1, 0, 0, 'Sacrifice to give target Character -2 damage until the end of the turn.', 'One dies willingly that the rest might feed.', '', 'Jeremy Dale', 5, 7, 2, 15, 'N', '| skin darkness darknes darkne darkn bat bats netherworld horror horrors |');
