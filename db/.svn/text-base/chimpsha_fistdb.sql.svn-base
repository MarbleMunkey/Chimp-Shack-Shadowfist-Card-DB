INSERT INTO `card` VALUES (3271, 'Scramble Suit', 'State of the Art Tech', 1, 0x4848, '', 0, 0, 0, 'Subject Character cannot be intercepted by Characters controlled by players with cards in their hands. Any player may discard his or her hand when subject turns to attack.', '', '', 'Brian England', 3, 9, 3, 17, 'N', '| scramble suit art tech |');
INSERT INTO `card` VALUES (3270, 'Safety Third!', 'Event', 0, 0x6a, '', 0, 0, 0, 'Target a Character and flip a coin :: Heads: smoke target. Tails: inflict 1 damage on all Characters you control. [C] cards are not affected by this card.', 'Most Jammers are surprised it''s that high on the list.', '', 'Dan Harding', 4, 10, 2, 17, 'N', '| safety third |');
INSERT INTO `card` VALUES (3269, 'Sacred Heart Hospital', 'Feng Shui Site', 0, '', '', 0, 1, 6, 'Unique. Turn :: Heal target Character.', '"Gunshot wounds, Trauma 1. Demon bites, Trauma 2. Weird recurring glow, please wait outside."', '', 'Martin Montalbetti', 1, 6, 6, 17, 'N', '| sacred heart hospital |');
