INSERT INTO `card` VALUES (3302, 'Zheng Yi Quan', 'Master of the World', 7, 0x6464646464, 0x646443, 10, 0, 0, 'Unique. Guts. Your opponents cannot generate effects from Feng Shui Sites, but you do so as if you controlled those Sites. (This does not allow you to reveal unrevealed Sites.)', '', '', 'Patrick McEvoy', 5, 4, 1, 17, 'N', '| zheng yi quan master world |');
INSERT INTO `card` VALUES (3301, 'Zero-G Sumo', 'Enhanced Colonist', 3, 0x7948, 0x7948, 4, 0, 0, 'Toughness: X. X= the number of Unique and Limited Sites controlled by opponents.', 'Mass times acceleration is still force.', '', 'Mike Jackson', 5, 15, 3, 17, 'N', '| zero g sumo enhanced colonist |');
INSERT INTO `card` VALUES (3300, 'Xu Mei, The Dragon', 'Cybernetic Martial Arts Master', 7, 0x7979797948, 0x794843, 8, 0, 0, 'Unique. Ambush. Other Characters you control cannot be intercepted.', '"The understanding that comes from designing your own cyberware will allow your chi to flow properly."', '', 'Dan Murray', 5, 15, 1, 17, 'N', '| xu mei the dragon cybernetic martial art arts master |');
INSERT INTO `card` VALUES (3298, 'Willow Step', 'Event', 0, 0x4343, '', 0, 0, 0, 'Reload: 1. Target a Character that is about to enter combat :: Target gains Toughness: 2 until the end of combat.', '"Be where your enemy isn''t."', '', 'Kevin Wasden', 4, 3, 3, 17, 'N', '| willow step |');
