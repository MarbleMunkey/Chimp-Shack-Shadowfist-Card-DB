INSERT INTO `card` VALUES (3263, 'Reascension Spy', 'Undercover Salaryman', 3, 0x6161, 0x61, 1, 0, 0, 'Turn and maintain and choose a damaged Character :: Take control of that Character.', '"I think we can offer you a very competitive benefits package."', '', 'Martin Montalbetti', 5, 2, 3, 17, 'N', '| reascension spy undercover salaryman |');
INSERT INTO `card` VALUES (3264, 'Rebel Without a Cause', 'Disaffected Saboteur', 2, '', 0x6a48, 2, 0, 0, 'If you have [j] in your pool, you may discard this card when an opponent''s effect causes one or more cards to be returned to play or played from a smoked pile :: Toast one of those cards.', '', '', 'Regis Moulun', 5, 10, 2, 17, 'N', '| rebel without cause disaffected saboteur |');
