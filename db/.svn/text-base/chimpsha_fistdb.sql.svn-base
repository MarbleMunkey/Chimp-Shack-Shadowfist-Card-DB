INSERT INTO `card` VALUES (1483, 'Yellow Geomancer', 'Crafty Monk', 1, 0x67, 0x6743, 1, 0, 0, 'Turn to redirect one damage inflicted on a Site to any other target.', 'Taking them on sometimes does more harm than good.', '', 'Jeremy Dale', 5, 8, 5, 11, 'N', '| yellow geomancer crafty monk |');
