INSERT INTO `card` VALUES (978, 'Gong Wei', 'Reformed Bandit', 4, 0x6767, 0x67, 6, 0, 0, 'Unique. Once during your Main Shot, you may sacrifice a <Rebel>, <Rabble>, or <Peasant> Character to return a [g] Event from your smoked pile to your hand.', '', '', 'Chris Chuckry', 5, 8, 1, 7, 'N', '| gong wei reformed bandit |');
INSERT INTO `card` VALUES (977, 'Geoscan Report', 'Event', 1, 0x6666, '', 0, 0, 0, 'Limited. Target a Site controlled by an opponent. Reveal that Site. Characters you control inflict +2 damage on that Site until the end of the turn.', '"The survey teams started complaining after the Temple incident."', '', 'Andrew Barlow', 4, 1, 2, 7, 'N', '| geoscan report |');
INSERT INTO `card` VALUES (976, 'General Senggelinqin', 'Manchu Military Mastermind', 4, 0x616161, 0x61, 7, 0, 0, 'Unique. Tactics. Once during your Main Shot, you may sacrifice a <Soldier> Character to play a <Commandant>, <Colonel>, or <Officer> Character from your hand or smoked pile at -X cost. X= the cost of the sacrificed Character.', '', '', 'Mark Evans', 5, 2, 1, 7, 'N', '| general senggelinqin manchu military mastermind |');
INSERT INTO `card` VALUES (975, 'Four Sorrows Island', 'Feng Shui Site', 0, '', '', 0, 1, 6, 'Unique. Cannot be healed except when seized. Except during an attack on this card, turn and inflict 2 damage on it and target a Character :: Target may not intercept until the end of the turn.', '', '', 'Jason Millet', 1, 6, 1, 7, 'N', '| four sorrow sorrows island |');
INSERT INTO `card` VALUES (974, 'Fortune of the Turtle', 'State', 1, 0x43, '', 0, 0, 0, 'Subject Character is not affected by Events played by an opponent.', 'The turtle symbolizes protection, caution, and forethought.', '', 'Jeff Menges', 3, 3, 3, 7, 'N', '| fortune turtle |');
INSERT INTO `card` VALUES (973, 'Forgotten Shrine', 'Feng Shui Site', 0, '', '', 0, 1, 6, 'Turn and maintain and target an opponent''s Site :: That Site takes two damage when it is turned.', '"...Lost to all but the lone monk whose duty was to tend it."', '', 'Melissa Benson', 1, 6, 2, 7, 'N', '| forgotten shrine |');
INSERT INTO `card` VALUES (971, 'Flying Monkey Squad', 'Guerrilla Gorillas', 3, 0x6a6a6a48, 0x6a48, 3, 0, 0, 'Independent. Superleap.', 'You''re not in Kansas anymore.', '', 'Anthony Grabski', 5, 10, 2, 7, 'N', '| flying monkey squad guerrilla gorilla gorillas |');
INSERT INTO `card` VALUES (972, 'Fong Sai Yuk vPAP', 'Martial Artist', 6, 0x67676767, 0x6743, 9, 0, 0, 'Unique. Superleap. The rules text of the back-row Site at the location of the target of Fong Sai Yuk''s attack is canceled until the end of the turn.', '', '', 'Brian Rood', 5, 8, 1, 7, 'N', '| fong sai yuk martial artist |');
INSERT INTO `card` VALUES (970, 'Flying Kick', 'Event', 1, 0x64, '', 0, 0, 0, 'Target Character gains Superleap until the end of the turn.', '"Yeah, yeah, we''ll give it back to the Hand when we''re done with it."', '', 'Margaret Organ-Kean', 4, 4, 2, 7, 'N', '| flying kick |');
INSERT INTO `card` VALUES (969, 'Fist of the Bear vPAP', 'Lodge Enforcer', 4, 0x6161, 0x61, 6, 0, 0, 'Toughness: 1.', 'Thalnasser chose brute force as the best way to deal with the Hand.', '', 'Paul "Prof" Herbert', 5, 2, 3, 7, 'N', '| fist bear lodge enforcer |');
INSERT INTO `card` VALUES (968, 'Fire Infiltrator', 'Netherworld Subversive', 2, 0x6d, 0x6d, 2, 0, 0, '+1 Fighting if you have [C] resources in your pool.', 'Li Ting saw the Muslim rebellion as an opportunity to establish his twisted version in China.', '', 'Jim Pavelec', 5, 7, 2, 7, 'N', '| fire infiltrator netherworld subversive |');
INSERT INTO `card` VALUES (967, 'Feng Kan', 'Wind Demon', 4, 0x65654d, 0x654d, 5, 0, 0, 'You may play Feng Kan at -1 cost for each opponent who controls more Characters than you do.', '"Who can see the wind before it arrives?"', '', 'Paul "Prof" Herbert', 5, 5, 3, 7, 'N', '| kan wind demon |');
INSERT INTO `card` VALUES (965, 'Elephant Gun', 'State', 2, '', '', 0, 0, 0, 'Weapon. Subject Character inflicts +5 damage.', '"Even Katie was impressed."', '', 'Chris Chuckry', 3, 12, 6, 7, 'N', '| elephant gun  |');
INSERT INTO `card` VALUES (966, 'Faked Death', 'Event', 0, 0x61, '', 0, 0, 0, 'Return a Character from your smoked pile to your hand.', 'The Lodge can write you a death certificate any time they want.', '', 'L. A. Williams', 4, 2, 2, 7, 'N', '| faked death |');
INSERT INTO `card` VALUES (964, 'Eagle vs. Snake', 'Event', 1, 0x6767, '', 0, 0, 0, 'Faceoff. Turn a Character you control and target an opponent''s Character with Fighting no lower than two less than yours. The winner may draw three cards.', 'Rachel was fast. Su Yin was faster.', '', 'Val Mayerik', 4, 8, 3, 7, 'N', '| eagle v vs snake |');
INSERT INTO `card` VALUES (963, 'Dragon Mountain', 'Feng Shui Site', 0, '', '', 0, 1, 7, 'Unique. Toughness: 1.', 'The dragon is hidden in the land. Its scales say, "Do not act."', '', 'Bryon Wackwitz', 1, 6, 1, 7, 'N', '| dragon mountain |');
INSERT INTO `card` VALUES (961, 'Dragon Boat Festival', 'Edge', 1, 0x676767, '', 0, 0, 0, 'Limited. Comes into play turned. While this card is unturned, each player''s hand size is increased by 2.', 'A tribute to the drowned scholar Chu Yuan.', '', 'Dennis Calero', 2, 8, 6, 7, 'N', '| dragon boat festival |');
INSERT INTO `card` VALUES (962, 'Dragon Dojo', 'Site', 2, '', 0x64, 0, 1, 5, 'You may draw a card when you play a <Student>, <Master>, or <Teacher> Character at this location. This effect is not cumulative.', '"Here, we teach discipline, philosophy, and how to kick butt."', '', 'Brian Rood', 1, 4, 2, 7, 'N', '| dragon dojo |');
INSERT INTO `card` VALUES (960, 'Doctor Shen', 'Martial Artist', 5, 0x6464, 0x6443, 7, 0, 0, 'Unique. Toughness: 1. At the end of combat, you may heal one damage from any other Character for each combat damage he inflicted on a Character.', '"First, do no harm" is not his motto.', '', 'David Fooden', 5, 4, 1, 7, 'N', '| doctor shen martial artist |');
INSERT INTO `card` VALUES (959, 'Dark''s Soft Whisper', 'Event', 0, 0x43, '', 0, 0, 0, 'Play when an attack you declared fails and an opponent is one Feng Shui Site away from victory or closer to victory than you. Gain 2 Power.', 'Defeat becomes victory. The wheel turns.', '', 'Heather Hudson', 4, 3, 2, 7, 'N', '| dark darks soft whisper |');
INSERT INTO `card` VALUES (958, 'Contingency Plans', 'Event', 0, 0x616161, '', 0, 0, 0, 'Limited. Play when a Feng Shui Site you control is seized or burned :: Play a Feng Shui Site face-up at no cost.', '"To be prepared for any contingency is the greatest of virtues." - Sun Tzu', '', 'Andrew Baker', 4, 2, 3, 7, 'N', '| contingency plan plans |');
INSERT INTO `card` VALUES (957, 'Commander Corliss', 'TacOps Officer', 2, 0x666666, 0x66, 6, 0, 0, 'Unique. Can only be played at any time during an opponent''s Main Shot. You do not generate any Power during your next Establishing Shot after you play this card.', '', '', 'Mark Evans', 5, 1, 1, 7, 'N', '| commander corlis corliss tac op ops officer |');
INSERT INTO `card` VALUES (956, 'Coil of the Snake', 'Lodge Assassin', 3, 0x6161, 0x61, 4, 0, 0, 'Stealth. When you turn Coil of the Snake to attack, you may discard and draw a card.', 'Quick, clean, lethal.', '', 'Brian Rood', 5, 2, 2, 7, 'N', '| coil snake lodge assassin |');
INSERT INTO `card` VALUES (955, 'Chinese Doctor', 'Wise Physician', 1, 0x64, 0x64, 1, 0, 0, 'Turn to heal target Character.', '"Lucky they only shot you in the head. Next time they might hit something important."', '', 'Andi Rusu', 5, 4, 2, 7, 'N', '| chinese doctor wise physician |');
INSERT INTO `card` VALUES (954, 'Chen Sho Kung', 'Master Swordsman', 5, 0x67676767, 0x67, 6, 0, 0, 'Unique. Once per turn, you may sacrifice a <Sword> State on Chen Sho Kung to reduce all damage dealt to him by a single source to 0. All <Swordsman> Characters you control gain +1 Fighting.', '', '', 'Paul "Prof" Herbert', 5, 8, 1, 7, 'N', '| chen sho kung master sword swords man |');
INSERT INTO `card` VALUES (953, 'Charmed Life', 'State', 1, 0x64, '', 0, 0, 0, 'Subject Character is not affected by effects that would smoke it. Subject can still be smoked due to damage.', 'Dragons have to be lucky _and_ good to last long.', '', 'Kaja Foglio', 3, 4, 3, 7, 'N', '| charmed life |');
INSERT INTO `card` VALUES (952, 'Charge of the Rhino', 'Lodge Enforcer', 4, 0x6161, 0x61, 6, 0, 0, 'When Charge of the Rhino smokes a Character by inflicting combat damage, you may inflict 1 damage on any card at its location.', 'They bring new meaning to the word "unsubtle".', '', 'Paul "Prof" Herbert', 5, 2, 3, 7, 'N', '| charge rhino lodge enforcer |');
INSERT INTO `card` VALUES (950, 'Butterfly Swords', 'State', 1, 0x43, '', 0, 0, 0, 'Weapon. Subject Character inflicts +3 damage.', 'Deadly in trained hands.', '', 'Steve Bryant', 3, 3, 2, 7, 'N', '| butterfly sword swords  |');
INSERT INTO `card` VALUES (951, 'Carnival of Carnage', 'Event', 1, 0x64, '', 0, 0, 0, 'Until the end of the turn, when any Character is smoked, you may inflict 1 damage on any Character in play. Each damage inflicted by Carnival of Carnage is considered a separate source of damage.', 'There''s always one more guy to kill.', '', 'Drew Tucker', 4, 4, 2, 7, 'N', '| carnival carnage |');
INSERT INTO `card` VALUES (949, 'Big Mack', 'Bar-Room Brawler', 4, 0x646464, 0x64, 6, 0, 0, 'Unique. Toughness: 1. Inflicts +3 damage on Sites.', '"Put it on my tab."', '', 'Mark Poole', 5, 4, 1, 7, 'N', '| big mack bar room brawler |');
INSERT INTO `card` VALUES (948, 'Bear vs. Fox', 'Event', 1, 0x6767, '', 0, 0, 0, 'Faceoff. Turn a Character you control and target an opponent''s Character with Fighting no lower than two less than yours. The winner may play one Event this turn, at no cost and ignoring resource conditions, from the loser''s smoked pile.', '', '', 'Kenneth Waters', 4, 8, 3, 7, 'N', '| bear v vs fox |');
INSERT INTO `card` VALUES (947, 'Ancient Monument', 'Feng Shui Site', 0, '', '', 0, 1, 12, 'Inflict 1 damage on this Site when an attack is declared against it.', 'Its placid beauty is camouflage for the danger that lies within.', '', 'Andrew Baker', 1, 6, 2, 7, 'N', '| ancient monument |');
INSERT INTO `card` VALUES (946, 'Ancient Grove', 'Site', 2, '', 0x4343, 0, 1, 5, '', 'These bamboo shoots are the mirror of your soul, warrior. When you understand why, you will find the path to mastery.', '', 'Nene Thomas', 1, 3, 2, 7, 'N', '| ancient grove |');
INSERT INTO `card` VALUES (945, '18 Bronze Men', 'State', 2, 0x6767, '', 0, 0, 0, 'Limited. Play on a front-row Site. All non-Unique <Shaolin>, <Martial>, and <Kung> <Fu> Characters you control gain +1 Fighting.', '', '', 'Aaron Boyd', 3, 8, 3, 7, 'N', '| 18 bronze man men |');
INSERT INTO `card` VALUES (1078, 'Tiger Hook Swords', 'State', 1, '', '', 0, 0, 0, 'Weapon. Subject Character inflicts +2 damage. If subject is a <Swordsman>, draw a card when subject smokes a Character by inflicting combat damage.', '', '', 'Steve Bryant', 3, 12, 2, 7, 'N', '| tiger hook sword swords  |');
INSERT INTO `card` VALUES (1079, 'Tiger vs. Crane', 'Event', 1, 0x6161, '', 0, 0, 0, 'Faceoff. Turn a Character you control and target an opponent''s Character with Fighting no lower than two less than yours. The winner may inflict 1 damage on up to 3 different cards controlled by the loser.', 'Tsung Jin was the first target.', '', 'Ravindra Rana', 4, 2, 3, 7, 'N', '| tiger v vs crane |');
