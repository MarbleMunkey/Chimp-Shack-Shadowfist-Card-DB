INSERT INTO `card` VALUES (1487, 'Armies of the Monarchs', 'Edge', 2, 0x6d6d, '', 0, 0, 0, 'All Unique Characters inflict -1 damage. All your non-Unique Characters inflict +1 damage.', 'No matter how good your kung fu, if all your enemies come at you at once, youÕre done for!', '', 'Andrew Barlow', 2, 7, 3, 12, 'N', '| army arm armies monarch monarchs |');
