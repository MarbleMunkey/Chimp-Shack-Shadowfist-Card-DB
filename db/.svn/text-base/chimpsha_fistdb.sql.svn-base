INSERT INTO `card` VALUES (1533, 'Ho Chen', 'Master of the Flawless Strike', 5, 0x7373, 0x73434d, 7, 0, 0, 'Unique.', 'This winter, Z-Man Games brings you another exciting Shadowfist expansion as hopping vampires, over-the-top kung fu, and a new faction appear in Seven Masters versus the Underworld.', '', 'Brian Rood', 5, 14, 6, 12, 'N', '| ho chen master flawles flawless strike |');
