INSERT INTO `card` VALUES (1085, 'Two Dragons Inn', 'Site', 2, '', '', 0, 1, 7, 'Unique. At the end of each player''s turn, that player loses one Power if he or she controls any Characters and did not declare an attack.', 'Mr. X tracked Iala Mane to the Inn, and then set up his ambush.', '', 'Tony Moseley', 1, 12, 1, 7, 'N', '| two dragon dragons inn |');
INSERT INTO `card` VALUES (1086, 'Underworld Contract', 'Edge', 0, 0x656565, '', 0, 0, 0, 'Limited. Place a counter on Underworld Contract when an attack declared against you fails. Turn and remove X counters to play a <Demon> card at -X cost.', '', '', 'Lissanne Lake', 2, 5, 3, 7, 'N', '| underworld contract |');
INSERT INTO `card` VALUES (1087, 'Walk of a Thousand Steps', 'State', 2, 0x4343, '', 0, 0, 0, 'Not cumulative. Subject Character takes -X damage from any source. X= half the damage inflicted, rounded up.', 'Those who keep the path are kept from harm.', '', 'Melissa Benson', 3, 3, 3, 7, 'N', '| walk thousand step steps |');
INSERT INTO `card` VALUES (1088, 'Web of the Spider vPAP', 'Lodge Mastermind', 2, 0x61, 0x61, 2, 0, 0, 'Gain 1 Power when an opponent plays a card at reduced cost.', 'Accountants are as important as assassins in the Secret War.', '', 'Heather Bruton', 5, 2, 3, 7, 'N', '| web spider lodge mastermind |');
INSERT INTO `card` VALUES (1089, 'Wei Fong-Yi', 'Master of the Fox Style', 4, 0x676767, 0x6743, 7, 0, 0, 'Unique. When Wei Fong-Yi enters combat with a Character, you may choose one State on that Character, and either smoke it or take control of it and place it on Wei Fong-Yi.', '', '', 'Kenneth Waters', 5, 8, 1, 7, 'N', '| wei fong yi master fox style |');
INSERT INTO `card` VALUES (1090, 'Year of the Snake', 'Event', 3, 0x616161, '', 0, 0, 0, 'One-Shot. Play when you are attacking a Site :: Change the target of all your attackers to any one legal card.', 'After the playful Dragon comes the wily Serpent.', '', 'Paul "Prof" Herbert', 4, 2, 3, 7, 'N', '| year snake |');
INSERT INTO `card` VALUES (1091, 'Yen Fan', 'Pledged Traitor', 4, 0x676767, 0x6761, 6, 0, 0, 'Unique. Superleap. Not affected by Stealth.', '"We cannot sell our freedom and our culture for prosperity. The price is too high, and so I must renounce my Pledge."', '', 'Melissa Benson', 5, 8, 1, 7, 'N', '| yen fan pledged traitor |');
INSERT INTO `card` VALUES (1092, 'Yen Song', 'Shaolin Traitor', 4, 0x616161, 0x6167, 6, 0, 0, 'Unique. Stealth. Not affected by Superleap.', '"Confucian principles have not brought prosperity to our people. The Wheel can."', '', 'Melissa Benson', 5, 2, 1, 7, 'N', '| yen song shaolin traitor |');
INSERT INTO `card` VALUES (1093, 'Zheng Yi Quan vPAP', 'Kung Fu Master', 6, 0x64646464, 0x6443, 10, 0, 0, 'Unique. Guts. All <Student> Characters you control gain +1 fighting.', '"I try to pass on what Kar Fai taught me. Not just the fighting, but also why we fight."', '', 'Melissa Benson', 5, 4, 1, 7, 'N', '| zheng yi quan kung fu master |');
INSERT INTO `card` VALUES (1094, 'Alchemist''s Lair', 'Site', 2, '', 0x4d4d, 0, 1, 5, '', '"You were right, old man. The power of immortality is what I sought--and the misery of duality is what I found."', '', 'Margaret Organ-Kean', 1, 11, 2, 8, 'N', '| alchemist alchemists lair |');
INSERT INTO `card` VALUES (1095, 'Arachnae', 'Reascended Spider', 4, 0x6161614848, 0x61, 7, 0, 0, 'Unique. Assassinate. Regeneration.', '"We will become more than human, and the force of our Reascension will remake the world." - Dunwa Saleem.', '', 'Mark Poole', 5, 2, 1, 8, 'N', '| arachnae reascended spider |');
INSERT INTO `card` VALUES (1096, 'Arcane Scientist', 'Initiate of Paradox', 2, '', 0x704d, 2, 0, 0, 'Turn to draw a card during your Main Shot.', 'Unbound by the traditions of the Lotus, the Purists took a radical approach to magic.', '', 'Arthur Roberg', 5, 13, 2, 8, 'N', '| arcan e scientist initiate paradox |');
INSERT INTO `card` VALUES (1097, 'Assault Drone', 'Arcanowave Robot', 5, 0x66666648, 0x6648, 7, 0, 0, 'Toughness: 1. Cannot intercept. Assault Drone enters play with four counters on it. Add one counter at the start of your turn. Remove a counter to inflict one damage on any target at this location.', '', '', 'Mark Evans', 5, 1, 3, 8, 'N', '| assault drone arcano wave robot |');
INSERT INTO `card` VALUES (1098, 'Battlechimp Potemkin', 'Subversive Chimp Mastermind', 4, 0x6a6a48, 0x6a6a48, 6, 0, 0, 'Unique. Once during each opponent''s Main Shot, you may play a [j] or [H] Event from any player''s smoked pile, ignoring resource conditions, and toasting the Event immediately.', '', '', 'Jim Pavelec', 5, 10, 1, 8, 'N', '| battlechimp potemkin subversive chimp mastermind |');
INSERT INTO `card` VALUES (1099, 'Battle Cry', 'Event', 1, 0x6d, '', 0, 0, 0, 'Inflict X damage on target Character. X= 2 plus the number of <Battleground> Sites in play.', 'Huan Ken rarely gets involved unless there''s a good fight to be had.', '', 'Edward Beard, Jr.', 4, 7, 2, 8, 'N', '| battle cry |');
INSERT INTO `card` VALUES (1100, 'Battle-Matic', 'State', 1, 0x6a6a48, '', 0, 0, 0, 'Vehicle. Subject Character gains Toughness: 1 and +X Fighting. X= the number of Vehicles, Weapons, and [H] States in your smoked pile.', '"I never saw a patchwork tank before."', '', 'Mark Poole', 3, 10, 3, 8, 'N', '| battle matic |');
INSERT INTO `card` VALUES (1101, 'Beta Beast', 'Early Model Abomination', 2, '', 0x6648, 2, 0, 0, 'Toughness: 1 against Characters who provide only one resource.', 'The second generation prototypes proved more vicious but just as expendable.', '', 'Mark Tedin', 5, 1, 2, 8, 'N', '| beta beast early model abomination |');
INSERT INTO `card` VALUES (1102, 'Big Macaque Attack', 'Monkey Horde', 2, '', 0x6a, 1, 0, 0, '+1 Fighting for each <Monkey> Character in play. Not a legal subject for States.', 'As it turns out, a barrel full of monkeys isn''t actually that much fun.', '', 'Lissanne Lake', 5, 10, 2, 8, 'N', '| big macaque attack monkey horde |');
INSERT INTO `card` VALUES (1103, 'BK97 Attack Chopper', 'State', 2, 0x666648, '', 0, 0, 0, 'Vehicle. Subject Character gains +3 Fighting. Damage subject inflicts cannot be reduced.', '"Magic don''t mean crap against a minigun."', '', 'Mark Evans', 3, 1, 2, 8, 'N', '| bk97 attack chopper |');
INSERT INTO `card` VALUES (1104, 'Blue Mandarin', 'Deceptive Bureaucrat', 1, 0x6767, 0x67, 1, 0, 0, 'Turn and maintain and target a Character. That Character cannot intercept or turn to generate an effect.', 'In the future, the Hand have to be especially subtle.', '', 'Steve Bryant', 5, 8, 2, 8, 'N', '| blue mandarin deceptive bureaucrat |');
INSERT INTO `card` VALUES (1105, 'Borrowed Nuke', 'State', 2, 0x64646a48, '', 0, 0, 0, 'One Shot. Play on an opponent''s Site, except during an attack. Subject cannot be seized or burned. If an attack on the subject succeeds, smoke all cards at this location. (Including any attackers.)', '"No, really, we''ll give it back."', '', 'Randy Asplund', 3, 4, 1, 8, 'N', '| borrowed nuke |');
INSERT INTO `card` VALUES (1106, 'Buro Blue Spear', 'Gun State', 1, 0x66, '', 0, 0, 0, 'Weapon. Subject Character gains +1 Fighting. Turn to inflict 1 damage on target Character at this location.', 'A BuroMil Grunt''s best friend.', '', 'Ron Rousselle', 3, 1, 2, 8, 'N', '| buro blue spear gun  |');
INSERT INTO `card` VALUES (1107, 'BuroMil Grunt', 'Brutal Soldier', 1, '', 0x66, 2, 0, 0, 'Uncopyable. You may play BuroMil Grunt during an attack against you. Smoke BuroMil Grunt at the end of the turn he is played.', 'Their military careers are violent and brief.', '', 'Douglas Shuler', 5, 1, 2, 8, 'N', '| buro mil grunt brutal soldier |');
INSERT INTO `card` VALUES (1108, 'BuroMil "Savage"', 'Tank State', 4, 0x666648, '', 0, 0, 0, 'Vehicle. Subject Character gains Mobility, +6 Fighting and Toughness: 1.', 'For the first time, the Architects turned their weapons against each other.', '', 'Mike Jackson', 3, 1, 3, 8, 'N', '| buro mil savage tank |');
INSERT INTO `card` VALUES (1109, 'Cataract Gorge', 'Feng Shui Site', 0, '', '', 0, 1, 7, 'Unique. Turn to give target Character Independent until the end of the turn.', 'The Purists tricked Bonengel into authorizing the construction of the four Nexus towers.', '', 'Mark Pennington', 1, 6, 1, 8, 'N', '| cataract gorge |');
INSERT INTO `card` VALUES (1110, 'Cellular Reinvigoration', 'Event', 1, 0x66, '', 0, 0, 0, 'Until the end of the turn, target Character gains Guts and is not smoked if its Fighting is 0.', 'Arcanotech medicine will get you through the fight. Afterwards is your problem.', '', 'Mark Tedin', 4, 1, 2, 8, 'N', '| cellular reinvigoration |');
INSERT INTO `card` VALUES (1111, 'Chaos Spirit', 'Volatile Construct', 2, 0x4d4d, 0x4d, 2, 0, 0, 'At the end of each player''s turn, you must inflict 2 damage on a card the player to his or her left controls.', 'Creating one is a simple spell. Controlling it....', '', 'Steve Bryant', 5, 11, 2, 8, 'N', '| chao chaos spirit volatile construct |');
INSERT INTO `card` VALUES (1112, 'Chi Reconfiguration', 'Event', 1, 0x7070, '', 0, 0, 0, 'Smoke a Feng Shui Site you control, and return a Feng Shui Site in your smoked pile to play in the same position as the smoked Site. This may change the target of an attack.', '"The only difference between places is the shape of the chi flows."', '', 'Heather Bruton', 4, 13, 3, 8, 'N', '| chi reconfiguration |');
