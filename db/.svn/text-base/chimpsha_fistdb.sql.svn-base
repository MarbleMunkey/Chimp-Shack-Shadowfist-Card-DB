INSERT INTO `card` VALUES (1284, 'Rocket Man', 'Cyborg', 2, 0x66, 0x6648, 2, 0, 0, 'While attacking, Rocket Man gains Toughness: X and +X damage. X= the number of unturned Characters and turned Sites at his target''s location.', '', '', 'Paul "Prof" Herbert', 5, 1, 2, 9, 'N', '| rocket man cyborg |');
INSERT INTO `card` VALUES (1285, 'Roller Rink', 'Feng Shui Site', 0, '', '', 0, 1, 6, 'Turn to return to your hand a Character in your smoked pile whose cost is less than the amount of damage on this card.', 'When regular Disco just isn''t enough!', '', 'Tony Moseley', 1, 6, 2, 9, 'N', '| roller rink |');
