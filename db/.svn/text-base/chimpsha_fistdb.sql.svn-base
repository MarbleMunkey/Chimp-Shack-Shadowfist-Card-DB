INSERT INTO `card` VALUES (2379, 'Netherflitter', 'Weird Science State', 1, 0x6a6a48, '', 0, 0, 0, 'Vehicle. Subject Character gains +1 Fighting and Mobility. While attacking, subject takes no damage from Characters whose Fighting is even.', 'It''s not so bad once it gets off the ground.', '', 'Kevin Wasden', 3, 10, 3, 15, 'N', '| netherflitter weird science |');
