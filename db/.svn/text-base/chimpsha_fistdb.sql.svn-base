INSERT INTO `card` VALUES (1118, 'Corruption', 'Event', 1, 0x65, '', 0, 0, 0, 'All opponents'' Characters with a cost or Fighting of 4 or greater take 2 damage and turn.', 'The Lotus have never seen much value in a clean kill.', '', 'Alec Keating', 4, 5, 3, 8, 'N', '| corruption |');
INSERT INTO `card` VALUES (1119, 'Dark Sacrifice', 'Event', 1, 0x6d6d, '', 0, 0, 0, 'Steal 1 Power from target opponent. If you could not, that player must sacrifice a Character of his or her choice.', 'Ming I takes no prisoners - for long.', '', 'Brian Rood', 4, 7, 2, 8, 'N', '| dark sacrifice |');
INSERT INTO `card` VALUES (1120, 'Defiant Bloom', 'Edge', 1, 0x6767, 0x43, 0, 0, 0, 'Limited. Turn to remove 1 damage each from up to 3 cards you control.', 'Even in the Architects'' future, hope survives.', '', 'Andrew Barlow', 2, 8, 3, 8, 'N', '| defiant bloom |');
INSERT INTO `card` VALUES (1121, 'Deja Vu', 'Event', 1, 0x70704d, '', 0, 0, 0, 'Toast It. Immediately play an Event from your smoked pile (at normal cost) and toast that Event.', 'It''s not just a feeling when you''re dealing with the Purists.', '', 'Anthony Grabski', 4, 13, 3, 8, 'N', '| deja vu |');
INSERT INTO `card` VALUES (1122, 'Demon Tank', 'State', 3, 0x65654d, '', 0, 0, 0, 'Vehicle. Subject Character gains Toughness: 1, Regenerate, and +3 Fighting. Smoke subject if it is a <Demon>.', '"Do you know how hard it is to stare down a tank when it''s staring back?!?"', '', 'Stephen Snyder', 3, 5, 3, 8, 'N', '| demon tank |');
INSERT INTO `card` VALUES (1123, 'Desdemona Deathangel', 'Abomination', 5, 0x666666, 0x66, 6, 0, 0, 'Unique. Ambush. Sacrifice a Character to give Desdemona Deathangel +1 Fighting until the end of the turn.', '', '', 'Thomas Gianni', 5, 1, 1, 8, 'N', '| desdemona deathangel abomination |');
INSERT INTO `card` VALUES (1124, 'Destroyer', 'Demon Martial Artist', 4, 0x6565, 0x65, 4, 0, 0, 'Unique. Cannot be sacrificed. If Destroyer is in your smoked pile at the start of your turn, return Destroyer to play.', '"There''s got to be a better way to keep him down!"', '', 'Christopher Rush', 5, 5, 1, 8, 'N', '| destroyer demon martial artist |');
INSERT INTO `card` VALUES (1125, 'Destroyer Drone', 'Arcanowave Robot', 5, 0x66666648, 0x6648, 7, 0, 0, 'Toughness: 1. Cannot attack Characters. +4 damage against Sites.', 'While the Buro was distracted, Omega''s drones quickly seized the launch platforms.', '', 'Stephen Snyder', 5, 1, 3, 8, 'N', '| destroyer drone arcano wave robot |');
INSERT INTO `card` VALUES (1126, 'Dirk Wisely', 'Man of Action', 3, 0x646464, 0x6448, 4, 0, 0, 'Unique. Once per turn, you may unturn him at the end of a successful attack in which he was the only attacker.', '"Time to save the world again."', '', 'Mark Poole', 5, 4, 1, 8, 'N', '| dirk wisely man action |');
INSERT INTO `card` VALUES (1127, 'Dump Scrounger', 'Netherworld Scavenger', 2, 0x6a, 0x6a48, 2, 0, 0, 'Turn this card and toast a Character in your smoked pile to gain 1 Power.', '', '', 'Diana Vick', 5, 10, 2, 8, 'N', '| dump scrounger netherworld scavenger |');
INSERT INTO `card` VALUES (1128, 'Dunwa Saleem', 'Prophet of the Reascension', 3, 0x6161, 0x6148, 4, 0, 0, 'Unique. Generates 1 Power during your Establishing Shot if you control another [H] card. Once during your turn, you may look through your deck for a <Reascended> Character, show it to your opponents, and put it in your hand.', '', '', 'Arthur Roberg', 5, 2, 1, 8, 'N', '| dunwa saleem prophet reascension |');
INSERT INTO `card` VALUES (1129, 'Energy Flail', 'State', 1, 0x4d4d, '', 0, 0, 0, 'Weapon. Play on a Character. Turn and discard a card :: Inflict X damage on target Character at this location. X= the number of [M] resources provided and required by the subject.', '', '', 'Paul "Prof" Herbert', 3, 11, 3, 8, 'N', '| energy flail |');
INSERT INTO `card` VALUES (1130, 'Entropy Sphere', 'Event', 1, 0x7070, '', 0, 0, 0, 'Smoke target Character and inflict X damage on a front-row Feng Shui Site you control. X= the target''s cost.', '"Destruction has a price."', '', 'Arthur Roberg', 4, 13, 2, 8, 'N', '| entropy sphere |');
INSERT INTO `card` VALUES (1131, 'Entropy Tap', 'Event', 0, 0x704d, '', 0, 0, 0, 'Limited. Play when a Site you control takes combat damage but its Body is not reduced to 0 :: Gain X Power. X= half (round down) the damage inflicted.', 'Most geomancers will repair damaged chi. The Purists scavenge it.', '', 'Tony Moseley', 4, 13, 2, 8, 'N', '| entropy tap |');
INSERT INTO `card` VALUES (1132, 'Esteban Vicente', 'Geomancer Mastermind', 3, 0x707070, 0x704d, 5, 0, 0, 'Unique. When this card comes into play, you may rearrange target player''s Site structure; you may not change the number of columns.', '"Many have tried to defeat him - and failed."', '', 'Anthony Grabski', 5, 13, 1, 8, 'N', '| esteban vicente geomancer mastermind |');
INSERT INTO `card` VALUES (1133, 'FAE Schwartz', 'Incendiary Fanatic', 3, 0x6a6a, 0x6a, 3, 0, 0, 'Unique. When this card is smoked, inflict X damage on each Character and Site at his location. X= his Fighting unmodified by damage.', '"Boom, baby, boom!"', '', 'Randy Asplund', 5, 10, 3, 8, 'N', '| fae schwartz incendiary fanatic |');
INSERT INTO `card` VALUES (1134, 'Fake Out', 'Event', 0, 0x6464, '', 0, 0, 0, 'Play while you are attacking with more than one Character. Target interceptor ceases intercepting, but its controller may choose to intercept a different attacker with it.', '"Mooks ain''t any brighter in the future."', '', 'Mark Poole', 4, 4, 3, 8, 'N', '| fake out |');
INSERT INTO `card` VALUES (1135, 'Fallen Heroes', 'Event', 0, 0x6464, '', 0, 0, 0, 'Toast a Character in your smoked pile to give target Character +X Fighting until the end of the turn. X= the number of resources required and provided by the toasted Character.', 'The roster is always growing, but the Dragons won''t surrender.', '', 'Brian Rood', 4, 4, 3, 8, 'N', '| fallen hero heroe heroes |');
INSERT INTO `card` VALUES (1136, 'Fearsome Foe', 'State', 0, 0x6565, '', 0, 0, 0, 'If subject Character is attacking, each opponent who intercepts must intercept it if he or she can.', 'The tallest nail gets hammered down first.', '', 'Thomas Manning', 3, 5, 3, 8, 'N', '| fearsome foe |');
INSERT INTO `card` VALUES (1137, 'Fist of Freedom', 'Rebel Martial Artist', 3, 0x6767, 0x6743, 4, 0, 0, '+2 damage against the target of his attack if no interceptors were declared against him this attack.', 'They went underground when kung fu was banned.', '', 'Lissanne Lake', 5, 8, 2, 8, 'N', '| fist freedom rebel martial artist |');
INSERT INTO `card` VALUES (1138, 'Fortress Omega', 'Site', 3, 0x66666648, 0x6648, 0, 1, 12, 'Unique. Toughness: 1. When a non-Unique Character enters combat with this Site, inflict 1 damage on that Character. (Before combat damage is dealt.) Turn to play a <Cyborg> or <Drone> Character at -1 cost.', '', '', 'Mark Tedin', 1, 1, 1, 8, 'N', '| fortres fortress omega |');
INSERT INTO `card` VALUES (1139, 'Fo Shen', 'Eunuch Pawn', 2, 0x65, 0x654d, 2, 0, 0, 'Unique. Turn to look at the top card of an opponent''s deck. You may toast that card.', 'Nobody likes failures or traitors. He''s both.', '', 'Lissanne Lake', 5, 5, 1, 8, 'N', '| fo shen eunuch pawn |');
INSERT INTO `card` VALUES (1140, 'Free Fire Zone', 'Battleground Site', 1, 0x6a, '', 0, 0, 5, 'Limited. Any player who seizes this Site gains 2 Power. If this Site is in your front row, gain 1 Power when you smoke a Site.', 'The outbreak of civil war was just what the Jammers were waiting for.', '', 'Carisa Swenson', 1, 10, 3, 8, 'N', '| free fire zone battleground |');
INSERT INTO `card` VALUES (1141, 'Funky Monkey', 'Chimp Pimp', 3, 0x6a6a6a, 0x6a, 5, 0, 0, 'Unique.', 'The Jammers take "Disco Inferno" literally as the factions battle for the scariest territory of all - The ''70s. Strap on your platform shoes and your Saturday Night Special as Boom Chaka Laka, boogies its way to retailers this summer!', '', 'Lissanne Lake', 5, 10, 6, 8, 'N', '| funky monkey chimp pimp |');
INSERT INTO `card` VALUES (1142, 'General Olivet', 'BuroMil Military Mastermind', 4, 0x666666, 0x6666, 6, 0, 0, 'Unique. Tactics. You may play <Soldier>, <BuroMil>, or <Military> Characters during opponents'' turns. May attack during opponents'' turns, and <Soldier>, <BuroMil>, or <Military> Characters may attack with him.', '', '', 'Randy Asplund', 5, 1, 1, 8, 'N', '| general olivet buro mil military mastermind |');
INSERT INTO `card` VALUES (1143, 'Gloating Laughter', 'State', 0, 0x6565, '', 0, 0, 0, 'Limited. Play on a Character you control when you seize or burn a Site for victory. Subject gains +1 Fighting, or +2 if it is a <Eunuch>.', '"You cannot stop me now! HAHAHAHAHAHA!"', '', 'Brian Rood', 3, 5, 3, 8, 'N', '| gloating laughter |');
INSERT INTO `card` VALUES (1144, 'Got My Mojo Working', 'Event', 1, 0x646464, '', 0, 0, 0, 'Play in response to an opponent''s Event. Choose a Character that the Event would damage or smoke :: Unturn that Character, and it is not affected by the Event.', '"Did you really think it would be that easy?"', '', 'Dennis Calero', 4, 4, 2, 8, 'N', '| got my mojo working |');
INSERT INTO `card` VALUES (1145, 'Hell Charger', 'Demon State', 2, 0x6565, '', 0, 0, 0, 'Vehicle. Subject Character gains +3 Fighting. Remove 2 damage from subject at the start of your turn. Turn Hell Charger to change subject''s location.', 'They can outrun a motorcycle and kick through battleship plate.', '', 'Tom Castillo', 3, 5, 3, 8, 'N', '| hell charger demon |');
INSERT INTO `card` VALUES (1146, 'Hexagram Spirit', 'Sorcerous Construct', 4, 0x70704d, 0x704d, 6, 0, 0, 'When this card turns to attack, target an opponent and name a card :: Look at that player''s hand, and gain 1 Power for each copy of the named card in it.', '', '', 'Jason Millet', 5, 13, 3, 8, 'N', '| hexagram spirit sorcerou sorcerous construct |');
INSERT INTO `card` VALUES (1147, 'Homemade Tank', 'State', 2, 0x6a48, '', 0, 0, 0, 'Toast It. Vehicle. Subject Character gains Toughness: 1 and +4 Fighting. Flip a coin at the start of your turn. Heads: Toast Homemade Tank.', '"The engine''s as big a threat as enemy fire."', '', 'Dan Frazier', 3, 10, 2, 8, 'N', '| homemade tank |');
INSERT INTO `card` VALUES (1148, 'Hot Springs', 'Feng Shui Site', 0, '', '', 0, 1, 6, 'Turn and maintain and target a Character :: Target takes no damage from Sites, and is not affected by effects generated by other Sites.', '', '', 'Val Mayerik', 1, 6, 2, 8, 'N', '| hot spring springs |');
INSERT INTO `card` VALUES (1149, 'Ice Sorceress', 'Geomancer Mastermind', 2, 0x6d6d, 0x6d4d, 2, 0, 0, 'Turn and maintain to either add 3 or subtract 2 from target Feng Shui Site''s Body. If this would reduce the Site''s Body below 1, reduce it to 1 instead.', '', '', 'Edward Beard, Jr.', 5, 7, 3, 8, 'N', '| ice sorceres sorceress geomancer mastermind |');
INSERT INTO `card` VALUES (1150, 'Identity Chop Shop', 'Feng Shui Site', 0, '', '', 0, 1, 8, 'Limited. Turn and maintain to give or remove a designator of your choice from target Character you control. (This is not considered to change a card''s title.)', '"New ID? No problem, but impersonating a eunuch''s going to cost, eh?"', '', 'Randy Asplund', 1, 6, 3, 8, 'N', '| identity chop shop |');
INSERT INTO `card` VALUES (1151, 'IFF Missiles', 'Event', 1, 0x48, '', 0, 0, 0, 'Target an opponent :: Inflict 2 damage on each Character that opponent controls who does not share a designator with any other Character that player controls.', '"Cookie cutter technology just isn''t what it''s supposed to be."', '', 'Mark Evans', 4, 9, 2, 8, 'N', '| iff missil missile missiles |');
INSERT INTO `card` VALUES (1152, 'Inoue Oram', 'Sorcerer Mastermind', 2, 0x70, 0x704d, 2, 0, 0, 'Unique. Gain 1 power at the start of your Main Shot if you drew any cards during your Draw Phase and the number of cards drawn was even.', '', '', 'Melissa Benson', 5, 13, 3, 8, 'N', '| inoue oram sorcerer mastermind |');
INSERT INTO `card` VALUES (1153, 'Jack of All Trades', 'Resourceful Techie', 1, 0x6a, 0x6a48, 1, 0, 0, 'Turn this card and sacrifice a State :: Search through your deck for a State with a different title. Show it to your opponents and put it in your hand.', '', '', 'Randy Asplund', 5, 10, 2, 8, 'N', '| jack all trad trade trades resourceful techie |');
INSERT INTO `card` VALUES (1154, 'Jason X', 'Brainwashed Supersoldier', 4, 0x666666, 0x6664, 6, 0, 0, 'Unique. Guts. The cost of all [d] Events is increased by 1.', 'There''s very little Jason X left.', '', 'Ron Rousselle', 5, 1, 1, 8, 'N', '| jason x brainwashed super soldier |');
INSERT INTO `card` VALUES (1155, 'Jet Pack', 'State', 1, 0x4848, '', 0, 0, 0, 'Vehicle. Subject Character gains Mobility and +1 Fighting. Turn and flip a coin :: Heads: Subject gains Superleap and +2 Fighting until the end of the turn. Tails: Inflict 3 damage on subject.', '"Just what you need for the morning commute."', '', 'Paul "Prof" Herbert', 3, 9, 2, 8, 'N', '| jet pack |');
INSERT INTO `card` VALUES (1156, 'Johann Bonengel', 'BuroPresident', 3, 0x6666666666, 0x6666, 5, 0, 0, 'Unique. Opponents cannot play States on cards you control while this card is unturned. Turn to gain 2 Power.', 'He thought he ruled 2062 with absolute authority.', '', 'Dennis Detwiller', 5, 1, 1, 8, 'N', '| johann bonengel buro president |');
INSERT INTO `card` VALUES (1157, 'Johnny Badhair', 'Wild Man', 5, 0x646464, 0x646a, 7, 0, 0, 'Unique. Guts. Cannot be sacrificed. At the end of a turn in which this card was smoked, cut your deck, draw a card, and show it to your opponents. If the card is a State or a Character with a resource condition, return this card to play.', '', '', 'Phil Hale', 5, 4, 1, 8, 'N', '| johnny badhair wild man |');
INSERT INTO `card` VALUES (1158, 'Jury-Rigged Dynamo', 'State', 1, 0x6a48, '', 0, 0, 0, 'Limited. Play on a Site you control. When an opponent gains Power outside of his or her Establishing Shot, you may either unturn this card, or turn it to gain 1 Power.', '"You gotta know where to kick it."', '', 'Randy Asplund', 3, 10, 2, 8, 'N', '| jury rigged dynamo |');
INSERT INTO `card` VALUES (1159, 'Just a Rat', 'Lodge Survivor', 3, 0x61, 0x61, 3, 0, 0, 'Stealth. Inflict 2 damage on any Character that Just a Rat bypasses using Stealth.', 'A few still survive, with distant memories of what they used to be.', '', 'Matt Wilson', 5, 2, 2, 8, 'N', '| just rat lodge survivor |');
INSERT INTO `card` VALUES (1160, 'Kar Fai''s Last Stand', 'Event', 1, 0x6464, '', 0, 0, 0, 'Faceoff. Turn a Character you control and target an opponent''s Character with Fighting no lower than two less than yours. The winner''s Character gains Guts and Toughness: 1 until it leaves play.', '"You can''t win."\n"I don''t need to."', '', 'Mark Tedin', 4, 4, 3, 8, 'N', '| kar fai fais last stand |');
INSERT INTO `card` VALUES (1161, 'Kar Fai''s Legacy', 'Event', 0, 0x646464, '', 0, 0, 0, 'Play when a Unique Character you control is smoked. Toast that Character :: Gain Power equal to half (round up) of that Character''s cost.', 'Kar Fai stayed behind to save us all.', '', 'Jason Millet', 4, 4, 3, 8, 'N', '| kar fai fais legacy |');
INSERT INTO `card` VALUES (1162, 'Killing Ground', 'Battleground Site', 0, '', '', 0, 0, 5, 'Any player who seizes this site gains 2 Power. If Killing Ground is in your front row, all of your Characters inflict +1 damage on interceptors, and you may play non-Unique 0-cost non-Feng Shui Sites even if you have already played a Site during your turn.', '', '', 'Mark Raabe', 1, 12, 2, 8, 'N', '| killing ground battleground |');
INSERT INTO `card` VALUES (1163, 'King Kung', 'Cyborg Gorilla', 6, 0x6a6a6a6a48, 0x6a, 12, 0, 0, 'Unique. If King Kung enters combat with a Site, he enters combat with all Sites at that location simultaneously. (Divide his combat damage among the Sites.)', '', '', 'Jim Pavelec', 5, 10, 1, 8, 'N', '| king kung cyborg gorilla |');
INSERT INTO `card` VALUES (1164, 'Legion of the Damned', 'Undead Soldiers', 3, 0x6565, 0x65, 3, 0, 0, 'When Legion of the Damned turns to attack, you may inflict 2 damage on a Site.', 'The Lotus now use the Architects'' forces against them.', '', 'Thomas Manning', 5, 5, 2, 8, 'N', '| legion damned undead soldier soldiers |');
INSERT INTO `card` VALUES (1165, 'Louie the Roach', 'Lodge Survivor', 2, 0x616161, 0x61, 2, 0, 0, 'Unique. Toughness: 2.', '"They''ve tried to kill me more ways than you can imagine, but they ain''t succeeded yet."', '', 'April Lee', 5, 2, 1, 8, 'N', '| louie roach lodge survivor |');
INSERT INTO `card` VALUES (1166, 'Major Hottie', 'Ex-Buro Operative', 4, 0x6a6a, 0x6a, 5, 0, 0, 'Unique. All [j] Characters gain Toughness: 1 against combat damage. Turn during an attack :: All [j] Characters inflict +2 damage until the end of the attack.', '', '', 'Edward Beard, Jr.', 5, 10, 1, 8, 'N', '| major hottie ex buro operative |');
INSERT INTO `card` VALUES (1167, 'Marauder Gang', 'Renegade Cyborgs', 2, 0x48, 0x48, 3, 0, 0, 'At the end of your turn, smoke this card if it did not attack this turn.', 'Deserters and brigands who thrive in the chaos of 2062.', '', 'Ron Rousselle', 5, 9, 2, 8, 'N', '| marauder gang renegade cyborg cyborgs |');
INSERT INTO `card` VALUES (1168, 'Marauder Lord', 'Ruthless Cyborg', 4, 0x4848, 0x48, 5, 0, 0, 'When Marauder Lord turns to attack, he gains +1 Fighting until he leaves play.', 'Petty warlords who carve out tiny empires in the fringes of order.', '', 'Ron Rousselle', 5, 9, 3, 8, 'N', '| marauder lord ruthles ruthless cyborg |');
INSERT INTO `card` VALUES (1169, 'Mark IV Fusion Rifle', 'State', 1, 0x48484848, '', 0, 0, 0, 'Weapon. Play on a Character. Unturn this card when an opponent targets a card you control with an Event or Site. Turn to inflict 3 damage to a Character or Site at subject''s location.', '"Yeah, it rocks, _if_ you can figure out the manual."', '', 'Tony Moseley', 3, 9, 3, 8, 'N', '| mark iv fusion rifle |');
INSERT INTO `card` VALUES (1170, 'Master Gardener', 'Elderly Geomancer', 2, 0x6767, 0x6743, 2, 0, 0, 'Turn and maintain to cancel target Feng Shui Site''s effects and rules text.', 'Quan Lo found the idea of artificial Feng Shui Sites disturbing.', '', 'Brian Rood', 5, 8, 3, 8, 'N', '| master gardener elderly geomancer |');
INSERT INTO `card` VALUES (1171, 'Master Hao', 'Superior Martial Artist', 4, 0x67676767, 0x6743, 6, 0, 0, 'Unique. You may choose a Character once per turn while Master Hao is attacking. He takes no damage from that Character until the end of the turn.', '', '', 'Dennis Calero', 5, 8, 1, 8, 'N', '| master hao super ior martial artist |');
INSERT INTO `card` VALUES (1172, 'Master Mechanic', 'Renegade Techie', 3, 0x6464, 0x6448, 4, 0, 0, 'Turn during any Main Shot to play a Vehicle from your smoked pile on a card you control at -1 cost.', '"Well, I''ve seen worse... I think."', '', 'Paul Carrick', 5, 4, 3, 8, 'N', '| master mechanic renegade techie |');
INSERT INTO `card` VALUES (1173, 'MegaTank', 'State', 5, 0x484848, '', 0, 0, 0, 'Vehicle. Subject Character gains +7 Fighting and Toughness: 2. Subject cannot be damaged or removed from play by [f] Events.', 'The only commuter vehicle you can drive on both sidewalks.', '', 'Jeff Menges', 3, 9, 3, 8, 'N', '| mega tank |');
INSERT INTO `card` VALUES (1174, 'Memory Palace', 'Event', 1, 0x707070, '', 0, 0, 0, 'One-Shot. Lay your entire hand on the table, face up. You may play these cards as if they were in your hand, but cannot discard them. These cards are not in play or in your hand.', '', '', 'Brian Rood', 4, 13, 1, 8, 'N', '| memory palace |');
INSERT INTO `card` VALUES (1175, 'Memory Reprocessing', 'Event', 1, 0x4d4d, '', 0, 0, 0, 'Toast It. Immediately play an event from target opponent''s smoked pile at normal cost; that player gains 1 Power.', 'The mind is the window to the soul. The Paradox Cube is the brick.', '', 'Anson Maddocks', 4, 11, 3, 8, 'N', '| memory reprocessing |');
INSERT INTO `card` VALUES (1176, 'Memory Spirit', 'Psychic Construct', 3, 0x704d, 0x704d, 4, 0, 0, 'When any opponent does not discard any cards during his or her Establishing Shot, Memory Spirit gains +1 Fighting until it leaves play.', 'It feeds on what you won''t let go of.', '', 'Steve Bryant', 5, 13, 2, 8, 'N', '| memory spirit psychic construct |');
INSERT INTO `card` VALUES (1177, 'Misery Totelben', 'Insane Musician', 3, 0x70, 0x704d, 3, 0, 0, 'Unique. Uncopyable. -2 damage. Cannot attack, intercept, or turn. Each of your back-row Sites copies the rules text of the Feng Shui Site in front of it.', '', '', 'Mark Poole', 5, 13, 1, 8, 'N', '| misery totelben insane musician |');
INSERT INTO `card` VALUES (1178, 'Mobile HQ', 'State', 3, 0x6464, 0x48, 0, 0, 0, 'Unique. Vehicle. Subject Character gains +3 Fighting and Mobility. If subject is unturned, turn Mobile HQ to heal target Character.', '"You have to stay on the move when operating in 2062."', '', 'Thomas Manning', 3, 4, 1, 8, 'N', '| mobile hq |');
INSERT INTO `card` VALUES (1179, 'Motor Pool', 'Site', 2, '', '', 0, 1, 5, 'Generates no Power while turned. Turn to play a Vehicle on a Character you control at -2 cost.', '"It''s like a supermarket. Megatanks, aisle three...."', '', 'Brian Rood', 1, 12, 3, 8, 'N', '| motor pool |');
INSERT INTO `card` VALUES (1180, 'Mount Makarakomburu', 'Feng Shui Site', 3, '', '', 0, 2, 8, 'Unique. Cannot be burned. Mount Makarakomburu can be the target of attacks while in the back row.', 'Location of the first of the Purists'' four towers.', '', 'Anthony Grabski', 1, 6, 1, 8, 'N', '| mount makarakomburu |');
INSERT INTO `card` VALUES (1181, 'Mutator', 'Probability Spirit', 4, 0x70704d4d, 0x704d, 6, 0, 0, 'Once per turn, you may raise or lower by one until the end of the turn any number not written as words on another card at this location. This cannot change a number to zero.', '', '', 'Anthony Grabski', 5, 13, 3, 8, 'N', '| mutator probability spirit |');
INSERT INTO `card` VALUES (1182, 'Newest Model', 'State', 0, 0x4848, '', 0, 0, 0, 'Not cumulative. The values of all numbers not written as words in the rules text of subject Vehicle or [H] State are increased by one. Any opponent''s effect that would directly remove subject from play smokes Newest Model instead.', '', '', 'Tony Moseley', 3, 9, 2, 8, 'N', '| newest model |');
INSERT INTO `card` VALUES (1183, 'Nexus Tower', 'Site', 3, 0x7070704d, 0x704d, 0, 1, 8, 'Unique. The number of Feng Shui Sites you need for victory is reduced by one. Once during your turn, you may play a [M] card at +1 cost from your smoked pile, that card is considered to be Toast-It.', '', '', 'Carisa Swenson', 1, 13, 1, 8, 'N', '| nexu nexus tower |');
INSERT INTO `card` VALUES (1184, 'No Man''s Land', 'Battleground Site', 1, '', '', 0, 0, 5, 'Any player who seizes this Site gains 2 Power. If this card is in your front row, turn and maintain and target a Character :: That Character''s damage cannot be reduced or redirected by opponents.', '', '', 'Mark Evans', 1, 12, 2, 8, 'N', '| no man mans land battleground |');
INSERT INTO `card` VALUES (1185, 'Nuked', 'Event', 1, 0x6a6a, '', 0, 0, 0, 'Choose an opponent and flip a coin. Tails: repeat the coin flip with the next player to the left. Heads: choose a Site controlled by that player :: Smoke the chosen Site.', '"Blow Things Up! Blow Things Up! Blow Things - Wait, not that!"', '', 'Nicola Leonard', 4, 10, 2, 8, 'N', '| nuked |');
INSERT INTO `card` VALUES (1186, 'Orbital Laser Strike', 'Event', 1, 0x48, '', 0, 0, 0, 'Inflict X damage on target Site. X= the number of [H] resources in your pool.', 'It was sold to the public as a purely defensive technology.', '', 'John T. Snyder', 4, 9, 2, 8, 'N', '| orbital laser strike |');
INSERT INTO `card` VALUES (1187, 'Paradox Divination', 'Event', 1, 0x704d, '', 0, 0, 0, 'Look at the top X cards of your deck, and return them in any order. X= the number of [M] resources in your pool.', 'The Purists see the future as a series of possibilities to be manipulated.', '', 'Tom Castillo', 4, 13, 3, 8, 'N', '| paradox divination |');
INSERT INTO `card` VALUES (1188, 'Parting Gift', 'Event', 1, 0x6a6a, '', 0, 0, 0, 'Play when an attack you declared on a Site ends if you did not reduce the Site''s Body to 0. Inflict 2 damage on the Site and all Characters at that location. (Including any remaining attackers.)', '"Here''s a present. Don''t mind the fuse."', '', 'Arthur Roberg', 4, 10, 2, 8, 'N', '| parting gift |');
INSERT INTO `card` VALUES (1189, 'Portal Nexus', 'State', 1, 0x7070, '', 0, 0, 0, 'Schtick. Subject non-Unique Site has +3 Body. Turn to exchange the positions of Feng Shui Sites you control. This may change the target of an attack.', 'The heart of the four towers constructed by the Purists.', '', 'Aaron Boyd', 3, 13, 3, 8, 'N', '| portal nexu nexus |');
INSERT INTO `card` VALUES (1190, 'Primus', 'Master of Paradox', 4, 0x707070, 0x704d4d, 7, 0, 0, 'Unique. When Primus enters play, target an opponent, who cannot respond with Events :: Target player cannot play cards this turn.', '"Nothing can stop us from reshaping the world."', '', 'Thomas Gianni', 5, 13, 1, 8, 'N', '| primu primus master paradox |');
INSERT INTO `card` VALUES (1191, 'Purist Aspirant', 'Expendable Lackey', 1, '', 0x70, 1, 0, 0, 'Sacrifice to reduce to 0 all non-combat damage from a single source to Characters you control.', '', '', 'Arthur Roberg', 5, 13, 2, 8, 'N', '| purist aspirant expendable lackey |');
