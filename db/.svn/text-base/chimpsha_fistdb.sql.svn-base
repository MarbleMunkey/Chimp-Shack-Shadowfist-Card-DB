INSERT INTO `card` VALUES (3230, 'Iron Hands Ma', 'Rebel Martial Artist', 2, 0x6767, 0x67, 2, 0, 0, 'Unique. Toughness: 1. +3 damage to Characters.', 'The law is strong, but the outlaws are ten times stronger.', '', 'Diego Candia', 5, 8, 3, 17, 'N', '| iron hand hands ma rebel martial artist |');
INSERT INTO `card` VALUES (3231, 'The Iron Palace', 'Secret Feng Shui Site', 0, '', '', 0, 1, 7, 'Unique. Turn and maintain :: Target Character gains Toughness: 1.', 'Xin Kai Sheng wanted a more defensible capital.', '', 'Adam Bray', 1, 6, 1, 17, 'N', '| the iron palace secret |');
INSERT INTO `card` VALUES (3228, 'The Inner Garden', 'Site', 2, 0x674343, 0x43, 0, 1, 7, 'Turn :: Return a [C] State you own to your hand. Turn :: Play a [C] State at -1 cost.', 'Tranquility is a fortress of the mind.', '', 'Garrett Byers', 1, 8, 3, 17, 'N', '| the inner garden |');
INSERT INTO `card` VALUES (3229, 'Inside Man', 'Connected Hood', 4, 0x7979, 0x79, 5, 0, 0, 'Turn X Influence :: This card gains +X Fighting until the end of the turn.', 'Somebody in your organization owes the Syndicate a favor.', '', 'Banu Adhimuka', 5, 15, 3, 17, 'N', '| inside man connected hood |');
INSERT INTO `card` VALUES (3227, 'Improvised Weapons', 'State', 1, '', '', 0, 0, 0, 'Weapon. Enters play with 1 counter. When subject Character enters combat, place a counter on this card. Subject Character inflicts +X damage. X= the number of counters.', '"You take the hockey stick. I''ve got dibs on the lobster."', '', 'Brian England', 3, 12, 2, 17, 'N', '| improvised weapon weapons |');
INSERT INTO `card` VALUES (3226, 'Impossible Men', 'Self-Made Paradoxes', 4, 0x70704d, 0x70, 6, 0, 0, 'When this card turns to attack, you may immediately turn 1 card controlled by each opponent.', 'Few among the Purists expected their Paradox Cubes to start hatching.', '', 'Eric Williams', 5, 13, 3, 17, 'N', '| impossible man men self made paradox paradoxe paradoxes |');
INSERT INTO `card` VALUES (3224, 'Ice Blessing', 'Event', 0, 0x6d, 0x4d, 0, 0, 0, 'Limited. Heal one or two cards controlled by opponents and gain 1 Power per card chosen. (You cannot heal undamaged cards.)', 'For a fleeting moment, you feel the cool kiss of Pi Tui.', '', 'Kevin Wasden', 4, 7, 2, 17, 'N', '| ice blessing |');
INSERT INTO `card` VALUES (3225, 'Ice Carvers', 'Netherworld Mystics', 1, 0x6d, 0x6d4d, 1, 0, 0, 'You may remove 1 damage from a card you control at the end of each turn.', 'Only the most talented may attempt the likeness of their Queen.', '', 'Cristian Saksida', 5, 7, 2, 17, 'N', '| ice carver carvers netherworld mystic mystics |');
INSERT INTO `card` VALUES (3222, 'Howling for Blood', 'Event', 1, 0x66, '', 0, 0, 0, 'Play during an attack :: Target Character gains +X damage and Toughness: X until the end of the attack. X= 1, or 2 if target is an <Abomination>.', '"The foaming spittle means they''re ready."', '', 'Brian England', 4, 1, 2, 17, 'N', '| howling blood |');
INSERT INTO `card` VALUES (3223, 'Hydroponic Garden', 'Martian Feng Shui Site', 0, '', '', 0, 1, 7, 'When this card is seized or burned, gain 1 Power and you may play a Feng Shui Site face-up at no cost.', 'The terraforming process requires careful management of chi flow.', '', 'Cynthia Cummens', 1, 6, 2, 17, 'N', '| hydroponic garden martian |');
INSERT INTO `card` VALUES (3221, 'Hot Zone', 'Wasteland Site', 2, '', '', 0, 1, 4, 'Turn and maintain :: Cancel target Site.', 'A nanotech accident reduced most of North America to gray goo.', '', 'Wes Jones', 1, 12, 3, 17, 'N', '| hot zone wasteland |');
INSERT INTO `card` VALUES (3220, 'Homicide Detective', 'Jaded Cop', 4, 0x6464, 0x64, 6, 0, 0, 'Cannot be intercepted by Characters whose Fighting is less than the number of cards in their controller''s hand. Once per turn, you may pay 1 Power :: All players draw 3 cards.', '', '', 'Cristian Saksida', 5, 4, 3, 17, 'N', '| homicide detective jaded cop |');
INSERT INTO `card` VALUES (3218, 'Hirake Kazuko', 'Corporate Mastermind', 3, 0x79797979, 0x79, 3, 0, 0, 'Unique. Influence: 4. Turn and maintain :: Take control of target non-Unique Character.', '"Everybody has a price. Not all of them are worth paying."', '', 'Banu Adhimuka', 5, 15, 1, 17, 'N', '| hirake kazuko corporate mastermind |');
INSERT INTO `card` VALUES (3219, 'Hit Squad', 'Violent Gangsters', 1, 0x61, 0x61, 1, 0, 0, 'Turn :: Turn target Character.', 'A carload of heavily armed gunmen makes a great diversion.', '', 'Jon Hodgson', 5, 2, 2, 17, 'N', '| hit squad violent gangster gangsters |');
INSERT INTO `card` VALUES (3215, 'Harbinger', 'Secret Herald of the Fire King', 5, 0x6d6d, 0x6d4348, 9, 0, 0, 'Unique. Play only if you control a <Secret> card. When this card enters combat, inflict 2 damage on every card it is in combat with.', '', '', 'Eric Williams', 5, 7, 1, 17, 'N', '| harbinger secret herald fire king |');
INSERT INTO `card` VALUES (3216, 'Haunted', 'Ghost State', 0, 0x65, '', 0, 0, 0, 'All damage inflicted on subject card is increased by 1. You may Reload this card when subject is smoked.', '"The voices came to me again last night. They told me that my blood was trapped inside my body -- and I should let it out."', '', 'Banu Adhimuka', 3, 5, 2, 17, 'N', '| haunted ghost |');
INSERT INTO `card` VALUES (3217, 'The Hegemeister', 'Tank State', 3, 0x4848, '', 0, 0, 0, 'Unique. Vehicle. Subject Character gains +3 Fighting and Mobility. Subject cannot be intercepted by Characters that are not the subject of a Vehicle.', '"Target acquired, buddy! Who wants a hug?"', '', 'Brian England', 3, 9, 1, 17, 'N', '| the hegemeister tank |');
INSERT INTO `card` VALUES (3213, 'Habbakuk', 'Floating Feng Shui Site', 3, 0x616161, 0x61, 0, 1, 7, 'Unique. All <Lodge> Characters you control gain +2 Fighting. Turn :: Draw 3 cards.', 'In its halls, the old families will either unite or destroy each other.', '', 'Diego Candia', 1, 2, 1, 17, 'N', '| habbakuk floating |');
INSERT INTO `card` VALUES (3214, 'The Hand of the Underworld', 'Killer Demon', 2, 0x654d, 0x654d, 4, 0, 0, 'Unique. Mobility. At the end of the turn in which this card entered play, return it to your hand unless you sacrifice a Character.', '', '', 'Adam Bray', 5, 5, 3, 17, 'N', '| the hand underworld killer demon |');
INSERT INTO `card` VALUES (3208, 'Gathering the Fire', 'Event', 1, 0x6d, '', 0, 0, 0, 'Inflict X damage on target card. X= the number of <Fire> cards in play.', '"Focus yourself, then strike with incendiary fury."', '', 'Dan Harding', 4, 7, 2, 17, 'N', '| gathering fire |');
INSERT INTO `card` VALUES (3209, 'General Gog', 'Abomination Mastermind', 5, 0x66666648, 0x66, 9, 0, 0, 'Unique. Tactics. Once per turn when this card uses Tactics, you may unturn it, and the attack is considered to be a success.', '', '', 'Eric Williams', 5, 1, 1, 17, 'N', '| general gog abomination mastermind |');
INSERT INTO `card` VALUES (3210, 'Geoffrey Smythe', 'Cultist Traitor', 2, 0x666666, 0x66704d, 3, 0, 0, 'Unique. Turn :: Turn target Site, inflict 1 damage on it, and cancel it until the end of the turn.', 'Purity is relative.', '', 'Regis Moulun', 5, 1, 1, 17, 'N', '| geoffrey smythe cultist traitor |');
INSERT INTO `card` VALUES (3211, 'The Gimp', 'Nanotech Mutant', 5, 0x6a48, 0x6a, 7, 0, 0, 'Unique. All Sites at the location of the target of this card''s attack are canceled until the end of the turn. Any Character intercepting this card is canceled until the end of the turn.', '', '', 'Ed Cox', 5, 10, 6, 17, 'N', '| the gimp nanotech mutant |');
INSERT INTO `card` VALUES (3212, 'The Gray', 'Spirit of Absence', 5, 0x7070704d, '', 7, 0, 0, 'Unique. Uncopyable. The rules text of all Characters you do not control is considered to be blank. Any opponent may pay you 1 Power :: This card''s rules text is blank until the end of the turn.', '', '', 'Eric Williams', 5, 13, 1, 17, 'N', '| the gray spirit absence |');
INSERT INTO `card` VALUES (3207, 'Frenzy Engine', 'Arcanowave State', 1, 0x6648, '', 0, 0, 0, 'Play on a Site. When you declare attackers, any that were at this location gain +1 damage until the end of the attack.', '"When you have fewer troops to throw at a problem, you need to throw them harder."', '', 'Cristian Saksida', 3, 1, 3, 17, 'N', '| frenzy engine arcano wave |');
INSERT INTO `card` VALUES (3204, 'Floating Teeth', 'Darkness Horrors', 3, 0x6d, 0x6d, 4, 0, 0, 'When you sacrifice a Character, this card gains +1 Fighting until it leaves play.', 'Once they bite you, they don''t let go.', '', 'Dan Harding', 5, 7, 2, 17, 'N', '| floating teeth darkness darknes darkne darkn horror horrors |');
INSERT INTO `card` VALUES (3205, 'Forgotten Temple', 'Feng Shui Site', 0, '', '', 0, 1, 7, 'Limited. Turn at the end of any opponent''s turn in which at least one card you own was smoked :: Gain 1 Power.', 'Consecrated ground, healing the scarred earth of a terrible massacre.', '', 'Susan Stejskal', 1, 6, 3, 17, 'N', '| forgotten temple |');
INSERT INTO `card` VALUES (3206, 'Freelance Platoon', 'Abandoned Pledged Agents', 2, 0x61, 0x61, 2, 0, 0, 'Stealth. When this card damages the target of its attack, you may draw a card.', '"Just because we don''t get orders doesn''t mean the mission stops."', '', 'Jonny Scull', 5, 2, 2, 17, 'N', '| freelance platoon abandoned pledged agent agents |');
INSERT INTO `card` VALUES (3203, 'Flambards', 'Fire Secret Society', 3, 0x6d, 0x6d43, 4, 0, 0, 'When this card enters or leaves play, inflict 1 damage on all opponents'' Sites.', 'Lost souls remade by their allegiance to the Fire King.', '', 'Jonny Scull', 5, 7, 2, 17, 'N', '| flambard flambards fire secret society |');
INSERT INTO `card` VALUES (3202, 'Feral Regression', 'State', 1, 0x6161, '', 0, 0, 0, 'Subject Character''s rules text is blank. It gains +X Fighting if it is <Lodge> or <Transformed>. X= the number of [a] resources required and provided by subject.', '"I am still a man. They have not taken that away from me. Yet."', '', 'Adam Bray', 3, 2, 3, 17, 'N', '| feral regression |');
INSERT INTO `card` VALUES (3200, 'Elephant Style', 'State', 1, 0x676743, '', 0, 0, 0, 'Subject Character gains Toughness: X. X= 1 + the number of [C] provided by subject.', 'Strikes unfold like the great beast''s trunk; blows that would fell a horse are easily received.', '', 'Kevin Wasden', 3, 8, 2, 17, 'N', '| elephant style |');
INSERT INTO `card` VALUES (3201, 'Everything Falls Apart', 'Edge', 0, 0x616161, '', 0, 0, 0, 'Limited. When any player declares one or more attackers, he or she gains 1 Power. No player may gain more than 2 Power this way per turn.', '', '', 'Martin Blanco', 2, 2, 3, 17, 'N', '| everything fall falls apart |');
INSERT INTO `card` VALUES (3199, 'Echo Distortion', 'Event', 1, 0x70, 0x70, 0, 0, 0, 'Copy a Site''s rules text to target Site until the end of the turn.', '"There are no perfect reflections."', '', 'Dan Harding', 4, 13, 2, 17, 'N', '| echo distortion |');
INSERT INTO `card` VALUES (3198, 'Dimitri Lyapunov', 'Cybernetics Mastermind', 2, 0x7979, 0x7948, 2, 0, 0, 'Unique. Pay 1 Power or turn :: Return any card from your smoked pile to play as a 1-Fighting Toast-It Character with the title "Drone". The rest of the card is treated as if it were printed blank.', '', '', 'Dan Murray', 5, 15, 1, 17, 'N', '| dimitri lyapunov cybernetic cybernetics mastermind |');
INSERT INTO `card` VALUES (3197, 'Devendra Chalal', 'The Burning Hand', 5, 0x79797948, 0x79, 7, 0, 0, 'Unique. +3 damage. Cannot be intercepted by Characters with a cost of 2 or less.', '"If the world ends in three years, why not let them all burn?"', '', 'Dan Murray', 5, 15, 1, 17, 'N', '| devendra chalal the burning hand |');
INSERT INTO `card` VALUES (3196, 'Demonic Alliance', 'Edge', 1, 0x6565, '', 0, 0, 0, 'Turn and maintain :: Target Character gains Regenerate.', 'In Empire of Evil, the plans of the Lotus come to fruition and could change the Secret War forever!', '', 'Diego Candia', 2, 5, 6, 17, 'N', '| demonic alliance |');
INSERT INTO `card` VALUES (3193, 'Cybermod Parlor', 'Site', 2, '', 0x7948, 0, 1, 6, 'If there is only one Site at a location you control, you may play this card into the front row at that location. (Move the other Site to the back row.)', 'The place to go when you want to pimp your chi.', '', 'Oliver Castaneda', 1, 15, 2, 17, 'N', '| cybermod parlor |');
INSERT INTO `card` VALUES (3194, 'Cyborg Mermaid', 'Aquatic Saboteur', 4, 0x6a6a, 0x6a48, 5, 0, 0, 'Guts. Once per turn, you may inflict 1 damage on all your Sites :: This card gains +2 Fighting until the end of the turn.', '', '', 'Oliver Castaneda', 5, 10, 3, 17, 'N', '| cyborg mermaid aquatic saboteur |');
INSERT INTO `card` VALUES (3195, 'Delay the Inevitable', 'Event', 1, 0x7070, '', 0, 0, 0, 'Play in response to an Event, State, or Edge :: Cancel that card, and put it face up in front of the player who played it. He or she may play it normally at no cost, but cannot do so this turn.', 'Time is only there to keep everything from happening at once.', '', 'Oliver Castaneda', 4, 13, 2, 17, 'N', '| delay inevitable |');
INSERT INTO `card` VALUES (3191, 'Corporate Warfare', 'Edge', 1, 0x79, '', 0, 0, 0, 'Influence: 2. Sacrifice :: Smoke target Edge.', 'The Syndicate can mobilize entire industrial sectors with as little as a single text message.', '', 'Oliver Castaneda', 2, 15, 3, 17, 'N', '| corporate warfare |');
INSERT INTO `card` VALUES (3192, 'Crane Stance', 'State', 0, 0x6767, '', 0, 0, 0, 'The next time subject Character would take damage, reduce the damage to zero and sacrifice this card instead.', '"Adopt the stance, grasshopper, and wait."', '', 'Mark Poole', 3, 8, 2, 17, 'N', '| crane stance |');
INSERT INTO `card` VALUES (3190, 'Cloaca', 'Demon King of the Hong Kong Sewers', 5, 0x656565, 0x654d, 8, 0, 0, 'Unique.\r\nSacrifice a Character and choose Event, Site, or Edge :: This card is not affected by cards of the chosen type until the end of the turn.', '', '', 'Adam Bray', 5, 5, 1, 17, 'N', '| cloaca demon king hong kong sewer sewers |');
INSERT INTO `card` VALUES (3189, 'Christine Winter', 'Fractured Sorcerer Prodigy', 2, 0x70704d, 0x704d, 3, 0, 0, 'You may sacrifice a Character when this card is smoked :: Return this card to play turned.', 'She barely noticed when her own body died.', '', 'Oliver Castaneda', 5, 13, 3, 17, 'N', '| christine winter fractured sorcerer prodigy |');
INSERT INTO `card` VALUES (3188, 'Cheng Hu Bai', 'Red Master', 4, 0x676767, 0x6743, 7, 0, 0, 'Unique. Superleap. Can attack back-row Sites. Cannot be declared as an attacker on front-row Sites.', '', '', 'Regis Moulun', 5, 8, 1, 17, 'N', '| cheng hu bai red master |');
INSERT INTO `card` VALUES (3186, 'Cannibal''s Banquet', 'Event', 0, 0x654d, '', 0, 0, 0, 'Limited. Play when a Character you control is smoked by an opponent. You gain Power equal to half (round down) that Character''s cost.', '"Those who can no longer fight may feed the rest."', '', 'Mike Jackson', 4, 5, 2, 17, 'N', '| cannibal cannibals banquet |');
INSERT INTO `card` VALUES (3187, 'Catching Bullets', 'Event', 1, 0x79, '', 0, 0, 0, 'Play in response to an Event that targets a Character you control :: Cancel the Event, and put the card face up in front of you. You may play it, ignoring resource conditions, as if it were in your hand, but cannot discard it. Toast it at the end of your next turn if it is unplayed.', '', '', 'Diego Candia', 4, 15, 2, 17, 'N', '| catching bullet bullets |');
INSERT INTO `card` VALUES (3185, 'Cannibal Army', 'Albino Horde', 2, '', 0x65, 2, 0, 0, 'When an opponent plays a Site, you may inflict 1 damage on a card that he or she controls.', 'An army marches on its stomach. This one marches on yours.', '', 'Zach McCain', 5, 5, 2, 17, 'N', '| cannibal army albino horde |');
INSERT INTO `card` VALUES (3184, 'Buzzsaw Arm', 'Brutal Weapon State', 1, 0x4848, '', 0, 0, 0, 'Weapon. Subject Character gains +2 damage. When subject enters combat with a Character, that Character gains -1 Fighting until it leaves play.', '"Come get some!"', '', 'Ed Cox', 3, 9, 3, 17, 'N', '| buzzsaw arm brutal weapon |');
INSERT INTO `card` VALUES (3183, 'Bush Pilot', 'Rugged Hero', 2, '', 0x64, 2, 0, 0, 'Mobility. You may attack back-row Sites if this card is declared as an attacker.', '"That scar? I got it in the Congo saving the life of a beautiful heiress."', '', 'Cristian Saksida', 5, 4, 2, 17, 'N', '| bush pilot rugged hero |');
INSERT INTO `card` VALUES (3182, 'Burning Man', 'Fire Secret Agent', 4, 0x6d, 0x6d43, 6, 0, 0, 'Stealth. Play only if you control at least one <Secret> card. +2 damage.', 'Sent by Li Ting to 2068 to bring back the Syndicate''s technology.', '', 'Cristian Saksida', 5, 7, 3, 17, 'N', '| burning man fire secret agent |');
INSERT INTO `card` VALUES (3181, 'Bull Market v2', 'Event', 0, 0x616161, '', 0, 0, 0, 'Limited. Every player gains 5 Power.', 'Even without their feng shui sites, the Lodge still pull the strings of high finance.', '', 'Phil Foglio', 4, 2, 3, 17, 'N', '| bull market v2 |');
INSERT INTO `card` VALUES (3180, 'Box of Bones', 'Edge', 1, 0x65654d, '', 0, 0, 0, 'Turn when a Character is healed :: Inflict X damage on a Character. X= the amount of damage healed.', '', '', 'Alex McVey', 2, 5, 3, 17, 'N', '| box bon bone bones |');
INSERT INTO `card` VALUES (3177, 'The Blind', 'Sorcerer Assassin', 3, 0x7070, 0x704d, 2, 0, 0, 'Unique. Assassinate. +X Fighting. Immediately choose an opponent when this card enters play and at the start of each of your turns. X= the number of cards in the chosen player''s hand at that time.', '', '', 'Diego Candia', 5, 13, 1, 17, 'N', '| the blind sorcerer assassin |');
INSERT INTO `card` VALUES (3178, 'Botzilla', 'Behemoth Mechanical Lizard', 6, 0x6a6a6a6a48, 0x6a48, 10, 0, 0, 'Unique. Guts. Inflict 2 damage on any opponent''s card that turns while Botzilla is attacking.', '"Well, there goes Nichinan-cho."', '', 'Ed Cox', 5, 10, 1, 17, 'N', '| botzilla behemoth mechanical lizard |');
INSERT INTO `card` VALUES (3179, 'Bouncing the Rubble', 'Edge', 0, 0x6a6a, '', 0, 0, 0, 'Limited. Put a counter on this card when a Site''s Body is reduced to 0. Turn and remove all counters :: Inflict damage on target Site equal to the number of counters.', '', '', 'Ed Cox', 2, 10, 3, 17, 'N', '| bouncing rubble |');
INSERT INTO `card` VALUES (3176, 'Black Helicopter Squad', 'Pledged Agents', 1, '', 0x61, 1, 0, 0, '+2 damage. Cannot be declared as an attacker unless a Unique Character is also attacking.', '"I''m not authorized to tell you why we have to kill you."', '', 'Ed Cox', 5, 2, 2, 17, 'N', '| black helicopter squad pledged agent agents |');
INSERT INTO `card` VALUES (3174, 'Arctic Fortress', 'Feng Shui Site', 0, '', '', 0, 1, 8, 'Takes no damage from Characters that are the subject of States.', 'Too tough to storm, too cold to besiege.', '', 'Mike Jackson', 1, 6, 3, 17, 'N', '| arctic fortres fortress |');
INSERT INTO `card` VALUES (3175, 'Black Belt Rebels', 'Fanatic Nationalists', 3, 0x67, 0x67, 4, 0, 0, 'You may play this card from your smoked pile at -2 cost when an opponent who is closer to victory than you declares an attack on a card you control.', '', '', 'Diego Candia', 5, 8, 2, 17, 'N', '| black belt rebel rebels fanatic nationalist nationalists |');
INSERT INTO `card` VALUES (3173, 'Arcanomoth', 'Metamorphic Abomination', 2, '', 0x6648, 2, 0, 0, 'If you have [f][f][f] before you play this card, you may pay 2 additional Power while doing so to give it +4 Fighting and Toughness: 1 until it leaves play.', '', '', 'Dan Harding', 5, 1, 2, 17, 'N', '| arcano moth metamorphic abomination |');
INSERT INTO `card` VALUES (3170, 'Acupressure Master', 'Martial Artist', 4, 0x6767, 0x6743, 6, 0, 0, 'Toughness: 1. When damage inflicted on this card is reduced by Toughness, you may remove X damage, divided any way you want, from cards in play. X= the amount of damage reduced.', '', '', 'Diego Candia', 5, 8, 3, 17, 'N', '| acupressure master martial artist |');
INSERT INTO `card` VALUES (3171, 'Anubis', 'Reascended Jackal', 5, 0x6161614848, 0x6148, 7, 0, 0, 'Unique. Stealth. Gains +1 Fighting until he leaves play when any Character is smoked.', '"Death provides many answers."', '', 'Diego Candia', 5, 2, 1, 17, 'N', '| anubi anubis reascended jackal |');
INSERT INTO `card` VALUES (3172, 'Ape Shall Kill Ape', 'Event', 0, 0x6a6a, '', 0, 0, 0, 'Faceoff. Turn a Character you control and target an opponent''s Character with Fighting no lower than two less than yours. The winner may blank the text of a Site controlled by the loser until it leaves play.', '...usually with high explosives.', '', 'Alex McVey', 4, 10, 3, 17, 'N', '| ape shall kill |');

