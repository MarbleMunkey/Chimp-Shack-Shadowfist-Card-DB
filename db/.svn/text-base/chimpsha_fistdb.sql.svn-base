INSERT INTO `card` VALUES (3260, 'Project Apocalypse', 'Doomsday Site', 3, 0x6a6a6a48, 0x6a, 0, 1, 5, 'Unique. Toughness: X. Enters play with one counter. Add a counter at the start of your turn if this card is in your front row. When there are four counters, you win the game. X= the number of counters.', '', '', 'Martin Blanco', 1, 10, 1, 17, 'N', '| project apocalypse doomsday |');
INSERT INTO `card` VALUES (3258, 'Petal''s Attendant', 'Eunuch Sorcerer Assassin', 3, '', 0x654d, 3, 0, 0, 'Assassinate. You may inflict two damage on a Character when this card enters play. +1 Fighting while you have more [M] resources than each opponent.', '', '', 'Wes Jones', 5, 5, 2, 17, 'N', '| petal petals attendant eunuch sorcerer assassin |');
INSERT INTO `card` VALUES (3259, 'Probability Soldiers', 'Agents of the Uncertainty Principle', 3, 0x70, 0x70, 4, 0, 0, 'You may flip a coin when this card attacks :: Heads: unturn this card.', 'How do you kill someone who isn''t dead until he stops to check?', '', 'Wes Jones', 5, 13, 2, 17, 'N', '| probability soldier soldiers agent agents uncertainty principle |');
INSERT INTO `card` VALUES (3256, 'Palace of Virtual Light', 'Feng Shui Site', 0, '', '', 0, 1, 9, 'Unique. Turn and discard your hand :: Gain 1 Power.', 'In rare circumstances, the principles of geomancy can apply to constructs that should exist only in the imagination.', '', 'Martin Blanco', 1, 6, 1, 17, 'N', '| palace virtual light |');
