INSERT INTO `card` VALUES (3282, 'Soul of the Wolf', 'State', 1, 0x6161, '', 0, 0, 0, 'Subject Character gains Stealth and +1 Fighting.', '"We all have fangs; some of us just need to sharpen them."', '', 'Chris Quilliams', 3, 2, 2, 17, 'N', '| soul wolf |');
