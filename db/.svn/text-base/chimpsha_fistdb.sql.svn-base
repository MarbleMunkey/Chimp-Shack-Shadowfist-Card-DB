INSERT INTO `card` VALUES (1294, 'Spies Everywhere', 'Edge', 0, 0x656565, '', 0, 0, 0, 'Unique. When an opponent targets you, or targets or attacks a card you control, you may look at his or her hand.', '', '', 'Mark Evans', 2, 5, 3, 9, 'N', '| spy sp spies everywhere |');
