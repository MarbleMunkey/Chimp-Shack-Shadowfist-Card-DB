INSERT INTO `card` VALUES (3296, 'The Unspoken Name', 'Pledged Renegade', 4, 0x61616161, 0x61, 8, 0, 0, 'Unique. Unturn this card when an auction occurs.', '"The Pledged lack direction. They are a tool waiting for a hand to grasp them."', '', 'Mike Jackson', 5, 2, 1, 17, 'N', '| the unspoken name pledged renegade |');
INSERT INTO `card` VALUES (3297, 'Wailing Apparition', 'Vengeful Ghost', 1, '', 0x654d, 1, 0, 0, 'Turn and maintain and target an opponent''s Character :: Target cannot be healed.', 'Her haunting cry lets no one rest.', '', 'Chris Quilliams', 5, 5, 2, 17, 'N', '| wailing apparition vengeful ghost |');
INSERT INTO `card` VALUES (3295, 'Twelve Thousand Skulls', 'Edge', 2, 0x6565, '', 0, 0, 0, 'Unique. When an opponent''s Character is smoked, you may give a Character you control +1 Fighting until it leaves play.', '', '', 'Chris Quilliams', 2, 5, 3, 17, 'N', '| twelve thousand skull skulls |');
INSERT INTO `card` VALUES (3294, 'Turing Machine', 'Chi-Hating Silicon', 3, 0x6a, 0x6a, 4, 0, 0, 'When a Site is smoked or burned, this card gains +1 Fighting until it leaves play.', 'Potemkin found a use for the crude, angry AIs they''d stolen.', '', 'Dan Murray', 5, 10, 2, 17, 'N', '| turing machine chi hating silicon |');
INSERT INTO `card` VALUES (3293, 'Turbo Boost', 'Event', 0, 0x6a48, '', 0, 0, 0, 'Limited. Until the end of the turn, target Character gains Mobility and +2 damage.', '"What do you mean there''s no off switch?"', '', 'Dan Murray', 4, 10, 2, 17, 'N', '| turbo boost |');
INSERT INTO `card` VALUES (3292, 'True Believer', 'Indoctrinated Pawn', 1, '', 0x704d, 1, 0, 0, 'When this card leaves play, you may give target Character -1 damage until the end of the turn.', 'To become a Purist, one must understand that madness is just the price of entry.', '', 'Chris Quilliams', 5, 13, 2, 17, 'N', '| true believer indoctrinated pawn |');
