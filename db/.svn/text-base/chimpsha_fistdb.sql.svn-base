INSERT INTO `card` VALUES (3290, 'Three Days to Retirement', 'Cop State', 1, 0x64, '', 0, 0, 0, 'Limited. Schtick. All effects that could legally target subject Character must target it if they target any Character you control.', '"Just this last case, and I''m done."', '', 'Brian England', 3, 4, 3, 17, 'N', '| three day days retirement cop |');
INSERT INTO `card` VALUES (3289, 'Temporal Anomaly', 'Event', 1, 0x70704d, '', 0, 0, 0, 'Choose an Edge in your smoked pile or pay the cost of an Edge in an opponent''s smoked pile :: Return that Edge to play under your control.', 'Sometimes, everything happens at once anyway.', '', 'Kevin Wasden', 4, 13, 3, 17, 'N', '| temporal anomaly |');
