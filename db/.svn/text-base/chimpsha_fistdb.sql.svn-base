INSERT INTO `card` VALUES (1038, 'Peasant Mob', 'Angry Rabble', 3, 0x6767, 0x67, 4, 0, 0, 'Inflicts +2 damage on any Site with a printed Body of 7 or more and -2 damage on any Site with a printed Body of 5 or less, even if the Site is unrevealed.', '', '', 'Stephen Snyder', 5, 8, 2, 7, 'N', '| peasant mob angry rabble |');
