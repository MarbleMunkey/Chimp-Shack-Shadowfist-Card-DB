INSERT INTO `card` VALUES (1502, 'Bouncing Benji', 'Mass-Produced Abomination', 1, 0x6666, 0x66, 1, 0, 0, '+1 damage. Reload: discard a card.', '"Kill all you want. We''ll make more."', '', 'Glen Osterberger', 5, 1, 2, 12, 'N', '| bouncing benji mas mass produced abomination |');
INSERT INTO `card` VALUES (1503, 'Brass Monkey', 'Simian Automaton', 4, 0x6a6a4848, 0x6a48, 6, 0, 0, 'Opponents cannot intercept Brass Monkey unless they pay one Power for each interceptor they declare against him. Reload: 2', '', '', 'Dave Allsop', 5, 10, 3, 12, 'N', '| bras brass monkey simian automaton |');
INSERT INTO `card` VALUES (1504, 'Bribery', 'Event', 0, 0x6565, '', 0, 0, 0, 'Give an opponent 1 Power and target a Character he or she controls with a printed numerical Fighting of 5 or less :: Take control of target until the end of the turn.', 'Money talks.', '', 'Patrick McEvoy', 4, 5, 2, 12, 'N', '| bribery |');
INSERT INTO `card` VALUES (1505, '"Bring It"', 'State', 1, 0x6467, '', 0, 0, 0, 'Unique. Schtick. Turn to redirect all damage from a single source to subject Character. Sacrifice to give subject Guts until the end of the turn. Reload: 1', '"Who''s first?"', '', 'Alexander Leonard', 3, 4, 1, 12, 'N', '| bring it |');
INSERT INTO `card` VALUES (1506, 'Broken Wheel Brigade', 'Netherworld Soldiers', 2, '', 0x61, 2, 0, 0, 'Discard an Event, State, or Edge to reduce damage inflicted on this card from a single source by 1.', '', '', 'Roberto Campus', 5, 2, 2, 12, 'N', '| broken wheel brigade netherworld soldier soldiers |');
INSERT INTO `card` VALUES (1507, 'Buddhist Monk', 'Martial Artist', 2, '', 0x6743, 2, 0, 0, '+X Fighting. X = the number of Power-generating Sites you control minus two. X cannot be less than 0.', '', '', 'Patrick McEvoy', 5, 8, 2, 12, 'N', '| buddhist monk martial artist |');
INSERT INTO `card` VALUES (1508, 'BuroMil Scout', 'Recon Soldier', 1, 0x66, 0x66, 1, 0, 0, 'While BuroMil Scout is attacking, other attackers inflict +1 damage.', '', '', 'Patrick McEvoy', 5, 1, 2, 12, 'N', '| buro mil scout recon soldier |');
INSERT INTO `card` VALUES (1509, 'Butterfly Armor', 'Thunder State', 1, 0x6d6d6d, '', 0, 0, 0, 'If subject Character is unturned, it may attack without turning once during your Main Shot. If subject is unturned at the beginning of any player''s turn, it takes 2 damage, which cannot be reduced or redirected.', '', '', 'Heather Bruton', 3, 7, 3, 12, 'N', '| butterfly armor thunder |');
INSERT INTO `card` VALUES (1510, 'Cassandra', 'Netherworld Seer', 2, '', '', 2, 0, 0, 'Unique. Turn to look at the top 5 cards of your deck. You may put as many of these cards as you wish on the bottom of your deck in any order, and return the rest to the top of your deck in any order.', '', '', 'Lissanne Lake', 5, 12, 3, 12, 'N', '| cassandra netherworld seer |');
INSERT INTO `card` VALUES (1511, 'Chamberlain', 'Skin and Darkness Zealot', 5, 0x6d6d4d, 0x6d, 7, 0, 0, 'Unique. Assassinate. Heals when a Unique Character is smoked. Once during your Main Shot, if Chamberlain is unturned, he may attack a Character with a Fighting of 5 or higher without turning.', '', '', 'Roberto Campus', 5, 7, 1, 12, 'N', '| chamberlain skin darkness darknes darkne darkn zealot |');
INSERT INTO `card` VALUES (1512, 'Chi Detachment', 'State', 1, 0x4d4d4d, '', 0, 0, 0, 'Play on a Character during any player''s Main Shot. Subject provides no resources and its rules text is considered blank.', '"Your kung fu is strong. Let''s see how you do without it...."', '', 'Patrick McEvoy', 3, 11, 2, 12, 'N', '| chi detachment |');
INSERT INTO `card` VALUES (1513, 'Chizu', 'State', 1, 0x6a, '', 0, 0, 0, 'Play on a <Netherworld> Site you control. All cards that are [C] or [M] have blank rules text while at this location. Reload: 1', 'You wouldn''t think property values in the Netherworld could get worse....', '', 'Patrick McEvoy', 3, 10, 3, 12, 'N', '| chizu |');
INSERT INTO `card` VALUES (1514, 'Contest of Arms', 'Event', 1, 0x6d, '', 0, 0, 0, 'Toast It. Faceoff. Turn a Character you control and target an opponent''s Character with Fighting no lower than two less than yours. The winner may return an Event from his or her smoked pile to hand.', 'To the victor go the spoils!', '', 'Chris Chuckry', 4, 7, 3, 12, 'N', '| contest arm arms |');
INSERT INTO `card` VALUES (1515, 'Corrupt Bookie', 'Pledged Hood Gambler', 2, 0x61, 0x61, 2, 0, 0, 'When a Faceoff effect is generated, gain one Power and predict who will win. If your prediction is correct, gain one Power. If incorrect, or if there is no winner, smoke this card.', '', '', 'Mike Jackson', 5, 2, 3, 12, 'N', '| corrupt bookie pledged hood gambler |');
INSERT INTO `card` VALUES (1516, 'Dark Traveler', 'Netherworld Hero', 2, '', '', 2, 0, 0, '+2 Fighting for each Feng Shui Site in your smoked pile.', '', '', 'Anson Maddocks', 5, 12, 2, 12, 'N', '| dark traveler netherworld hero |');
INSERT INTO `card` VALUES (1517, 'Demonic Plague', 'Event', 0, 0x6565, '', 0, 0, 0, 'Limited. Inflict one damage on target Character. During your turn, you may return exactly one "Demonic Plague" to your hand when a Character you control is smoked by an opponent.', '', '', 'Randy Asplund', 4, 5, 2, 12, 'N', '| demonic plague |');
INSERT INTO `card` VALUES (1518, 'Diamond Beach', 'Netherworld Feng Shui Site', 0, '', '', 0, 1, 5, 'Toughness: 2', 'If you have attuned to enough Feng Shui sites in your life, you can shape anything you want - but only within the Netherworld.', '', 'Randy Asplund', 1, 6, 2, 12, 'N', '| diamond beach netherworld |');
INSERT INTO `card` VALUES (1519, 'Dragon Graveyard', 'Netherworld Feng Shui Site', 0, '', '', 0, 1, 8, 'Gain 1 Power when 3 or more combat damage is inflicted on this card. If this card is in your hand when an opponent''s effect attempts to look at your hand or force you to discard, you may discard it to cancel that effect and gain 2 Power.', '', '', 'Cristian Saksida', 1, 6, 2, 12, 'N', '| dragon graveyard netherworld |');
INSERT INTO `card` VALUES (1520, 'Dr. Jean-Marc Ngubane', 'Buro Scientist Double Agent', 3, 0x6666, 0x666d4d48, 4, 0, 0, 'Unique. Turn to unturn target [f] Character or turn target [m] Character.', 'Sent by Boatman as a spy and an assassin.', '', 'Patrick McEvoy', 5, 1, 1, 12, 'N', '| dr jean marc ngubane buro scientist double agent |');
INSERT INTO `card` VALUES (1521, 'Dr. Timbul Damiri', 'Fugue Sorcerer', 2, 0x7070, 0x704d, 1, 0, 0, 'Unique. Turn this card and choose another Character :: Sacrifice that Character. If the sacrifice is successful, return the Character to play under its owner''s control at the same location. If it was turned, it enters play turned.', '', '', 'Matt Bober', 5, 13, 3, 12, 'N', '| dr timbul damiri fugue sorcerer |');
INSERT INTO `card` VALUES (1522, 'Echo Cancellation', 'Event', 1, 0x70, '', 0, 0, 0, 'Limited.  Play when an opponent plays a non-Event, non-foundation card with the same title as a card he or she controls or has in his or her smoked pile :: Cancel and smoke the played card.', '', '', 'Cristian Saksida', 4, 13, 3, 12, 'N', '| echo cancellation |');
INSERT INTO `card` VALUES (1523, 'Escher Hotel', 'Netherworld Feng Shui Site', 0, '', '', 0, 1, 8, 'Unique. All Sites you control are considered to be in the front row. (Back-row Sites are also still considered to be back-row Sites.) This does not allow your back-row Sites other than Escher Hotel to be attacked.', '', '', 'Randy Asplund', 1, 6, 1, 12, 'N', '| escher hotel netherworld |');
INSERT INTO `card` VALUES (1524, 'Evil Whispers', 'Event', 0, 0x65654d, '', 0, 0, 0, 'Limited.  Play when an opponent spends 2 or more Power at one time :: Gain 1 Power.', '', '', 'Ravindra Rana', 4, 5, 2, 12, 'N', '| evil whisper whispers |');
INSERT INTO `card` VALUES (1525, 'Exile Village', 'Battleground Site', 1, '', '', 0, 0, 6, 'Any player who seizes this Site gains 1 Power. If this card is in your front row, it generates one Power during your Establishing Shot if you have more different faction resources in your pool than any other player.', '', '', 'Brian Rood', 1, 12, 3, 12, 'N', '| exile village battleground |');
