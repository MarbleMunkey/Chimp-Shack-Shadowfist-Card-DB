INSERT INTO `card` VALUES (1631, 'Cenotaph', 'Undead Assassin', 3, 0x65654d, 0x65, 100, 0, 0, 'Unique. Assassinate. X= the number of Characters in your smoked pile.', '"There is no innocent blood. Only blood."', '', 'Roberto Campus', 5, 5, 1, 13, 'N', '| cenotaph undead assassin |');
INSERT INTO `card` VALUES (1632, 'Chi Syphon', 'State', 1, 0x7070, '', 0, 0, 0, 'Play on a Feng Shui Site you control and choose an opponent''s Feng Shui Site. Subject''s rules text is considered to be blank, and it gains the rules text of the other Site.', '"Your good fortune is now ours."', '', 'Mark Evans', 3, 13, 3, 13, 'N', '| chi syphon |');
INSERT INTO `card` VALUES (1633, 'Cobra Clan Stalkers', 'Transformed Animals', 3, 0x6161, 0x61, 3, 0, 0, 'Stealth. +2 damage while you have Power.', 'Kong Jun She tricked the transformed animals into declaring war on the Seven Masters.', '', 'Joachim Barrum', 5, 2, 2, 13, 'N', '| cobra clan stalker stalkers transformed animal animals |');
INSERT INTO `card` VALUES (1634, 'Combat Courtship', 'Event', 1, 0x6d, '', 0, 0, 0, 'Faceoff. Turn a Character you control and target an opponent''s Character with Fighting no lower than two less than yours. The winner gains Power equal to the number of resources provided by the loser''s Character.', 'Love hurts.', '', 'Brian Rood', 4, 7, 3, 13, 'N', '| combat courtship |');
INSERT INTO `card` VALUES (1635, 'Cry of the Forgotten Ancestor', 'Event', 1, 0x6161, '', 0, 0, 0, 'Cancel and smoke target [M] card.', 'The Lodge was founded in the 11th century with the express purpose of driving magic from the world.', '', 'Mark Poole', 4, 2, 3, 13, 'N', '| cry forgotten ancestor |');
INSERT INTO `card` VALUES (1636, 'Curtain of Fullness', 'Event', 0, 0x4d4d, '', 0, 0, 0, 'Target player must discard three cards at random.', 'The stars hold a thousand might-have-beens, none to be beheld by mortal eyes.', '', 'Nicola Leonard', 4, 11, 2, 13, 'N', '| curtain fullnes fullness |');
INSERT INTO `card` VALUES (1637, 'Deep-Cover Rebels', 'Subversive Operatives', 5, 0x6a6a, 0x6a, 8, 0, 0, 'You may attack back-row Sites. You may return a Character with a cost of 2 or less from your smoked pile to play when this card is smoked.', '', '', 'Cristian Saksida', 5, 10, 3, 13, 'N', '| deep cover rebel rebels subversive operativ operative operatives |');
INSERT INTO `card` VALUES (1638, 'Desolation', 'Underworld Demon King', 7, 0x65656565, 0x654d4d, 16, 0, 0, 'Unique. Uncopyable. Opponents cannot take control of Desolation. Any Event targeting this card costs 1 extra Power to play.', 'Eruption of the Underworld; the heavens weep.', '', 'Mikael Noguchi', 5, 5, 1, 13, 'N', '| desolation underworld demon king |');
INSERT INTO `card` VALUES (1639, 'Eight Pillars of Heaven Array', 'Event', 100, 0x676743, '', 0, 0, 0, 'Limited.  Target X Characters you control that share a designator. Each Character gains +2 damage and Toughness: 1 until the end of the turn. X must be at least 2.', '', '', 'Michael Komarck', 4, 8, 3, 13, 'N', '| eight pillar pillars heaven array |');
INSERT INTO `card` VALUES (1640, 'Essence-Absorbing Stance', 'Kung Fu State', 1, 0x6767, '', 0, 0, 0, 'Schtick. Turn to heal subject Character and inflict X damage to target Character that is about to enter combat with subject. X= the amount of damage healed.', '', '', 'Joachim Barrum', 3, 8, 3, 13, 'N', '| essence absorbing stance kung fu |');
INSERT INTO `card` VALUES (1641, 'Evil Chanting', 'Event', 1, 0x654d4d, '', 0, 0, 0, 'Either inflict 4 damage on target [s] Character, or all [e] Characters gain +2 damage until the end of the turn. Reload: Discard a non-Character [M] card.', '"Oh no! Evil chanting!"', '', 'Borja Peaea Marcano', 4, 5, 2, 13, 'N', '| evil chanting |');
INSERT INTO `card` VALUES (1642, 'Feast of Souls', 'Edge', 4, 0x65, '', 0, 0, 0, 'Gain 1 Power when a Character you control is smoked. When this card leaves play, the player to your left steals all your Power.', 'The Lords of the Underworld demand tribute.', '', 'Susan Van Camp', 2, 5, 1, 13, 'N', '| feast soul souls |');
