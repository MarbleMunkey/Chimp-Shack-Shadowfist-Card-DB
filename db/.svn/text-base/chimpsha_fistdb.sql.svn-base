INSERT INTO `card` VALUES (3283, 'Squadron-Leader Holz', 'Retired Soldier', 4, 0x6666, 0x66, 6, 0, 0, 'Unique. Takes no damage from cards that provide more than 1 resource.', 'When presented with no options, the best soldiers make their own.', '', 'Mike Jackson', 5, 1, 1, 17, 'N', '| squadron leader holz retired soldier |');
