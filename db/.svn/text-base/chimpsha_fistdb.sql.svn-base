INSERT INTO `card` VALUES (1297, 'Street Fighter', 'Martial Artist', 3, 0x6464, 0x64, 3, 0, 0, 'Guts. Target another Character when Street Fighter enters play. That Character gains +2 Fighting while Street Fighter is in play.', 'No flashy moves; just deadly force.', '', 'Thomas Gianni', 5, 4, 3, 9, 'N', '| street fighter martial artist |');
