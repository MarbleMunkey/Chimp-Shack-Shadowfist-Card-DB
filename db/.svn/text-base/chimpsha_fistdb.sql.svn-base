INSERT INTO `card` VALUES (1477, 'Virtuous Hood', 'Martial Artist', 4, 0x6767, 0x67, 6, 0, 0, 'When Virtuous Hood turns to attack, take a point of Power from the pool of one of the players with the most Power and give it to one of the players with the least Power.', '', '', 'Edward Beard, Jr.', 5, 8, 5, 11, 'N', '| virtuou virtuous hood martial artist |');
