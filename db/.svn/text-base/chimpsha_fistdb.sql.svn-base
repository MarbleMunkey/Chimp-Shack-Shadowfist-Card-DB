INSERT INTO `card` VALUES (1335, 'Brain Fire', 'Event', 0, 0x6d6d, '', 0, 0, 0, 'Play in response to an Event. Choose an equal number of new and different legal targets for that Event :: These targets replace the original targets of that Event.', '', '', 'Nicola Leonard', 4, 7, 5, 11, 'N', '| brain fire |');
