INSERT INTO `card` VALUES (1489, 'Ashes of the Fallen', 'Event', 0, 0x646464, '', 0, 0, 0, 'Toast a non-Unique Character in your smoked pile to return a [d] Event to your hand.', 'The lessons of defeat are the harshest.', '', 'Matt Bober', 4, 4, 2, 12, 'N', '| ash ashe ashes fallen |');
INSERT INTO `card` VALUES (1490, 'Assassin Bug', 'Killer Abomination', 1, 0x6648, 0x66, 1, 0, 0, 'If Assassin Bug damages a Unique Character, smoke that Character.', 'When Boatman wants somebody gone, he sends these.', '', 'Lissanne Lake', 5, 1, 3, 12, 'N', '| assassin bug killer abomination |');
INSERT INTO `card` VALUES (1491, 'Battle Arena', 'Battleground Site', 1, '', '', 0, 0, 6, 'Any player who seizes this Site gains 1 Power. While this Site is in your front row, all Characters you control gain both Toughness: 1 and +1 damage while in a Faceoff.', '', '', 'Alexander Leonard', 1, 12, 3, 12, 'N', '| battle arena battleground |');
INSERT INTO `card` VALUES (1492, 'Bei Tairong', 'Lodge Traitor', 3, 0x6464, 0x6461, 4, 0, 0, 'Unique. Superleap. +1 damage to cards controlled by a player with [a] in his or her pool.', 'She couldn''t take what the Lodge had become.', '', 'Melissa Benson', 5, 4, 1, 12, 'N', '| bei tairong lodge traitor |');
