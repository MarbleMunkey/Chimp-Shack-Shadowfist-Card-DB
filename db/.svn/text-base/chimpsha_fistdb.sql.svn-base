INSERT INTO `card` VALUES (2372, 'Math Bomb', 'Event', 2, 0x70704d, '', 0, 0, 0, 'Smoke all Characters whose Fighting is odd.', 'The equation does not describe the explosive, it is the explosive.', '', 'Mike Jackson', 4, 13, 3, 15, 'N', '| math bomb |');
INSERT INTO `card` VALUES (2371, 'Mars Colonist', 'Expendable Student', 1, '', 0x7943, 1, 0, 0, '', 'The Syndicate now control the future, but the Secret War burns hotter than ever throughout history. Look for the Critical Shift set in 2007, and check www.shadowfist.com for all your Shadowfist needs.', '', 'Allan Bednar', 5, 15, 6, 15, 'N', '| mar mars colonist expendable student |');
INSERT INTO `card` VALUES (2370, 'Mah-Jongg Parlor', 'Feng Shui Site', 0, '', '', 0, 1, 7, 'Limited. Turn and pay one Power :: Put a counter on this card. Turn and remove all counters from this card :: Gain Power equal to twice the number of counters removed.', 'Never bet against the house.', '', 'Dan Frazier', 1, 6, 3, 15, 'N', '| mah jongg parlor |');
INSERT INTO `card` VALUES (2368, 'Liquored Up', 'Event', 0, 0x6464, '', 0, 0, 0, 'Target Character gains Guts until the end of the turn.', '"You made me spill my drink!"', '', 'Mike Jackson', 4, 4, 2, 15, 'N', '| liquored up |');
INSERT INTO `card` VALUES (2369, 'Living Legend', 'State', 2, '', '', 0, 0, 0, 'Unique. Subject gains +2 Fighting, cannot be taken control of, and its damage cannot be reduced or redirected.', '"Who''s next?"', '', 'Mike Kimble', 3, 12, 6, 15, 'N', '| living legend |');
INSERT INTO `card` VALUES (2367, 'Lateral Reincarnation', 'Event', 1, '', '', 0, 0, 0, 'Unique. You must have at least 3 resources of the same type to play this card. Sacrifice a Character and choose a non-Unique Character in an opponent''s smoked pile with the same cost or less :: Return the chosen Character to play under your control.', '', '', 'Allan Bednar', 4, 12, 3, 15, 'N', '| lateral reincarnation |');
INSERT INTO `card` VALUES (2365, 'Just a Scratch', 'Event', 0, 0x6464, '', 0, 0, 0, 'Heal target Character that has at least 3 damage.', '"Don''t think this''ll stop me from putting you down like the mad dog you are."', '', 'Matt Bober', 4, 4, 2, 15, 'N', '| just scratch |');
INSERT INTO `card` VALUES (2366, 'Kunlun Clan Assault', 'Event', 2, '', 0x73434d, 0, 0, 0, 'Inflict 3 damage, divided any way you choose, on target Characters and/or Sites.', 'The Kunlun were legendary for their prowess in battle.', '', 'Chris Watkins', 4, 14, 6, 15, 'N', '| kunlun clan assault |');
INSERT INTO `card` VALUES (2363, 'Johann Bonengel', 'NeoBuroPresident', 3, 0x666666, 0x6666, 5, 0, 0, 'Unique. When you play a 1-cost [f] Character from your hand, and not due to this card, you may immediately play a second 1-cost [f] Character from your hand or smoked pile at no cost.', '', '', 'Mike Jackson', 5, 1, 1, 15, 'N', '| johann bonengel neoburo president |');
INSERT INTO `card` VALUES (2364, 'Joshua Norton', 'Emperor of the USA', 3, 0x707070, 0x70614d, 3, 0, 0, 'Unique. Your Characters cannot be attacked while he is unturned. Turn during your Main Shot :: Look at target opponent''s hand. Gain 2 Power if it contains at least three cards with different costs that aren''t Feng Shui Sites.', '', 'Tag (unprinted): "Before he went mad, he was what he later claimed to be."', 'Mike Kimble', 5, 13, 1, 15, 'N', '| joshua norton emperor usa |');
INSERT INTO `card` VALUES (2361, 'Jessica Ng', 'CDCA Scientist', 3, 0x66, 0x6648, 4, 0, 0, 'Unique. Tactics. In addition to inflicting normal combat damage on a Site, this card may also inflict half as much combat damage (round up) on another Site to the immediate left, right, or behind that Site.', '', '', 'Oliver Specht', 5, 1, 6, 15, 'N', '| jessica ng cdca scientist |');
INSERT INTO `card` VALUES (2362, 'Jia Baoyu', 'Lovelorn Shaolin Agent', 2, 0x6767, 0x6743, 2, 0, 0, 'Unique. Superleap. When he turns to attack, draw three cards if he is the only attacker.', '"I will never forget her."', '', 'Dan Murray', 5, 8, 3, 15, 'N', '| jia baoyu lovelorn shaolin agent |');
INSERT INTO `card` VALUES (2360, 'Jessica Ng', 'Corporate Mastermind', 3, 0x797979, 0x79, 4, 0, 0, 'Unique. All opponents play with the top card of their decks face-up. You may play these cards at +1 cost, ignoring resource conditions.', 'She looked into Jia''s eyes with no hint of recognition.', '', 'Olilver Specht', 5, 15, 1, 15, 'N', '| jessica ng corporate mastermind |');
INSERT INTO `card` VALUES (2359, 'The Jackson Gang', 'Heroic Outlaws', 3, 0x646464, 0x64, 5, 0, 0, 'Unique. When this card damages the target of its attack, steal 1 Power from the target''s controller.', 'Out of money, out of time, and out of luck.', '', 'Jonny Scull', 5, 4, 3, 15, 'N', '| the jackson gang heroic outlaw outlaws |');
INSERT INTO `card` VALUES (2358, 'Isothermal Zodiac', 'Weird Science Event', 0, 0x6a, '', 0, 0, 0, 'Choose a player who controls a copy of the first card in alphabetical order by title, ignoring everything but letters :: That player gains 1 Power.', 'Proponents claimed that as more people moved west, the territories would produce more water.', '', 'Matt Bober', 4, 10, 2, 15, 'N', '| isothermal zodiac weird science |');
INSERT INTO `card` VALUES (2357, 'Involuntary Embalming', 'Event', 1, 0x65654d, '', 0, 0, 0, 'Target a Character and toast X cards from your smoked pile at random :: Target gains +X Fighting until it leaves play. X= half (round up) the number of toasted cards.', 'There is no act so horrifying that the Lotus will not commit it.', '', 'Ed Cox', 4, 5, 3, 15, 'N', '| involuntary embalming |');
INSERT INTO `card` VALUES (2355, 'Inconvenient Debt', 'Event', 1, 0x79, '', 0, 0, 0, 'Play when an attack is declared. Target an opponent''s unturned Character that could legally attack the target of the attack. That Character cannot turn in response :: Target turns and joins the attack.', '', '', 'Mike Raabe', 4, 15, 3, 15, 'N', '| inconvenient debt |');
INSERT INTO `card` VALUES (2356, 'The Insidious Dr. Fermat', 'Mad Mathemagician Mastermind', 4, 0x7070, 0x704d4d, 6, 0, 0, 'Unique. Independent. When he enters combat, immediately cancel all Characters he is in combat with and States they are the subject of until the end of combat.', '', '', 'Mike Jackson', 5, 13, 1, 15, 'N', '| the insidiou insidious dr fermat mad mathemagician mastermind |');
INSERT INTO `card` VALUES (2353, 'Hydrophobia', 'State', 1, 0x61, '', 0, 0, 0, 'Choose a designator when this card enters play. Subject Character cannot attack or intercept cards with that designator, and cannot change location to that of a card with that designator. (Attacking is not considered to be changing location.)', '', '', 'Flavio Augusto', 3, 2, 2, 15, 'N', '| hydrophobia |');
INSERT INTO `card` VALUES (2354, 'Hyper Alloy Blade', 'State', 1, 0x7948, '', 0, 0, 0, 'Weapon. Subject Character inflicts +2 damage and cannot have its damage reduced or redirected.', 'The alloys that can be manufactured in zero-g will hold an edge a molecule thin.', '', 'Cristian Saksida', 3, 15, 2, 15, 'N', '| hyper alloy blade |');
INSERT INTO `card` VALUES (2352, 'Huichen Kan', 'Dust Demon', 4, 0x65654d, 0x65, 6, 0, 0, 'Stealth. While this card is attacking, Characters controlled by its target''s controller cannot change location.', 'What good are bullets against a storm?', '', 'Matthew Laznicka', 5, 5, 3, 15, 'N', '| huichen kan dust demon |');
INSERT INTO `card` VALUES (2351, 'Horse Thief', 'Lodge Operator', 3, 0x6161, 0x61, 4, 0, 0, 'Stealth. When an opponent plays an Event during your turn, gain Power equal to the Power he or she spent.', 'Subtle, efficient, gone before dawn.', '', 'Flavio Augusto', 5, 2, 3, 15, 'N', '| horse thief lodge operator |');
INSERT INTO `card` VALUES (2350, 'Hoosegow Jackson', 'Desperate Outlaw', 0, 0x646464, 0x64, 4, 0, 0, 'Unique. Guts. When you play this card, immediately start an auction by bidding exactly one Power. The winning bidder is considered to have played this card.', '', '', 'Matthew Laznicka', 5, 4, 1, 15, 'N', '| hoosegow jackson desperate outlaw |');
INSERT INTO `card` VALUES (2349, 'The Honorable Earl Mason', 'Hangin'' Judge', 2, 0x6161, 0x61, 2, 0, 0, 'Unique. Turn, target a Character, and pay that Character''s cost :: Smoke target.', '"The law ''round here is whatever I say it is."', '', 'Mike Jackson', 5, 2, 1, 15, 'N', '| the honorable earl mason hangin judge |');
INSERT INTO `card` VALUES (2348, 'High Noon', 'Event', 1, 0x6767, '', 0, 0, 0, 'Limited. Faceoff. Turn a Character you control and target an opponent''s Character with Fighting no lower than two less than yours. Put the loser''s Character in the winner''s burned-for-victory pile.', 'Eventually, it must come down to this.', '', 'Matt Bober', 4, 8, 3, 15, 'N', '| high noon |');
INSERT INTO `card` VALUES (2347, 'Hideout', 'Site', 0, '', '', 0, 0, 4, 'Attacks cannot be declared against Characters at this location unless you control no other legal targets.', '"Yeah, we''re surrounded all right. Now, you gonna call or fold?"', '', 'Scott Reeves', 1, 12, 3, 15, 'N', '| hideout |');
INSERT INTO `card` VALUES (2346, 'Gunslinger', 'Pledged Assassin', 3, 0x61, 0x61, 3, 0, 0, 'Turn to enter a Faceoff with target Character. The winner gains 1 Power.', '"By the time you draw, you''ll already have six bullets in you."', '', 'Cristian Saksida', 5, 2, 2, 15, 'N', '| gun guns linger pledged assassin |');
INSERT INTO `card` VALUES (2345, 'Gunboat Diplomacy', 'Edge', 1, 0x616161, '', 0, 0, 0, 'Limited. When an opponent turns a non-Character card, you may inflict 1 damage on any card he or she controls.', '', '', 'Bryant Griffin', 2, 2, 3, 15, 'N', '| gun boat diplomacy |');
INSERT INTO `card` VALUES (2344, 'The Golden Spike', 'Railroad Event', 1, '', '', 0, 0, 0, 'One-Shot. Play if you have at least three columns in your Site structure. You may immediately play a card from your smoked pile at no cost, regardless of type.', 'When it was hammered in, it created the world''s longest feng shui site.', '', 'Flavio Augusto', 4, 12, 1, 15, 'N', '| the golden spike railroad |');
INSERT INTO `card` VALUES (2343, 'Gamma Beast', 'Third-Generation Abomination', 3, '', 0x66, 2, 0, 0, '+1 Fighting for each <Abomination> in play.', 'At Dao Biotech, Boatman started recreating arcanoscience.', '', 'Chris Quilliams', 5, 1, 2, 15, 'N', '| gamma beast third generation abomination |');
INSERT INTO `card` VALUES (2342, 'Gambler', 'Lucky Bastard', 3, 0x64, 0x64, 3, 0, 0, 'Twice a turn, you may flip a coin :: Heads: this card gains +2 Fighting until the end of the turn. Tails: it takes 1 damage.', 'Lucky to be alive.', '', 'Matt Bober', 5, 4, 2, 15, 'N', '| gambler lucky bastard |');
INSERT INTO `card` VALUES (2341, 'Frenzy of the Shark', 'Lodge Killers', 4, 0x6161, 0x61, 6, 0, 0, 'Toughness: 1 against damaged Characters. +X Fighting. X= the number of damaged Characters in play.', '', '', 'Flavio Augusto', 5, 2, 3, 15, 'N', '| frenzy shark lodge killer killers |');
INSERT INTO `card` VALUES (2340, 'Flesh Eater', 'Bloodthirsty Demon', 3, 0x65, 0x65, 4, 0, 0, 'Regenerate. When this card reduces a Site''s Body to 0, you may return a card from your smoked pile to your hand.', '"Nobody goes out alone after dark anymore."', '', 'Quinton Hoover', 5, 5, 2, 15, 'N', '| flesh eater bloodthirsty demon |');
INSERT INTO `card` VALUES (2339, 'Fistful of Dollars', 'Edge', 1, 0x6161, '', 0, 0, 0, 'At the beginning of each opponent''s Main Shot, he or she may place a counter on this card to gain 1 Power. Turn and remove X counters :: Gain X Power.', '', '', 'Cristian Saksida', 2, 2, 3, 15, 'N', '| fistful dollar dollars |');
INSERT INTO `card` VALUES (2338, 'Fire Woman', 'Crazed Pyromaniac', 1, 0x6a, 0x6a, 1, 0, 0, '+1 Fighting for each damaged Feng Shui Site you control.', 'Pyromania is a cruel and hungry mistress.', '', 'Matt Bober', 5, 10, 2, 15, 'N', '| fire woman crazed pyromaniac |');
INSERT INTO `card` VALUES (2337, 'Fire Ants', 'Deadly Insects', 1, '', 0x6d, 1, 0, 0, 'No Max. +X damage to Characters. X= the number of other attacking "Fire Ants".', 'March. Scorch. Feed.', '', 'Matt Bober', 5, 7, 2, 15, 'Y', '| fire ant ants deadly insect insects |');
INSERT INTO `card` VALUES (2335, 'Fermat''s Last Stand', 'Event', 1, 0x7070, '', 0, 0, 0, 'All Characters you control gain +1 Fighting until they leave play, then take 1 damage.', '"Under extreme duress, our deepest truths are revealed."', '', 'Mike Jackson', 4, 13, 3, 15, 'N', '| fermat fermats last stand |');
INSERT INTO `card` VALUES (2336, 'Fermat''s Soldiers', 'Secret Shock Troops', 4, 0x7070, 0x70, 6, 0, 0, 'Ambush against Characters controlled by players who control more cards than you.', '"With a few loyal men to carry out my brilliant plans, I will be unstoppable!"', '', 'Mike Jackson', 5, 13, 3, 15, 'N', '| fermat fermats soldier soldiers secret shock troop troops |');
INSERT INTO `card` VALUES (2334, 'Fastest Gun in the West', 'State', 2, 0x6464, '', 0, 0, 0, 'Unique. Subject Character gains Ambush.', '"Walk away, kid. Just walk away."', '', 'Michael Grills', 3, 4, 3, 15, 'N', '| fastest gun  west |');
INSERT INTO `card` VALUES (2333, 'Exiled Monk', 'Shaolin Outcast', 3, 0x67, 0x6743, 4, 0, 0, 'Heal this card at the end of each attack.', '"I have been away for a long time, but I still remember what I learned."', '', 'Matthew Laznicka', 5, 8, 2, 15, 'N', '| exiled monk shaolin outcast |');
INSERT INTO `card` VALUES (2331, 'Elephant Gun', 'State', 2, '', '', 0, 0, 0, 'Weapon. Subject Character inflicts +5 damage.', '"Even Katie was impressed."', '', 'Chris Chuckry', 3, 12, 6, 15, 'N', '| elephant gun  |');
INSERT INTO `card` VALUES (2332, 'Evacuation: 2066', 'Event', 0, 0x6666, '', 0, 0, 0, 'Sacrifice X Characters :: Unturn X Characters.', '"You misunderstand, doctor. You are Plan B."', '', 'Flavio Augusto', 4, 1, 3, 15, 'N', '| evacuation: 2066 |');
INSERT INTO `card` VALUES (2330, 'Echo and Silence', 'Twin Assassins', 4, 0x797948, 0x7943, 5, 0, 0, 'Unique. Ambush. If they inflict combat damage during the Ambush period, they also inflict combat damage at the normal time.', '', '', 'Cristian Saksida', 5, 15, 1, 15, 'N', '| echo silence twin assassin assassins |');
INSERT INTO `card` VALUES (2329, 'Dust Storm', 'Event', 1, 0x65, '', 0, 0, 0, 'Play during an attack :: The first interceptor currently assigned to each attacker ceases intercepting.', '"There ain''t much to see out there at the best of times."', '', 'Chris Quilliams', 4, 5, 2, 15, 'N', '| dust storm |');
INSERT INTO `card` VALUES (2328, 'Dr. Quentin Higginsbotham', 'Weirder Scientist', 1, 0x6a6a, 0x6a48, 1, 0, 0, 'Unique. Turn :: Reveal all front-row Sites and rearrange each player''s front-row Sites. This may change the target of an attack.', '', '', 'Ed Cox', 5, 10, 1, 15, 'N', '| dr quentin higginsbotham weirder scientist |');
INSERT INTO `card` VALUES (2327, 'Dr. Curtis Boatman', 'Corporate Scientist Mastermind', 3, 0x66666666, 0x66484d, 4, 0, 0, 'Unique. Opponents must pay you 1 Power to declare attackers against your cards. All <Arcano> and <Abomination> Characters you control gain +1 Fighting.', '"It''s not the world, but it will do."', '', 'Oliver Specht', 5, 1, 1, 15, 'N', '| dr curti curtis boatman corporate scientist mastermind |');
INSERT INTO `card` VALUES (2326, 'Donner Lake', 'Feng Shui Site', 0, '', '', 0, 1, 6, 'Unique. At the beginning of your Main Shot, all Characters at this location gain +1 Fighting until they leave play and you must sacrifice a Character at this location.', 'Men like to believe that they''re not savages.', '', 'Mike Kimble', 1, 6, 1, 15, 'N', '| donner lake |');
INSERT INTO `card` VALUES (2324, 'Devil''s Rope', 'State', 1, 0x65, '', 0, 0, 0, 'Schtick. Play on a Site. Once per turn, you may sacrifice a Character at this location to place X counters on Devil''s Rope. X= that Character''s Fighting. Remove a counter :: Inflict 1 damage on any Character at this location.', '', '', 'Ed Cox', 3, 5, 6, 15, 'N', '| devil devils rope |');
INSERT INTO `card` VALUES (2325, 'Dog Soldiers', 'Thunder Braves', 3, 0x6d, 0x6d, 4, 0, 0, 'When this card turns to attack, you may unturn a Power-generating Site you control. (No card can unturn another card more than once a turn.)', '', '', 'Cristian Saksida', 5, 7, 2, 15, 'N', '| dog soldier soldiers thunder brav brave braves |');
INSERT INTO `card` VALUES (2323, 'Devil''s Mountain', 'Feng Shui Site', 0, '', '', 0, 1, 6, 'Unique. Turn to redirect all damage inflicted on a Site by a single source to a Character you control.', 'Animals stay away. Men aren''t always so smart.', '', 'Rob Alexander', 1, 6, 1, 15, 'N', '| devil devils mountain |');
INSERT INTO `card` VALUES (2322, 'Detonating Corpses', 'Event', 1, 0x654d, '', 0, 0, 0, 'Play when a Character you control is smoked by combat damage. All opponents'' Characters at that location take damage equal to the smoked Character''s printed Fighting.', '"The dead will serve - one way or another."', '', 'Dan Harding', 4, 5, 2, 15, 'N', '| detonating corps corpse corpses |');
INSERT INTO `card` VALUES (2321, 'Demon Whiskey', 'Rotgut Horde', 3, '', 0x65, 3, 0, 0, 'When an opponent burns a Site, you may return this card to play from your smoked pile.', '"I''m telling you, that stuff will kill you."', '', 'Matthew Laznicka', 5, 5, 2, 15, 'N', '| demon whiskey rotgut horde |');
INSERT INTO `card` VALUES (2320, 'Daughter of Flame', 'Fire Assassin', 4, 0x6d6d, 0x6d, 6, 0, 0, 'Assassinate. When this card attacks, its target''s controller must discard two cards at random.', 'She meant for cleansing flames to burn the fog from San Francisco.', '', 'Ed Cox', 5, 7, 3, 15, 'N', '| daughter flame fire assassin |');
INSERT INTO `card` VALUES (2318, 'Dan Dammer, Jammer Slammer', 'Fanatic Cop', 4, 0x6666, 0x66, 6, 0, 0, 'Unique. Guts. Toughness: 3 against [j] cards. Not affected by [j] Events.', 'The Buro''s toughest cop also had the highest-rated show on the Cop Channel.', '', 'Cristian Saksida', 5, 1, 6, 15, 'N', '| dan dammer jammer slammer fanatic cop |');
INSERT INTO `card` VALUES (2319, 'Data Theft', 'Event', 0, 0x79, '', 0, 0, 0, 'Turn a [y] Character you control and target a player :: Reveal the top 3 cards of that player''s deck. You may play one of those cards at normal cost, regardless of type, and replacing each resource requirement with [H].', '"Whatever you know, so do we."', '', 'Banu Adhimuka', 4, 15, 3, 15, 'N', '| data theft |');
INSERT INTO `card` VALUES (2317, 'Curse of Discord', 'Event', 1, 0x4d4d, '', 0, 0, 0, 'Target two interceptors :: Inflict damage on each equal to the other''s Fighting.', '"Brother will fight brother. It is the way of things."', '', 'Ed Cox', 4, 11, 2, 15, 'N', '| curse discord |');
INSERT INTO `card` VALUES (2315, 'Coyote Clan Scavengers', 'Transformed Animals', 2, '', 0x61, 2, 0, 0, 'Ambush against Characters with resource conditions.', '"How can the Lodge keep its power if we cannot prevent entire bloodlines from going feral?"', '', 'Flavio Augusto', 5, 2, 2, 15, 'N', '| coyote clan scavenger scavengers transformed animal animals |');
INSERT INTO `card` VALUES (2316, 'Crazed Preacher', 'Dangerous Zealot', 2, 0x7070, 0x70, 2, 0, 0, 'Sacrifice this card and turn two Sites you control :: Smoke all non-Unique Characters at his location.', '"Why bother to repent? The end is upon us right now."', '', 'Matthew Laznicka', 5, 13, 2, 15, 'N', '| crazed preacher dangerou dangerous zealot |');
INSERT INTO `card` VALUES (2314, 'Corrupt Land Agent', 'Pledged Flim-Flam Man', 1, 0x61, 0x61, 1, 0, 0, 'Turn when an opponent plays a Site :: Gain 1 Power.', '"By the time we learned he didn''t own the Baxter Mine, he was probably halfway to Bolivia."', '', 'Matthew Laznicka', 5, 2, 2, 15, 'N', '| corrupt land agent pledged flim flam man |');
INSERT INTO `card` VALUES (2313, 'Corporate Hacker', 'Sleep-Deprived Operator', 1, 0x79, 0x7948, 1, 0, 0, 'Turn to look at the top three cards of an opponent''s deck. You may put one of those cards on the bottom of the deck, and return the other two to the top of the deck in any order.', '', '', 'Zak Hennessey', 5, 15, 3, 15, 'N', '| corporate hacker sleep deprived operator |');
INSERT INTO `card` VALUES (2312, 'Children of the Sharp Knives', 'Self-Destructive Cultists', 2, 0x70, 0x70, 4, 0, 0, 'Cannot turn to heal. Inflict 1 damage on this card at the end of your Main Shot.', '"It''s time, little poppet."', '', 'Oliver Castaneda', 5, 13, 2, 15, 'N', '| children sharp kniv knive knives self destructive cultist cultists |');
INSERT INTO `card` VALUES (2310, 'Celestials', 'Chinatown Secret Society', 2, '', 0x6743, 2, 0, 0, 'You may play this card at no cost during an attack by an opponent who is one Feng Shui Site away from victory. +1 Fighting while you control any Unique Characters.', '', '', 'Oliver Castaneda', 5, 8, 2, 15, 'N', '| celestial celestials chinatown secret society |');
INSERT INTO `card` VALUES (2311, 'Chain Lightning', 'Thunder Event', 2, 0x6d4d, '', 0, 0, 0, 'Target four Characters :: Inflict 4 damage on one, 3 on another, 2 on another, and 1 on the last.', 'Huan Ken''s fury is truly a sight to behold - from a safe distance.', '', 'Mike Jackson', 4, 7, 3, 15, 'N', '| chain lightning thunder |');
INSERT INTO `card` VALUES (2309, 'Cavalry Regiment', 'Horseback Heroes', 3, 0x6464, 0x64, 3, 0, 0, 'Independent. When you play this card from your hand, you may choose a Character :: Unturn that Character, and it gains Independent until the end of the turn.', '', '', 'Oliver Castaneda', 5, 4, 2, 15, 'N', '| cavalry regiment horseback hero heroe heroes |');
INSERT INTO `card` VALUES (2307, 'Bountiful Fields', 'Feng Shui Site', 0, '', '', 0, 1, 8, 'Once per turn when you play a State, you may draw a card.', 'In a land of plenty, many things can be harvested.', '', 'Oliver Castaneda', 1, 6, 3, 15, 'N', '| bountiful field fields |');
INSERT INTO `card` VALUES (2308, 'Bridge of Birds', 'Edge', 0, 0x7373, '', 0, 0, 0, 'You may play Characters during other players'' Main Shots.', 'The Seven Masters come and go for their own reasons, and in their own ways.', '', 'Matt Bober', 2, 14, 3, 15, 'N', '| bridge bird birds |');
INSERT INTO `card` VALUES (2306, 'Boot Hill', 'Feng Shui Site', 0, '', '', 0, 1, 7, 'Limited. If any opponent is closer to victory than you, turn and discard a Character that is not a foundation :: Gain 1 Power.', 'Most gunfighters end up here, with a body full of lead.', '', 'Mike Kimble', 1, 6, 3, 15, 'N', '| boot hill |');
INSERT INTO `card` VALUES (2305, 'BoneChill', 'Reprogrammed Pawn', 4, 0x6666, 0x66, 6, 0, 0, 'When this card enters play, you may immediately toast a Character in an opponent''s smoked pile. This card''s printed rules text copies that of the toasted Character.', '', '', 'Dan Murray', 5, 1, 3, 15, 'N', '| bone chill reprogrammed pawn |');
INSERT INTO `card` VALUES (2304, 'Boiler Room', 'Mad Science Site', 2, 0x6a, 0x6a, 0, 1, 5, 'Limited. If this Site is in your front row, turn it and discard a card at random :: Gain 1 Power.', '"Emergency pressure release valve? Don''t think we''ve got one. Why?"', '', 'Mike Jackson', 1, 10, 3, 15, 'N', '| boiler room mad science |');
INSERT INTO `card` VALUES (2303, 'Bloody Herd', 'Vampire Cattle', 3, 0x6565, 0x65, 4, 0, 0, 'Toughness: 1. Prior to combat with a Character, you may copy the rules text of that Character onto this card until it leaves play or until it copies another card this way.', '', '', 'Mike Jackson', 5, 5, 3, 15, 'N', '| bloody herd vampire cattle |');
INSERT INTO `card` VALUES (2302, 'Blood Eagles', 'Abomination Scouts', 3, 0x66, 0x66, 4, 0, 0, 'When this card turns to attack, you may give another Character Ambush and +1 damage until the end of the attack.', '"Damned things smell you from a mile above..."', '', 'Ed Cox', 5, 1, 2, 15, 'N', '| blood eagl eagle eagles abomination scout scouts |');
INSERT INTO `card` VALUES (2299, 'Big Red Barn', 'Feng Shui Site', 0, '', '', 0, 1, 7, '+2 Body for each Character you control.', 'A farm needs good chi as much as it does workers.', '', 'Mike Jackson', 1, 6, 2, 15, 'N', '| big red barn |');
INSERT INTO `card` VALUES (2301, 'Blasting Crew', 'Railroad Workers', 3, 0x6a, 0x6a, 4, 0, 0, 'When this card turns to attack, inflict 2 damage on all Sites at the location of its target.', '"Wait, you''ll pay us to do this?"', '', 'Oliver Castaneda', 5, 10, 2, 15, 'N', '| blasting crew railroad worker workers |');
INSERT INTO `card` VALUES (2300, 'Black Ophir', 'Gorilla Bartender', 5, 0x6a6a6a, 0x6a, 7, 0, 0, 'Unique. Cannot be intercepted by Characters whose Fighting is lower than this card''s.', '"Water? What kinda saloon you think this is?"', '', 'Matt Bober', 5, 10, 1, 15, 'N', '| black ophir gorilla bartender |');
INSERT INTO `card` VALUES (2297, 'Avenging Darkness', 'Vengeance Spirit', 0, 0x6d, 0x6d, 2, 0, 0, 'Unique. Uncopyable. Ambush. Play only when an opponent burns a Feng Shui Site you control. If Avenging Darkness would successfully inflict combat damage on a card, toast that card instead.', '', '', 'Oliver Castaneda', 5, 7, 3, 15, 'N', '| avenging darkness darknes darkne darkn vengeance spirit |');
INSERT INTO `card` VALUES (2298, 'Bait and Switch', 'Event', 1, 0x7979, '', 0, 0, 0, 'Play during an attack you declared, when you no longer control any attackers. Turn a Character you control :: The attack does not end. Instead, that Character becomes an attacker.', '', '', 'Oliver Castaneda', 4, 15, 2, 15, 'N', '| bait switch |');
INSERT INTO `card` VALUES (2296, 'Arcanovirus', 'Event', 1, 0x6666, '', 0, 0, 0, 'Target a Character with a printed numerical Fighting of 5 or less :: Smoke target. Alternately, pay 2 additional Power :: Smoke all Characters with a printed numerical Fighting of 5 or less.', 'Boatman simply took over Dao Biotech.', '', 'Oliver Castaneda', 4, 1, 3, 15, 'N', '| arcano viru virus |');
INSERT INTO `card` VALUES (2295, 'Anastasia', 'Orphan Ice Ward', 4, 0x6d6d6d4d, 0x6d4d, 7, 0, 0, 'Unique. When this card smokes a Character by inflicting combat damage, you may heal a card. (Including this one.)', 'Pi Tui took pity on her, once.', '', 'Matthew Laznicka', 5, 7, 1, 15, 'N', '| anastasia orphan ice ward |');
INSERT INTO `card` VALUES (2293, 'Agathon''s Deputies', 'Demon Cops', 2, 0x65, 0x65, 3, 0, 0, 'Regenerate. If you control no other <Demon>, <Police>, or <Cop> cards at the end of your Main Shot, smoke this card.', 'Don''t cry for help too loudly. They may just answer.', '', 'Cristian Saksida', 5, 5, 2, 15, 'N', '| agathon agathons deputy deput deputies demon cop cops |');
INSERT INTO `card` VALUES (2294, 'Agent Tanaka', 'CDCA Ninja', 3, 0x66, 0x66, 4, 0, 0, 'Unique. Ambush. Assassinate.', '"I no longer have the power I once did. That does not mean I am without resources." - Curtis Boatman', '', 'Oliver Castaneda', 5, 1, 1, 15, 'N', '| agent tanaka cdca ninja |');
INSERT INTO `card` VALUES (2432, 'Blade Palm', 'Event', 2, 0x4343, NULL, NULL, NULL, NULL, 'Return target card to owner''s hand.', 'The ancient art of the Blade Palm is known by few, but feared by all.', NULL, 'Brian Rood', 4, 3, 6, 16, 'N', '| blade palm |');
INSERT INTO `card` VALUES (2433, 'Burned for Victory', 'Former Feng Shui Site', NULL, NULL, NULL, NULL, 0, 0, 'This card is to be put into your Burned for Victory pile.\r\nIt counts towards victory.', 'Sic transit gloria mundi.', NULL, 'Max Hufnagel', 6, 12, 6, 16, 'N', '| burned victory former |');
